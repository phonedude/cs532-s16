    <script language="JavaScript" type="text/javascript">
    
        var gstrJobInfo = "%%";
        //global var to store jobsite info - GTG
        var gJobSiteInfo = "";
        var SR_READY = "no";
        var SR_PRIDE = "%5eXPo1WBzaZuM%3d";
        var SR_STID = "5086";
        var SR_CLID = "25545";
        var SR_LOCALE = "1033";
        var SR_FN = "";
        var SR_LN = "";
        var enableSocialReferral = "no";
