$(document).ready(function() {		
	var TIMEZONE_OFFSET_COOKIE = "timezoneOffset";  		
	var timezoneOffset = $.cookie( TIMEZONE_OFFSET_COOKIE );
	var localTimezoneOffset = getTimezoneOffset();
	
	if ((typeof timezoneOffset === "undefined" || !timezoneOffset) || ( timezoneOffset && timezoneOffset != localTimezoneOffset)) {
		$.cookie( TIMEZONE_OFFSET_COOKIE, localTimezoneOffset, { expires: 365, domain: ".vlive.tv", path: "/"});
		
		if ($.cookie(TIMEZONE_OFFSET_COOKIE) === localTimezoneOffset) {
			location.reload(); 
			return;
		}
	}

	
	var userLanguage = "en";
	var cookieLang = $.cookie("userLanguage");
	var paramLang = vlive.tv.language.change.handler.getLangQueryString();
	if (paramLang && userLanguage !== cookieLang) { 
		$.cookie("userLanguage", userLanguage, {expires: 30, domain: ".vlive.tv", path: "/"});		
	} else if ((userLanguage && (typeof cookieLang === "undefined" || !cookieLang))) {
		$.cookie("userLanguage", userLanguage, {expires: 30, domain: ".vlive.tv", path: "/"});
	}
	var userCountry = "US";
	var cookieCountry = $.cookie("userCountry");
	if (userCountry !== cookieCountry) {
		$.cookie("userCountry", userCountry, {expires: 30, domain: ".vlive.tv", path: "/"});
	}
	
	
	if (/^\/video\/[0-9]+/.test(location.pathname) === false) {
		vlive.tv.auth.logininfo.init("en");
	}
		
	$(".btn_lang").click(function(event) { 
		event.preventDefault();
		event.stopPropagation();
		$(".lang_area .lyr").toggle();
		$(".login_area .lyr").hide();
		$(".pop_lyr1").hide();
		
		$("#search_area").hide();
		$("#btn_search1").show();
	});

	 
	$(document).click(function() {
		$(".pop_lyr1").hide();
		$(".lyr").hide();
		$(".channel_lyr").hide();
		
		$("#search_area").hide();
		$("#btn_search1").show();		
	});
	
	$("#btn_search1").click(function(event) {
		event.preventDefault();
		event.stopPropagation();
		$(this).hide();
		$("#search_area").show();
		$("#search_text").focus();
		$(".pop_lyr1").hide();
		$(".login_area .lyr").hide();
	});
	
	$("#search_text").click(function(event) {
		event.preventDefault();
		event.stopPropagation();
	});
	
	$("#btn_search2").click(function(event) {		
		if (!$("#search_text").val()) {
			$("#search_area").hide();
			$("#btn_search1").show();
			event.preventDefault();
		}
	});
	/*
	var footerHeight = $("#footer").height();
	if (isNaN(footerHeight)) {
		footerHeight = 130;
	}
	*/
	$(window).scroll(function(event) {
		var iScrollTop = $(this).scrollTop();
		if (isNaN(iScrollTop)) {
			return;
		}		
		if (iScrollTop > 0) {
			$(".btn_top_area").show();			
				
			if ($("#footer").is(":visible")) {
				var footerY = $("#footer").offset().top;
				var scrollEndY = iScrollTop + $(window).height();				
				if (isNaN(footerY) || isNaN(scrollEndY)) {
					return;
				}
				if (footerY < scrollEndY) {
					$(".btn_top_area").css({"position":"absolute", "bottom":"200px"});
				} else {
					$(".btn_top_area").css({"position":"fixed", "bottom":"20px"});
				}
			}
		} else if (iScrollTop == 0) {
			$(".btn_top_area").css("position", "fixed");
			$(".btn_top_area").hide();
		}
	});
	
	$(".ico_top").click(function(event) {
		event.preventDefault();
		$("html,body").scrollTop(0);				
	});	
	
	$(".btn_vapp").click(function (event) {
		event.stopPropagation();
		$(".lyr:visible").hide();
		$("#search_area").hide();
		$("#btn_search1").show();			
	});
	$(".btn_share").click(function (event) {
		event.stopPropagation();
		$(".lyr:visible").hide();
		$("#search_area").hide();
		$("#btn_search1").show();	
	});
	$(".video_list_cont").click(function (event) {
		event.stopPropagation();
		$(".lyr:visible").hide();
		$("#search_area").hide();
		$("#btn_search1").show();	
	});
	$(".pop_lyr1").click(function (event) {
		event.stopPropagation();
		$(".lyr:visible").hide();
		$("#search_area").hide();
		$("#btn_search1").show();
	});
	$("embed[src*=clipboard]").click(function (event) {
		event.stopPropagation();
	});
	
	
	$("#loginDimmedArea").click(function (event) {
		event.stopPropagation();
	});		
	
	

	$("#tweiboStartLayer .btn").on("click", function(evt) {
		evt.preventDefault();			
		$("#loginDimmedArea").removeClass("dimmed_bg");
		$("body").removeClass("no_scroll");
		$("#tweiboStartLayer").toggle();
		
		$.cookie("tweiboClose", "Y", {expires: 1, domain: ".vlive.tv", path: "/"});
	});
});



/**
 * KST 기준의 분단위 DIFF 조회
 */
function getTimezoneOffset(){
	var KST_MINITE_OFFSET = -9 * 60; 
	var minOffsetGMT = (new Date()).getTimezoneOffset() * -1;
	var minOffsetKST = minOffsetGMT + KST_MINITE_OFFSET; 			
	return minOffsetKST;
}

function partnersWindow(userLanguage) {
	if (userLanguage === "vi") {
		userLanguage = "en";
	}
	centerWindow('/partners/join?lang=' + userLanguage);
}
