$(document).ready(function(){
	var resp = $.cookie("RTT_TIMESTAMP");
	if (resp != null) {
		var tsArray = resp.split(",");
		if(typeof performance !== "undefined"){
			try {
				var start = performance.timing.navigationStart;
				var end = performance.timing.responseStart;

				var isCached = tsArray.shift();
				tsArray.unshift(start);
				tsArray.push(end);
				tsArray.unshift(isCached);
				
				$.removeCookie("RTT_TIMESTAMP",{path:location.pathname} );
				$.cookie("RTT", tsArray, {path: "/logging"});
			} catch (e) {
				JEagleEyeClient.sendError("[RTT] create cookie : " + e.message);
			}
		}
	}
	
	new Image().src="/logging?url=" + location.pathname + "&width=" + $(window).width() + "&height=" + $(window).height();
});
</script></body>
</html>