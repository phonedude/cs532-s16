<script type='text/javascript'>

	function newHttpReq()
	{
		var newRequest = null;
		
		try
		{
			newRequest = new XMLHttpRequest();
		}
		catch(trymicrosoft)
		{
			try
			{
				newRequest = new ActiveXObject("Msxml2.XMLHTTP");
			}
			catch(othermicrosoft)
			{
				try
				{
					newRequest = new ActiveXObject("Microsoft.XMLHTTP");
				}
				catch(failed)
				{
					newRequest = null;
				}			
			}
		}
		
		return newRequest;
	
	}	
	
	var request = newHttpReq();	
	
	function changegallery()
	{
		var newgalleryid = document.getElementById('newgallery').value;
		locationstring = "/galleries.html?tab=artworkgalleries&artworkgalleryid="+newgalleryid;
		document.location = locationstring;	
	}
	
	
	function confirmdeletegallery(id)
	{
		result = confirm ('Are you sure you want to delete this gallery?');
		
		if (result == true)
		{
			locationstring = "/galleries.html?deletegalleryid="+id;
			document.location = locationstring;	
		}
		
	}

	function initiatechange(id,position)
	{
		var url = '../queries/querygalleriesartistwebsites.php?initiatechangeid=' + id + '&position=' + position;		

     	if((request.readyState != 4) && (request.readyState != 0))
     	{
			request.abort();
			request = newHttpReq();
     	}

		request.open('GET',url,true);
     	request.onreadystatechange = updateinitiatechange;
     	request.send(null);			
	
		return;

	}

	function updateinitiatechange()
	{
   
   		try 
		{
			if(request.readyState == 4)
			{
			}
		
			if(request.status == 200)
			{
			}
		}
		catch (e)
		{
			return;
		}
   
   
   		if (request.readyState == 4)
   		{
   			if(request.status == 200)
   			{   
				var response = request.responseText.split(",");
				document.getElementById('change[' + response[0] + ']').innerHTML = response[1];
   			}
   		}
	}


	function changeposition(id)
	{	
		var targetposition = document.getElementById('newposition[' + id + ']').value;
	
		var url = '../queries/querygalleriesartistwebsites.php?artworkgalleryid=' + id + '&targetposition=' + targetposition;		

     	if((request.readyState != 4) && (request.readyState != 0))
     	{
			request.abort();
			request = newHttpReq();
     	}

		request.open('GET',url,true);
     	request.onreadystatechange = updatepage;
     	request.send(null);				

	}	



	function cancelposition(id)
	{	

		var url = '../queries/querygalleriesartistwebsites.php?cancel=true';		

     	if((request.readyState != 4) && (request.readyState != 0))
     	{
			request.abort();
			request = newHttpReq();
     	}

		request.open('GET',url,true);
     	request.onreadystatechange = updatepage;
     	request.send(null);				

	}	



	function updatepage()
	{
   
   		try 
		{
			if(request.readyState == 4)
			{
			}
		
			if(request.status == 200)
			{
			}
		}
		catch (e)
		{
			return;
		}
   
   
   		if (request.readyState == 4)
   		{
   			if(request.status == 200)
   			{      			
				document.getElementById('gallerydiv').innerHTML = request.responseText;
   			}
   		}
	}

	
</script>
