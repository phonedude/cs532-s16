    	/* keeps popup on top */
	  	function checkPopup()
	  	{
	    	try
	    	{
	      		if (newwindow != null && !newwindow.closed)
	      		{
		      		newwindow.focus();
	      		}
	    	}
	    	catch(e)
	    	{
  	
    		}
	  	}
  		</script>
