  <!-- ### FOLIO ### -->
  <script src="/csp/mediapool/sites/shared/assets/javascripts/folio.js"></script>
	
	<!-- ### IMAGE GALLERIES ### -->
	<script src="//cdn.jsdelivr.net/jquery.slick/1.4.1/slick.min.js"></script>
  <script src="/csp/mediapool/sites/shared/assets/javascripts/ccp-gallery-2.js"></script>
  <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery.waitforimages/1.5.0/jquery.waitforimages.min.js"></script>
  <script>
    $(document).ready(function() {
      var $holder = $('#gallery-holder');
      var $gallery = $('#gallery');
      var width = $gallery.parent().width();
      
      var viewerHeight = 540;
      if (width < 800)
        viewerHeight = 540
      if (width < 480)
        viewerHeight = 260;
      
      var gallery = function($holder, $gallery) {
        $gallery.ccpGallery2({
          debug: true,
          slickAfterChange: function() {
            window.loadAds(window.adSelector);
          },
          slickTheme: 'http://theadvocate.com/csp/mediapool/sites/shared/assets/stylesheets/slick-gallery.css',
          viewerHeight: viewerHeight,
          width: width
        });
        $holder.css('height', 'auto');
        $('#gallery .slick-dots, #gallery .slick-prev, #gallery .slick-next').css('top', viewerHeight);
        $('#gallery .slick-loading .slick-list, #gallery-holder').css('background', 'none');
        $gallery.animate({opacity: 1}, 300);
      }
      
      var $images = $gallery.find('img');
      var imagesCount = $images.length;
      var loadedCount = 0;
      
      $images.each(function() {
        $(this).attr('src', $(this).attr('src') + '?' + new Date().getTime());
        $(this).on('load', function() {
          loadedCount++;
          if (loadedCount === imagesCount) {
            gallery($holder, $gallery);
          }
        });			
      });
    });
  </script>
    
  <!-- EXTRA AD WILL APPEAR IF HEIGHT OF LATEST NEWS COLUMN REACHES 1013PX -->	

  <script language="Javascript">
	$( document ).ready(function() {
	  if ($('#latest-news') !=null && ('#remnantAd') !=null){
		var newsHeight = $('#latest-news').height();
		  if (newsHeight > 1013 ) {
		  $('#remnantAd').attr("style","");
		  }
		}
	});
  </script>


