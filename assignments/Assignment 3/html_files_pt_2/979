            <script type="text/javascript">
                //<![CDATA[

                $(function() {
                    // Check cookies again here so that the banner is not shown when going back using the Back button after clicking the "Learn More" link.
                    if (!$.cookies.get('cookiespolicy')) {
                        // Insert the cookies banner when the page has loaded (you should move this CSS elsewhere)
                        $('body').prepend("<div id='cookie-wrapper'  class='cookie-wrapper' ><div class='cookie-banner' > <div class='text-wrapper'>  <p>We use cookies to provide you with the best possible user experience. If you continue to use the site without changing your cookie settings, we assume you are fine with our cookies and the way we use them. The cookies store information about how you use our website, and help you use some of the functions on the site. Our cookies do not store any sensitive information, and we never use your cookies for targeted advertising. If you want, you can change your computer's settings so that it does not accept cookies. We have information here on how you can do that.</p> <a id='acknowledge'  href='javascript:void(0)' class='button'>Continue</a><a id='cookiesLearnMore' href='http://www.ericsson.com/cookies' target='_blank'>Cookies on www.ericsson.com</a></div></div></div>");


                        // '<div> <div style="background-color:#999;color:white;padding:10px;text-align:center;font-size:smaller;border-bottom:1px solid #333;">THIS IS A TEST OF THE UK COOKIE POLICY. THIS IS ONLY A TEST, A REAL UK COOKIE POLICY WOULD INCLUDE A SERIES OF SENTENCES TELLING YOU WHAT TO DO. <a id="cookiesLearnMore" style="color: #DDD;" href="javascript:void(0)">Learn more</a>.<a id="acknowledge" style="color: #DDD;" href="javascript:void(0)">Dismiss</a> </div></div>');

                        // *** Event Bindings
                        $('#cookiesLearnMore').on('mouseup', function() {
                            dropCookie();

                            document.location.href = '';
                        });

                        $('#acknowledge').on('mouseup', function() {
                            dropCookie();

                            $(this).closest('div').remove();
                        });

                        // *** Support Functions
                        function dropCookie() {
                            $.cookies.set('cookiespolicy', 1, {
                                expiresAt: (function() {
                                    i = new Date;
                                    i.setFullYear(i.getFullYear() + 30);
                                    return i;
                                }())
                                , domain: 'ericsson.com'
                            });
                        }
                    }
                });

                //]]>
            </script>
