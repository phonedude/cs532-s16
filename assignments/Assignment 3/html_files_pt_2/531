  <a class="fa fa-chevron-circle-up" id="lnkBackToTop" href="BackToTop"></a>

    

 
 <script type="text/javascript">
 function setBackToTop(){
      $("#lnkBackToTop").toggleClass("visible", ($(window).height() < $(document).height() && $(window).scrollTop() > 0));
    }
  $(document).ready(function(){
    var url = location.href;
    var reg = /J(\d*)\.aspx|jid=(\d*)|jobid=(\d*)/gi;
    var matches = reg.exec(url);
    var qry = "";
    if (matches != null){
      if (matches[0].indexOf("=") > -1)
        qry = matches[0].substring(matches[0].indexOf("=")+1);
      else
        qry = matches[0].toLowerCase().replace(".aspx", "").replace("j", "");
    }
    if (qry.length > 0) $("#submitresume").attr("href","/JobSeeker/Profile.aspx?jobid=" + qry)
      $(window).on("resize, scroll", setBackToTop);
      $("#lnkBackToTop").on("click", function(e){
        $("html, body").animate({ scrollTop: 0 }, 500);
        e.preventDefault();
        return false;
      })
      setBackToTop();
  })
 </script>
</body>

