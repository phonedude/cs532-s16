    var skeleton = $('#startingSearchHtmlSkeleton').html();
    skeleton = skeleton.replace(/^\s*<!--\s*/, '').replace(/\s*-->\s*$/, '');
    amzn.sx.utils.jsDepMgr.when('search-ajax', "search-html-skeleton-installer-runner", function() {
