          <div class="ssw-control-group">
            <input id="first_name" tabindex="4"
                   name="customer[first_name]" type="text" placeholder="First Name" class="required ssw-span3"
                   minlength="2">
          </div>
          <div class="ssw-control-group">
            <input id="last_name" tabindex="5"
                   name="customer[last_name]" type="text" placeholder="Last Name" class="required ssw-span3"
