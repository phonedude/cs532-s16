<script type="text/javascript">
  if (window.addEventListener){ addEventListener("message", sswListener); }
  else { window.attachEvent("onmessage", sswListener) }
  function  sswListener(event){
    if(typeof event.data.ssw_message != 'undefined' && event.data.ssw_message){
      if(typeof event.data.hide_admin_bar && event.data.hide_admin_bar){
        ssw('#admin_bar_iframe').hide();
        ssw('html').css('margin-top', '0px');
      }
      if(typeof event.data.css_code != 'undefined' && event.data.css_code){
        ssw('#ssw-design-preview').html(event.data.css_code);
      }
    }
  }
  function targetFbShareSetting(action, id, name, product_id, message, caption) {
    ssw('#fb_share_img').attr('src', 'http://graph.facebook.com/' + id + '/picture');
    ssw('#fb_share_message').val(message);
    ssw('#fb_share_caption').val(caption);
    ssw('#fb_share_body_text').html('Do you wanna share your ' + action + ' with your friends on Facebook?');
    ssw('#fb_share_action').val(action);
    ssw('#fb_share_product_id').val(product_id);
    ssw('#fb_share_name').html(name).attr('href', 'http://www.facebook.com/' + id);
    ssw('#fb_share_setting_modal').css('z-index', 3000001);
    ssw('#fb_share_setting_modal').sswModal();
  }
</script>



