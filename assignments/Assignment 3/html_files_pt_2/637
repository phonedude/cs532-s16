            <input id="hesy-sign-up-submit" tabindex="8" type="submit" class="btn button" value="Sign Up">
          </div>
        </form>
      </div>
      <div class="ssw-modal-footer">
        Already have an account? <a href="#"
                                    onclick="ssw('#signup_modal').sswModal('hide'); ssw('#login_modal').sswModal('show');">Log
          In</a>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
var service_id = 0, sl = 0, sswSocialLogin = 0;

function getParameterByName(name) {
  name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
  var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
      results = regex.exec(location.search);
  return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

function targetSEMModal(response, service_name) {
  var name = '';
  if (response.first_name) {
    name += response.first_name + " ";
  }
  if (response.last_name) {
    name += response.last_name;
  }
  onOpenForm();
  ssw('#social_exist_modal .img-circle').attr('src', response.photo_url);
  ssw('#social_exist_modal #sem_name').html(name);
  ssw('#social_exist_modal #sem_email').val(response.email);
  ssw('#social_exist_modal #sem_text').html("You already have an account registered to `" + response.email + "`. This is a first time you're logging in via " + service_name + ", please type your password below:");
  ssw('#login_modal').sswModal('hide');
  ssw('#social_exist_modal').sswModal();
}

function loginLoader(){
  ssw('.ssw-modal').sswModal('hide');
  ssw('body').prepend('<div style="background: none repeat scroll 0 0 rgba(255, 255, 255, 0.77);height: 100%;position: fixed;width: 100%;z-index: 999;"><div class="ssw-login-loading" id="ssw-login-loader"> <span class="ssw-spin ssw-loader"></span> </div> </div>');
  ssw('#ssw-login-loader').css('opacity', 1);
  ssw('#ssw-login-loader').css({'position': 'fixed', 'top': '50%', 'left':'50%'});
}
function loginLoaderCancel() {
  ssw('#ssw-login-loader').parent().remove();
}

function addTwMassage(message) {
  ssw('#tw_error').html(message);
  ssw('#tw_error').show();
}

function addTmMassage(message) {
  ssw('#tm_error').html(message);
  ssw('#tm_error').show();
}

function onSubmitForm(){
  ssw('.ssw-login form').css('opacity', '0.3');
  ssw('.ssw-login form input').removeAttr('enabled').attr('disabled', 'disabled');
  ssw('.ssw-signup-loading').show();
  ssw('.he-login').css('opacity', '0.3');
  ssw('.he-login input').removeAttr('enabled').attr('disabled', 'disabled');
  ssw('.ssw-login-loading').show();
}

function onOpenForm(){
  ssw('.ssw-login form').css('opacity', '1');
  ssw('.ssw-login form input').removeAttr('disabled').attr('enabled', 'enabled');
  ssw('.ssw-signup-loading').hide();
  ssw('.he-login').css('opacity', '1');
  ssw('.he-login input').removeAttr('disabled').attr('enabled', 'enabled');
  ssw('.ssw-login-loading').hide();
}


function validateLogin(){
  ssw('#tw_email_form').validate({
    rules: {
      'email': {required: true, email: true}
    },
    highlight: function (element) {
      ssw(element).closest('.ssw-control-group').addClass('ssw-error');
    },
    success: function (element) {
      ssw(element).closest('.ssw-control-group').removeClass('ssw-error');
    },
    submitHandler: function (form) {
      ssw.ajax({
        type: 'POST',
        url: '/apps/ssw/lite2/auth/twsetemail',
        data: {'_sid': ssw.cookie('hesid'), 'service_id': ssw('#tw_service_id').val(), email: ssw('#tw_email_input').val()},
        success: function (response) {
          switch (response.error) {
            case 0 : var shopifyLogin = new ShopifyLogin();
              shopifyLogin.preLogin(response.email, response.password);
              shopifyLogin.login(); break;
            case 1 : addTwMassage('This email is already used');break;
            case 2 : addTwMassage('Incorrect data');break;
            case 3 : addTwMassage('Twitter user not found');break;
            case 4 : addTwMassage('Not created customer on Shopify');break;
            case 5 : addTwMassage('Problem on create user');break;
          }
        },
        dataType: 'json'
      });
    }
  });
  ssw('#sem_modal_form').validate({
    rules: {
      'password': {required: true}
    },
    highlight: function (element) {
      ssw(element).closest('.ssw-control-group').addClass('ssw-error');
    },
    success: function (element) {
      ssw(element).closest('.ssw-control-group').removeClass('ssw-error');
    },
    submitHandler: function (form) {
      var shopifyLogin = new ShopifyLogin();
      shopifyLogin.preLogin(ssw('#sem_email').val(), ssw('#sem_password').val());
      shopifyLogin.login();
    }
  });
  ssw('#tm_email_form').validate({
    rules: {
      'email': {required: true, email: true}
    },
    highlight: function (element) {
      ssw(element).closest('.ssw-control-group').addClass('ssw-error');
    },
    success: function (element) {
      ssw(element).closest('.ssw-control-group').removeClass('ssw-error');
    },
    submitHandler: function (form) {
      ssw.ajax({
        type: 'POST',
        url: '/apps/ssw/lite2/auth/tmsetemail',
        data: {'_sid': ssw.cookie('hesid'), 'service_id': ssw('#tm_service_id').val(), email: ssw('#tm_email_input').val()},
        success: function (response) {
          switch (response.error) {
            case 0 : var shopifyLogin = new ShopifyLogin();
              shopifyLogin.preLogin(response.email, response.password);
              shopifyLogin.login(); break;
            case 1 : addTmMassage('This email is already used');break;
            case 2 : addTmMassage('Incorrect data');break;
            case 3 : addTmMassage('Tumblr user not found');break;
            case 4 : addTmMassage('Not created customer on Shopify');break;
            case 5 : addTmMassage('Problem on create user');break;
          }
        },
        dataType: 'json'
      });
    }
  });

  ssw('#he_create_customer').validate({
        rules: {
          'email': {required: true, email: true}
        },
        highlight: function (element) {
          ssw(element).closest('.ssw-control-group').addClass('ssw-error');
        },
        success: function (element) {
          ssw(element).closest('.ssw-control-group').removeClass('ssw-error');
        },
        submitHandler: function (form) {
          var signUp = new ShopifyLogin();
          signUp.signUp();
        }
      }
  );

  ssw('#he_customer_login').validate({
    rules: {
      'customer[email]': {required: true, email: true},
      'customer[password]': {required: true, minlength: 6}
    },
    highlight: function (element) {
      ssw(element).closest('.ssw-control-group').addClass('ssw-error');
    },

    success: function (element) {
      ssw(element).closest('.ssw-control-group').removeClass('ssw-error');
    },
    submitHandler: function (form) {
      var shopifyLogin = new ShopifyLogin();
      shopifyLogin.login();
    }
  });
}

var loginInterval = setInterval(function(){
  if (typeof ssw == "function" && typeof ssw('div').validate != 'undefined') {
    clearInterval(loginInterval);
    validateLogin();
  }

}, 250);

function loginConnectInterval(){
  ssw('.fbconnect').click(function () {
    onSubmitForm();
    ssw.oauthpopup({
  path: HE_DOMAIN + '/lite2/auth/fconnect?_sid=' + ssw.cookie("hesid") + '&shop=' + Shopify.shop,
  windowOptions: 'width=470,height=270',
  callback: function () {
    ssw.ajax({
      type: 'POST',
      url: '/apps/ssw/lite2/auth/fshopcallback',
      data: {'_sid': ssw.cookie('hesid')},
      success: function (response) {
        sswSocialLogin = 1;
        if (ssw.isEmptyObject(response)) {
          onOpenForm();
        } else {
          if (!response.password && response.email) {
            targetSEMModal(response, 'Facebook');
          } else {
            if (response.password) {
              var shopifyLogin = new ShopifyLogin();
              shopifyLogin.preLogin(response.email, response.password);
              shopifyLogin.login();
            } else {
              addMessage('#login_modal .login-failed', 'You already have an account registered to `' + response.email + '`. This is a first time you\'re logging in using your social account, please type your password.', 'warning');
              ssw('#login_modal').sswModal();
            }
          }
        }
      },
      dataType: 'json'
    });
  }
});
  });

  var gplusData = {};
  var gplusClicked = false;
  ssw('.gconnect').click(function(){
    onSubmitForm();
    if( gplusClicked )
    return;
  gplusClicked = true;

  setTimeout(function(){
    gplusClicked = false;
  },4000);
ssw.oauthpopup({
  path: HE_DOMAIN + '/lite2/auth/gplusconnect?_sid=' + ssw.cookie("hesid") + '&shop=' + Shopify.shop,
  windowOptions: 'width=400,height=550',
  callback: function () {
    ssw.ajax({
      type: 'POST',
      url: '/apps/ssw/lite2/auth/gpluscallback',
      data: {'_sid': ssw.cookie('hesid')},
      success: function (response) {
        sswSocialLogin = 1;
        if (ssw.isEmptyObject(response)) {
          onOpenForm();
        } else {
          if (!response.password && response.email) {
            targetSEMModal(response, 'Google +');
          } else {
            if (response.password) {
              var shopifyLogin = new ShopifyLogin();
              shopifyLogin.preLogin(response.email, response.password);
              shopifyLogin.login();
            } else {
              addMessage('#login_modal .login-failed', 'You already have an account registered to `' + response.email + '`. This is a first time you\'re logging in using your social account, please type your password.', 'warning');
              ssw('#login_modal').sswModal();
            }
          }
        }
      },
      dataType: 'json'
    });
  }
});
  });

  ssw('.twconnect').click(function () {
    onSubmitForm();
    var path = '/apps/ssw/lite2/auth/twconnect?_sid=' + ssw.cookie('hesid') + "&shop=" + Shopify.shop;
    ssw.oauthpopup({
      path: path,
      callback: function () {
        ssw.ajax({
          type: 'POST',
          url: '/apps/ssw/lite2/auth/twshopcallback',
          data: {'_sid': ssw.cookie('hesid')},
          success: function (response) {
            sswSocialLogin = 1;
            if (ssw.isEmptyObject(response)) {
              onOpenForm();
            }
            else if (!response.password) {
              onOpenForm();
              ssw('#tw_service_id').val(response.service_id);
              ssw('.ssw-modal').sswModal('hide');
              ssw('#tw_email_modal').sswModal();
            } else {
              var shopifyLogin = new ShopifyLogin();
              shopifyLogin.preLogin(response.email, response.password);
              shopifyLogin.login();
            }
          },
          dataType: 'json'
        });
        //window.location.reload();
      }
    });
  });

  ssw('.tmconnect').click(function () {
    onSubmitForm();
    var path = '/apps/ssw/lite2/auth/tmconnect?_sid=' + ssw.cookie('hesid') + "&shop=" + Shopify.shop;
    ssw.oauthpopup({
      path: path,
      callback: function () {
        ssw.ajax({
          type: 'POST',
          url: '/apps/ssw/lite2/auth/tmshopcallback',
          data: {'_sid': ssw.cookie('hesid')},
          success: function (response) {
            sswSocialLogin = 1;
            if (ssw.isEmptyObject(response)) {
              onOpenForm();
            }
            else if (!response.password) {
              onOpenForm();
              ssw('#tm_service_id').val(response.service_id);
              ssw('.ssw-modal').sswModal('hide');
              ssw('#tm_email_modal').sswModal();
            } else {
              var shopifyLogin = new ShopifyLogin();
              shopifyLogin.preLogin(response.email, response.password);
              shopifyLogin.login();
            }
          },
          dataType: 'json'
        });
        //window.location.reload();
      }
    });
  });


  ssw('.yconnect').click(function () {
    onSubmitForm();
    var path = HE_DOMAIN + '/lite2/auth/yconnect?_sid=' + ssw.cookie('hesid') + "&shop=" + Shopify.shop;
    ssw.oauthpopup({
      path: path,
      callback: function () {
        ssw.ajax({
          type: 'POST',
          url: '/apps/ssw/lite2/auth/yshopcallback',
          data: {'_sid': ssw.cookie('hesid')},
          success: function (response) {
            sswSocialLogin = 1;
            if (ssw.isEmptyObject(response)) {
              onOpenForm();
            } else {
              if (!response.password && response.email) {
                targetSEMModal(response, 'Yahoo');
              } else {
                if (response.password) {
                  var shopifyLogin = new ShopifyLogin();
                  shopifyLogin.preLogin(response.email, response.password);
                  shopifyLogin.login();
                } else {
                  addMessage('#login_modal .login-failed', 'You already have an account registered to `' + response.email + '`. This is a first time you\'re logging in using your social account, please type your password.', 'warning');
                  ssw('#login_modal').sswModal();
                }
              }
            }
          },
          dataType: 'json'
        });
        //window.location.reload();
      }
    });
  });
}
function serviceUserChecker(user, service) {
  ssw.ajax({
    type: 'POST',
    url: '/apps/ssw/lite2/auth/service',
    data: {'user_id': user.authResponse.userID, 'service': service, 'access_token': user.authResponse.accessToken},
    success: function (json) {
//        console.log(json);
      var shopifyLogin = new ShopifyLogin();
      if (json.status) {
        sswSocialLogin = 1;
        sl = 1;
        shopifyLogin.preLogin(json.email, json.password);
        shopifyLogin.login();
      } else {
        shopifyLogin.setServiceUser(user, 'facebook');
      }
    },
    dataType: 'json'
  });
}


var ShopifyLogin = function () {
  var o = this;

  this.preLogin = function (email, password) {
    ssw('#he_customer_login #customer_email').val(email);
    ssw('#he_customer_login #customer_password').val(password);
  }

  this.setServiceUser = function (user, service) {
    FB.api('/me', function(response) {
      ssw.ajax({
        type: 'POST',
        url: '/apps/ssw/lite2/auth/setService',
        data: {'user_id': user.authResponse.userID, 'access_token': user.authResponse.accessToken, 'user': response, 'service': service, '_sid': ssw.cookie("hesid")},
        success: function (json) {
          service_id = json.service_id;
          if (json.user_id == 0) {
            onOpenForm();
            ssw(ssw('.socialconnect')[1]).prev().remove();
            ssw(ssw('.socialconnect')[1]).next().remove();
            ssw(ssw('.socialconnect')[1]).next().remove();
            ssw(ssw('.socialconnect')[1]).remove();
            ssw('div.ssw-alert').remove();
            ssw('#first_name').val(response.first_name);
            ssw('#last_name').val(response.last_name);
            ssw('#email').val(response.email);
            ssw('#login_modal').sswModal('hide');
            ssw('#signup_modal').sswModal('show');
          } else {
            if (!json.password && json.email) {
              targetSEMModal(json);
            } else {
              o.preLogin(json.email, json.password);
              o.login();
            }
          }
        },
        dataType: 'json'
      });
    });
  }

  this.login = function () {
    onOpenForm();
    loginLoader();
    var serviceCheckId = 0;

    if (service_id == 0 && sl == 1)
      serviceCheckId = 1;
    else
      serviceCheckId = service_id;

    var data = ssw('#he_customer_login').serialize() + '&_sid=' + ssw.cookie('hesid') + '&hash_key=' + ssw.cookie('hash_key') + '&service_id=' + sswSocialLogin;
    var url = getParameterByName('checkout_url');
    if (url != "") {
      var l = url.split('/');
      data = 'form_type=customer_login&utf8=✓&checkout_url=' + encodeURIComponent(url) + '&cart=' + l[l.length - 1] + '&utf8=%E2%9C%93&customer%5Bemail%5D=' + ssw('#he_customer_login #customer_email').val()  + '&customer%5Bpassword%5D=' + ssw('#he_customer_login #customer_password').val();
    }
    ssw.ajax({
      type: 'POST',
      url: '/account/login',
      data: data,
      beforeSend: function () {
        onSubmitForm();
        ssw('#hesy-login-submit').attr('disabled', 'disabled');
        ssw('#login_modal .ssw-alert').sswAlert('close');
      },
      success: function (html) {
        html = html.split('script').join('div');
        var htmlElement = ssw('<div>').html(html);
        var cid = htmlElement.find('#ssw_cid').val();
        if (cid == 0) {
          onOpenForm();
          loginLoaderCancel();
          ssw('#login_modal').sswModal('show');
          if (sswSocialLogin) {
            trackShopStats('invalid_social_login', 'all');
          } else {
            trackShopStats('invalid_email_login', 'all');
          }

          ssw('#hesy-login-submit').removeAttr('disabled').attr('enabled', 'enabled');
          if (sl == 1) {
            addMessage('#login_modal .login-failed', 'Right now we\'re not able to log in you socially. Please type your email and password in the form below to log in.', 'warning');
          } else {
            addMessage('#login_modal .login-failed', 'Invalid login credentials.' , 'warning');
          }
        } else {
          ssw.post('/apps/ssw/lite2/auth/login', data, function (result) {
            if (result.error) {
              if (sswSocialLogin) {
                trackShopStats('invalid_social_login', 'all');
              } else {
                trackShopStats('invalid_email_login', 'all');
              }
            } else {
              if (url != "") {
                location.href = '/?checkout_url=' + url;
              } else {
                if (location.pathname.indexOf('account/login') != -1) {
                  location.href = '/';
                } else {
                  location.reload();
                }
              }
            }
          }, 'json');
        }
        sl = 0;
      },
      dataType: 'html'
    }).fail(function (err) {
      ssw.ajax({
        url: '/apps/ssw/lite2/auth/customeridpage',
        success: function (rsp) {
          ssw.cookie('ssw-userchecked', 0, {expires: -1});
          if (parseInt(rsp.match(/<span id="customer-id">([0-9]*)<\/span>/)[1]) > 0) {
            if (url != "") {
              location.href = '/?checkout_url=' + url;
            } else {
              location.reload();
            }
          }
          else {
            if (sswSocialLogin) {
              trackShopStats('invalid_social_login', 'all');
            } else {
              trackShopStats('invalid_email_login', 'all');
            }
            onOpenForm();
            if (sl) {
              addMessage('#login_modal .login-failed', 'Right now we\'re not able to log in you socially. Please type your email and password in the form below to log in.', 'warning');
            } else {
              addMessage('#login_modal .login-failed', 'Invalid login credentials.');
            }
            sl = 0;
            ssw('#hesy-login-submit').removeAttr('disabled').attr('enabled', 'enabled');
          }
        },
        dataType: 'html'
      });
    });
    sswSocialLogin = false;
  }

  this.signUp = function () {
    onOpenForm();
    var data = ssw('#he_create_customer').serialize() + '&_sid=' + ssw.cookie('hesid') + '&service_id=' + service_id + '&hash_key=' + ssw.cookie('hash_key');
    var url = getParameterByName('checkout_url');
    if (url != "") {
      var l = url.split('/');
      data += '&form_type=customer_login&utf8=✓&checkout_url=' + encodeURIComponent(url) + '&cart=' + l[l.length - 1];
    }
    ssw.ajax({
      type: 'POST',
      url: '/account',
      data: data,
      beforeSend: function () {
        onSubmitForm();
        ssw('#hesy-sign-up-submit').removeAttr('enabled').attr('disabled', 'disabled');
        ssw('#signup_modal .ssw-alert').sswAlert('close');
      },
      success: function (html) {
        html = html.split('script').join('div');
        var htmlElement = ssw('<div>').html(html);
        var errors = htmlElement.find('.errors li');
        if (errors.length == 0) {
          errors = htmlElement.find('.errors-list li');
        }
        if (errors.length) {
          onOpenForm();
          ssw('#hesy-sign-up-submit').removeAttr('disabled').attr('enabled', 'enabled');
          errors.each(function () {
            addMessage('#signup_modal .login-failed', ssw(this).text(), 'warning');
          });
        } else {
          ssw.post('/apps/ssw/lite2/auth/register', data, function (result) {
            if (url != "") {
              location.href = '/?checkout_url=' + url;
            } else {
              if (result.error == 0) {
                ssw.cookie('ssw-userchecked', 0, {expires: -1});
                location.reload();
              } else {
                ssw('.login-failed').empty();
              }
            }
          }, 'json');
        }
      },
      dataType: 'html'
    }).fail(function (err) {
      ssw.ajax({
        url: '/apps/ssw/lite2/auth/customeridpage',
        success: function (rsp) {
          ssw.cookie('ssw-userchecked', 0, {expires: -1});
          var t = parseInt(rsp.match(/<span id="customer-id">([0-9]*)<\/span>/)[1]);
          if (t > 0) {
            data += '$customer_id=' + t;
            ssw.post('/apps/ssw/lite2/auth/register', data, function (result) {
              if (url != "") {
                location.href = '/?checkout_url=' + url;
              } else {
                if (result.error == 0) {
                  ssw.cookie('ssw-userchecked', 0, {expires: -1});
                  location.reload();
                } else {
                  ssw('.login-failed').empty();
                }
              }
            }, 'json');
//              if (typeof localStorage != 'undefined' && localStorage.getItem(Shopify.shop + 'login' + localStorage.getItem('login_key')) != null) {
//                localStorage.removeItem(Shopify.shop + 'login' + localStorage.getItem('login_key'));
//              }
//              if (url != "") {
//                location.href = '/?checkout_url=' + url;
//              } else {
//                location.reload();
//              }
          }
          else {
          }
        },
        dataType: 'html'
      });
    });
  }

  var addMessage = function (selector, message, type) {
    type = typeof type !== 'undefined' ? 'ssw-alert-' + type : '';
    ssw(selector).prepend('<div class="ssw-alert ' + type + ' ssw-fade ssw-in"><button class="ssw-close" data-dismiss="ssw-alert">&times;</button>' + message + '</div>');
  }
}
</script>

