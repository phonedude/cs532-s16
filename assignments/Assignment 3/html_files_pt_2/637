<script type="text/javascript">
  var sswReviewBody = document.getElementById("ssw-review-body");
  if(sswReviewBody){
    sswReviewBody.addEventListener("input", function() {
      if(ssw.trim(sswReviewBody.value) == ''){
        sswReviewBody.setCustomValidity("This field is required!");
      }
      else{
        sswReviewBody.setCustomValidity("");
      }
    });
  }

  var sswReviewRate = 0;
  function sswReviewInterval(){
    ssw('#addReviewModal .ssw-stars > i').hover(
        function() {
          var sswReviewIndex = ssw(this).data('value');
          if(sswReviewIndex > sswReviewRate){
            ssw('#addReviewModal .ssw-stars > i:lt(' + sswReviewIndex + ')').removeClass('ssw-icon-star-empty').addClass('ssw-icon-star');
          }
        }, function() {
          if(sswReviewRate){
            ssw('#addReviewModal .ssw-stars > i:gt(' + (sswReviewRate - 1) + ')').removeClass('ssw-icon-star').addClass('ssw-icon-star-empty');
          }
          else{
            ssw('#addReviewModal .ssw-stars > i').removeClass('ssw-icon-star').addClass('ssw-icon-star-empty');
          }
        }
    );

    ssw('#addReviewModal .ssw-stars > i').click(function(){
      sswReviewRate = ssw(this).data('value');
      ssw('#addReviewModal .ssw-stars > i:lt(' + sswReviewRate + ')').removeClass('ssw-icon-star-empty').addClass('ssw-icon-star');
      ssw('#addReviewModal .ssw-stars > i:gt(' + (sswReviewRate -1) + ')').removeClass('ssw-icon-star').addClass('ssw-icon-star-empty');
    });

    ssw('#addReviewForm').submit(function(){
      if(!sswReviewRate){
        var reviewWarningPopover = ssw(this).find('.ssw-stars').data('ssw.sswPopover');
        if(reviewWarningPopover == null){
          reviewWarningPopover = ssw(this).find('.ssw-stars').sswPopover({
            trigger: "manual",
            placement: "bottom",
            content: 'Required',
            ssw_validator: true,
            template: "<div class=\"ssw-popover ssw-validator\"><div class=\"ssw-arrow\"></div><div class=\"ssw-popover-inner\"><div class=\"ssw-popover-content\"><p></p></div></div></div>"
          });
        }
        ssw(this).find('.ssw-stars').sswPopover('show');
        return false;
      }

      ssw('#addReviewSubmit').attr('disabled', true);
      ssw('#addReviewSubmit').addClass('disabled');
      ssw('#addReviewSubmit').val(ssw('#addReviewSubmit').data('loading-text'));
      var data = {
        'body': ssw('#ssw-review-body').val(),
        'rate': sswReviewRate,
        '_sid': sswCookie('hesid'),
        'hash_key': sswCookie('hash_key'),
        'product_id': 0
      };
      ssw.post('/apps/ssw/lite2/reviews/create', data, function(response){
        var sswTemp = ssw('<div>');
        sswTemp.html(response);
        ssw('#addReviewModal').sswModal('hide');
        ssw('.ssw-add-review').remove();
        ssw('.no-ssw-reviews').remove();
        if(sswTemp.find('.ssw-alert').length){
          ssw('.ssw-reviews-witems').before(response);
          ssw('ssw-reviews-page > .recomends_list').before(response);
        }
        else{
          sswExceptViewer = 1;
          ssw('.ssw-reviews-witems').prepend(response);
          ssw('ssw-reviews-page > .recomends_list').prepend(response);
          if(typeof sswReviewAvg != 'undefined' && typeof sswReviewCount != 'undefined'){
            sswReviewAvg = (sswReviewAvg * sswReviewCount + sswReviewRate) / (sswReviewCount + 1);
            sswReviewCount++;
            if(sswReviewCount == 1){
              ssw('#ssw-review-count').html("1 Review");
            }
            else{
              ssw('#ssw-review-count').html(sswReviewCount + " Reviews");
            }
            ssw('#ssw-review-avg > i').each(function(index, value){
              if(sswReviewAvg + 0.25 >= index + 1){
                if(!ssw(value).hasClass('ssw-icon-star')){
                  ssw(value).removeClass('ssw-icon-star-half-alt ssw-icon-star-empty').addClass('ssw-icon-star');
                }
              }
              else if(sswReviewAvg + 0.75 >= index + 1){
                if(!ssw(value).hasClass('ssw-icon-star-half-alt')){
                  ssw(value).removeClass('ssw-icon-star ssw-icon-star-empty').addClass('ssw-icon-star-half-alt');
                }
              }
              else{
                if(!ssw(value).hasClass('ssw-icon-star-empty')){
                  ssw(value).removeClass('ssw-icon-star ssw-icon-star-half-alt').addClass('ssw-icon-star-empty');
                }
              }
            });
          }
        }
      }, 'html');
      return false;
    });
  }
</script>

