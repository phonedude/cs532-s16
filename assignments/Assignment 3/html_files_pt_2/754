			function lPages(P,nId){
				
				if(P == "noticias.php" ){ 
					//alert(P);
					$("#Pages").load("noticias.php?nId="+nId);
				}else{				
					$("#Pages").load(P);
				}
				
			}
			
			function lVota(P, Opt){
				
				//alert(Opt)
				//alert(P+"?votar=" + sugerencias.votar.value + "&valor=" + sugerencias.valor.value)
				
				if(Opt == "C" ){
					$("#Consentidas").load(P);
				}else if(Opt == "S" ){
					$("#Sugerencias").load(P);
				}else if(Opt == "Cv" ){
					$("#Consentidas").load(P+"?votar=" + v_consentidas.votar.value + "&valor=" + v_consentidas.valor.value);
				}else if(Opt == "Sv" ){
					$("#Sugerencias").load(P+"?votar=" + v_sugerencias.votar.value + "&valor=" + v_sugerencias.valor.value);
				}
				
			}
