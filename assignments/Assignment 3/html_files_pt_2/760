                        <script type="text/javascript">
        (function ($) {
            "use strict";

            USN.load('jquery.cookie');

            $(function () {
                var cnameSubscribed = "cms.theReportSubscriber",
                    cookieSubscribed = $.cookie(cnameSubscribed),
                    $form = $('.theReport_emailSignUp_form'),
                    signupURL = '/emailprefs/list-add?list=The%20Report',
                    $initial = $('.theReport_emailSignUp-initial'),
                    $success = $('.theReport_emailSignUp-success');

                $form.submit(function (event) {
                    var $email = $('input[name=email]' ,$form),
                        email = $email.val();
                    event.preventDefault();
                    $.post(signupURL + '&email=' + email).done(function (response) {
                            if (response.result === 'success') {
                                $initial.hide();
                                $success.show();
                                $.cookie(cnameSubscribed, "true", { expires: 365, path: '/' , domain: 'usnews.com'} );
                               // Tealium check
                               if (typeof utag !== 'undefined') {
                                    // Track the link click within Tealium
                                    utag.link({
                                        'event_name': 'email_signup',
                                        'page_url': location.host + location.pathname,
                                        'customer_email': email
                                    });
                                }
                            } else {
                                $initial.show();
                                $success.hide();
                            }
                            // Reset the clearfield state
                            $email.blur();
                        });
                });
            });
        })(window.jQuery);
    </script>
