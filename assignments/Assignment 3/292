var is_home = '';
var category = '';

function render_native_ads(event) {
	if(is_home == '1') {
		for(var i=1; i<13; i++) {
			console.log('300x250_native_ad_home_pos' + i);
			if(event.slot == googleAds['300x250_native_ad_home_pos' + i] && !event.isEmpty) {				
				$("#secondary-hightlight-" + i).detach();
			}
		}
	}
	
	else if (category != ''){
		if(event.slot == googleAds['300x250_native_ad_category_pos1'] && !event.isEmpty) {							
			document.getElementById("native-ad-section-holder").style.display = 'block';			
		}	
	}		
}
