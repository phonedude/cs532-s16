function loadVideo(embedVideoId,callback){$('#videoPlayer-'+embedVideoId).find('.mantle-video-container').html(videoEmbeds[embedVideoId]);$('#videoPlayer-'+embedVideoId).find('.mantle-video-container').find('iframe').attr('id','player'+embedVideoId);$(".mantle-video-container").fitVids();}
$(window).load(function(){var player='';$('.mantle-play').click(function(){$(this).parent().addClass('hide');$(this).parent().next().removeClass('hide');var videoId=$(this).parent().attr('id').split('-')[1];loadVideo(videoId);return false;});var slider=$('#indexFlexslider'),hiddenSlide=slider.find('.slides > li:not(".slides > li.flex-active-slide")');hiddenSlide.show();hiddenSlide.hide();var animatingFrom=0;$('#indexFlexslider').flexslider({slideshow:false,animation:"slide",animationLoop:false,touch:true,video:true,useCSS:false,smoothHeight:false,prevText:"",nextText:"",controlNav:false,before:function(slider){animatingFrom=slider.currentSlide;},after:function(slider){var f=$('#videoPlayer-'+animatingFrom).find('iframe').remove();$('#videoPlayer-'+animatingFrom).addClass('hide');$('#videoInfo-'+animatingFrom).removeClass('hide');},start:function(){function flexHeight(h){$("#TakeoverBridge").css("margin-top",h);}
var playPosition=$('.index-mantle').height()/4.5;$('.index-mantle .mantle-play').css("top",playPosition);$(window).resize(function(){flexHeight($("#indexFlexslider").height());});flexHeight($("#indexFlexslider").height());if($('.flex-active-slide').width()>'970'){var navHeight=$('.header').outerHeight(false);var mantleHeight=$('.index-mantle').outerHeight(true);var heightOffset=navHeight+mantleHeight;$('#TakeoverBackground').css('background-attachment','scroll');$('#TakeoverBackground').css('background-position','50% '+heightOffset+'px');$(window).scroll(function(){var navHeight=$('.header').outerHeight(false);var mantleHeight=$('.index-mantle').outerHeight(true);var heightOffset=navHeight+mantleHeight;var mantleVisibleHeight=$('.index-mantle').offset().top;var viewportHeight=$(window).height();var docScrollTop=$(document).scrollTop();if(docScrollTop>mantleHeight){$('#TakeoverBackground').css('background-attachment','fixed');$('#TakeoverBackground').css('background-position','50% '+navHeight+'px');}else if(docScrollTop<heightOffset){$('#TakeoverBackground').css('background-attachment','scroll');$('#TakeoverBackground').css('background-position','50% '+heightOffset+'px');}});}}});});</script>	</div>

		<div class="ad-banner">
			<div id="gpt-ad-slot-1" class="pagebuilder_ad plate"><script>$(document).on('grind.adsDefined',function(){googletag.cmd.push(function(){googletag.display("gpt-ad-slot-1");});});</script></div>		</div>

