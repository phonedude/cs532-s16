		        						<div class="foot_mobilesite" style="float:right;display:none;"><a href="">Mobile Site</a></div>
						<script type="text/javascript">
							function getCookie(cname) {
									var name = cname + "=";
									var ca = document.cookie.split(';');
									for(var i=0; i<ca.length; i++) {
											var c = ca[i];
											while (c.charAt(0)==' ') c = c.substring(1);
											if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
									}
									return "";
							}
							var fullsiteCookie = getCookie('X-IgnoreUserAgent');
							if (fullsiteCookie == 1){
								// Show only if X-IgnoreUserAgent is set. X-IgnoreUserAgent Cookie is set when clicking "Full Site" on RWD.
								$( ".foot_mobilesite" ).show();
								function setCookie(cname,cvalue,exdays,domain){
									if(exdays){
										var d = new Date();
										d.setTime(d.getTime()+(exdays*6*60*60*1000));
										var expires = " expires="+d.toGMTString();
									}else{var expires = "";}
									var site_domain = (domain) ? " domain="+domain+";" : "";
									document.cookie = cname + "=" + cvalue + ";" + expires +";" + site_domain + " path=/;";
								}
								// VIEW MOBILE SITE
								$('.foot_mobilesite').on('click',function(e){
								 	e.preventDefault();
									var sitehost = window.location.host;
								 	var sitepath = window.location.pathname.replace(/\/$/,"");
									var currentUrl = sitehost.replace("www.","") + sitepath;
									var cookieUrl = sitehost.replace("www.","");
									setCookie('X-IgnoreUserAgent','1','-1',cookieUrl);
									document.location.href = 'http://' + currentUrl;
								});
							}
						</script>
			</div>
		</div>
