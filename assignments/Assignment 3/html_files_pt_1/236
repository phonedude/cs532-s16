			pagin.changepg('next',true,true);
			f5_art_ads();
		})
		$('#pgctrlp').click(function(){
			pagin.changepg('prev',true,true);
			f5_art_ads();
		})
    }
    function buildpagein(){
	    pagdata = '<li id="pgctrlp" class="pgctrl pointer" >Prev</li>';
    	for(tp=1;tp<=totalpg;tp++){
	    	pagdata += '<li id="pgctrl'+tp+'"><a class="pagn8next" style="color:#315680;" href="javascript:void(0)" onclick="pagin.changepg('+tp+',true,true);f5_art_ads();">'+tp+'</a></li>';
    	}
	    pagdata += '<li id="pgctrln" class="pgctrl pointer">Next</li>';
	    $('#artpagein').html(pagdata)
    }
	if(totalpg > 1){pagin.changepg(artcurrent,true,false);}
    $("#pagination1").prepend($("#toppage").html());
