<script type='text/javascript'>
	var eMenu = [];
	var menu = [];
	var cCount = 0;
	 var maxL = 117; 
	 var spingo = 0; 
	var innerHTML = '';
	var subHTML = '';
	function featNav(menu, eMenu, spingo) {
		$.each(menu, function(index, value){
			var fURL  = menu[index].url;
			var fName = menu[index].name;
			$.each(eMenu, function(i, value){
				if (fURL == eMenu[i].url) {
					if (fName == eMenu[i].name) {
						eMenu.splice(i,1);
						return false;
					}
				} else if (eMenu[i].name == 'Blogs') {
					eMenu.splice(i,1);
					return false;
				}    
				else if (eMenu[i].name == 'Calendar') {
					if(spingo == 1) {
						eMenu[i].url = '/section/calendar';
					}
					if (fName == eMenu[i].name) {
					    menu[index].name = 'Local ' + fName;
					}
				} else if (fName == eMenu[i].name) {
					menu[index].name = 'Local ' + fName;
				}
			});
		});
		return $.merge(menu, eMenu);
	}
</script>
