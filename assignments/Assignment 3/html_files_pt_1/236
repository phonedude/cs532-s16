	<script async type="text/javascript">
		/**
		 * Returns an object (from an array of objects) based on a property value
		 */
		function getObjWhenPropertyEquals(prop, val, myArray)
		{
		    for (var i = 0; i < myArray.length; i++)
		    {
		        // check the obj has the property before comparing it
		        if (typeof myArray[i][prop] === 'undefined') {continue;}
		        // if the obj property equals our test value, return the obj
		        if (myArray[i][prop] === val)
		        {
		        	// add array key to the object
		        	myArray[i].arrayKey = i;
		        	return myArray[i];
		        }
		    }
		    // didn't find an object with the property
		    return false;
		}
		/**
		 * Comparison for sorting by story date.  Most recent stories first.
		 */
		function compareStoryDates(a,b) {
		  if (a.date < b.date)
		     return 1;
		  if (a.date > b.date)
		    return -1;
		  return 0;
		}
		/**
		 * Prepare all data for Handlebars template
		 */
		function preprocessAllMainMenuItemData(d)
		{
			// MAIN MENU DATA
			var menuItems = d.main;
			for (var i = 0; i <= menuItems.length-1; i++) {
				// remove "More" from Main Menu
				if (menuItems[i].title=='More')
				{
					menuItems.splice(i,1);
					continue;
				}
				menuItems[i] = preprocessMainMenuItemData(menuItems[i])
			};
			d.main = menuItems;
			return d;
		}
		/**
		 * Prepare single menu item data for Handlebars template
		 * Used when updating a menu item on hover
		 */
		function preprocessMainMenuItemData(menuItem)
		{
			if(menuItem)
			{
				// SUB NAV ITEMS
				var menuItemData = preprocessMainMenuSubNavItems(menuItem);
				// STORIES
				menuItemData = preprocessMainMenuStories(menuItem);
				// add additional functions to preprocess any other data here.
			}
			return menuItemData;
		}
		/**
		 * Preprocess Sub Nav Items
		 */
		 function preprocessMainMenuSubNavItems (menuItem)
		 {
			var maxSubNavItems = 6;
			if (menuItem.subitems)
			{
				var subnavItems = menuItem.subitems;
				for (var j = 0; j <= menuItem.subitems.length-1; j++) {
					// REMOVE TOP STORIES
					if(subnavItems[j].title=="Top Stories")
					{
						menuItem.subitems.splice(j,1);
					}
				};
				// LIMIT TO MAX ITEMS
				if (menuItem.subitems.length > maxSubNavItems)
				{
					menuItem.subitems.splice(maxSubNavItems,menuItem.subitems.length);
				}
				// INJECT Nation & World INTO News
				// INJECT shareable INTO News
				// INJECT Databases INTO News
				// INJECT Auto Racing INTO Sports
				if(menuItem.title.toString() == "Sports") {
					var temp = {};
					temp.id = 'sportsAutoRacing';
					temp.target = '';
					temp.link = '/sports/motor-racing-network';
					temp.title = 'Auto Racing';
					menuItem.subitems.push(temp);
				}
				// INJECT CALENDAR INTO ENTERTAINMENT
				if(menuItem.title.toString() == "Entertainment") {
					var temp = {};
					temp.id = 'eventsCalendar';
					temp.target = (calendarVendor=='eviesays') ? '_blank' : '';
					temp.link = (calendarVendor=='eviesays') ? 'http://wicked-concord.eviesays.com' : '/section/calendar';
					temp.title = 'Calendar';
					menuItem.subitems.push(temp);
				}
							}
			return menuItem;
		 }
		/**
		 * Preprocess Stories
		 */
		function preprocessMainMenuStories (menuItem) {
			var maxStories = 4;
			if (menuItem.stories && menuItem.stories.length > 0)
			{
				menuItem.featured = [];
				var featImageCount = 0;
				var stories = menuItem.stories;
				for (var j = 0; j <= stories.length-1; j++) {
					// BUILD FEATURED STORIES
					// copy stories to "featured" property, grab stories with images if they exist.
					if(stories[j].image && featImageCount<2)
					{
						featImageCount++;
						menuItem.featured.push(menuItem.stories[j]);
						menuItem.stories.splice(j,1);
					}
				};
				// BUILD FEATURED STORIES (cont.)
				// Didn't find images for all items
				if (featImageCount == 1)
				{
					// Only 1 image, add the first story available
					menuItem.featured.push(menuItem.stories[0]);
					menuItem.featured.sort(compareStoryDates);
					menuItem.stories.splice(0,1);
				}
				else if (!featImageCount)
				{
					// No images, add the first 2 stories from "stories"
					menuItem.featured = [menuItem.stories[0],menuItem.stories[1]];
					menuItem.stories.splice(0,2);
				}
				// LIMIT TO MAX ITEMS
				if (menuItem.stories.length > maxStories)
				{
					menuItem.stories.splice(maxStories,menuItem.stories.length);
				}
			}
				return menuItem;
		}
		/**
		 * Build Mega Menu for all nav items
		 */
		function buildAllMegaMenus(d)
		{
			// BUILD ALL MEGA MENU ITEMS
			var menuItems = d.main;
			for (var i = 0; i <= menuItems.length-1; i++) {
				buildMegaMenu(menuItems[i]);
			}
		}
		/**
		 * Build single mega menu
		 */
		function buildMegaMenu(menuItemData)
		{
			console.log(menuItemData);
			var megaMenuTemplate = getNavView('mega-menu',menuItemData);
			$('#main-menu #'+menuItemData.id+' .mega-menu').html(megaMenuTemplate);
		}
		/**
		 * Build Main Menu
		 */
		function buildMainMenu(mainMenuTemplate)
		{
			$('#main-menu-wrap .centerwidth').prepend(mainMenuTemplate);
		}
		/**
		 * Build Default Menu
		 */
		 function buildDefaultMenu()
		 {
			var defaultMenuData = {"site_url":"http://concord.wickedlocal.com","obits":"http://www.legacy.com/obituaries/wickedlocal-concord/"}
			var mainMenuTemplate = getNavView('default-menu',defaultMenuData);
			buildMainMenu(mainMenuTemplate);
		 }
		/**
		 * Update Mega Menus
		 */
		 function updateMegaMenu(mainMenuItemId)
		 {
		 	$.getJSON( "http://concord.wickedlocal.com/section/nav-model&mime=application%2Fjson", function( data ) {
				var menuItem = getObjWhenPropertyEquals('id', mainMenuItemId, data.main);
				if (menuItem)
				{
					data.main[menuItem.arrayKey] = preprocessMainMenuItemData(menuItem);
					buildMegaMenu(data.main[menuItem.arrayKey]);
				}
			});
		 }
		/**
		 * Gets the Handlebars Template
		 */
		function getNavView(templateName,data)
		{
			if (templateName in Handlebars.templates)
			{
				var template = Handlebars.templates[templateName](data);
				return template;
			}
			else
			{
				console.log("Handlebars template: '"+templateName+"' doesn't exist.");
				return false;
			}
		}
		// DRAW MAIN NAV
		$.getJSON( "http://concord.wickedlocal.com/section/nav-model&mime=application%2Fjson", function( data ) {
			var mainMenuData = preprocessAllMainMenuItemData(data);
			var mainMenuTemplate = getNavView('main-menu',mainMenuData);
			if (mainMenuTemplate)
			{
				buildMainMenu(mainMenuTemplate);
				buildAllMegaMenus(mainMenuData);
			}
			else
			{
				buildDefaultMenu();
			}
		})
		.fail(function() {
			buildDefaultMenu();
		});
	</script>
