<script type="text/javascript">
var featNav = featNav(menu, eMenu, spingo);
var navSize = featNav.length;
var x = 1;
$.each(featNav, function(index, value){
	cCount += featNav[index].name.length;
	if ((cCount < maxL) && (cCount + 13 > maxL)) {
		cCount += 13;
		if(x == navSize) {
			innerHTML += '<li><a class="mmenu-a" style="color:#fff" href="' + featNav[index].url + '">' + featNav[index].name + '</a></li>';
		} else {
			subHTML += '<li><a class="mmenu-a" style="color:#fff" href="' + featNav[index].url + '">' + featNav[index].name + '</a></li>';
		}
	} else if (cCount < maxL) {
		innerHTML += '<li><a class="mmenu-a" style="color:#fff" href="' + featNav[index].url + '">' + featNav[index].name + '</a></li>';
	} else {
		subHTML += '<li><a class="mmenu-a" style="color:#fff" href="' + featNav[index].url + '">' + featNav[index].name + '</a></li>';
	}
	x++;
});
 if(subHTML != '') {
	innerHTML += '<li id="explorer-nav">&nbsp;|&nbsp;&nbsp;<a>EXPLORE&nbsp;&raquo;</a>';
		 innerHTML += '<div id="explorer-child"><ul>';
			innerHTML += subHTML;
		innerHTML += '</ul></div>';
	innerHTML += '</li>';
 }
	var origUL = $('.featbar-cont .secmenu ul').html();
	$('.featbar-cont .secmenu ul').html(origUL + innerHTML);
$('#explorer-nav').mouseover(function(){ $('#explorer-child').show();});
$('#explorer-child').mouseover(function(){ $('#explorer-child').show();});
$('#explorer-nav').mouseout(function(){ $('#explorer-child').delay(1000).hide(); });
$('#explorer-child').mouseout(function(){ $('#explorer-child').hide(); });
$("div#explorer-child ul li a:contains('Your Photos')").parent('li').hide();
</script>


