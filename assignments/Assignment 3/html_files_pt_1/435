<script type="text/javascript">
	var url = window.location.href.toLowerCase();
	if(url.indexOf("viewassessments.aspx")>=0){document.write("</td></tr></table>");}
	$(document).ready(function(){
		var curl = window.location.href.toLowerCase();
		if(curl.indexOf("searchopenings.aspx")>=0){
			$("div#ctl00_Header table:eq(0)").attr({"cellpadding":"0px", "cellspacing":"0px"});
		}
		if($.browser.msie && $.browser.version=="7.0"){
			$("img[src*='dg_careers_logo.jpg']").parent().parent().parent().parent().parent().css("width", "1024px");
		}
		//IE Specific issue fix for Tab Style Page title below gap between the grid.
		if($.browser.msie && $.browser.version=="8.0" && curl.indexOf("resumemanager.aspx")>=0){
			$("div#ctl00_MainContent_GridFormatter_datatable").css({"margin-top":"-10px"});
		}	
		
		if(curl.indexOf("viewassessments.aspx")>=0){
			if($.browser.msie && $.browser.version=="7.0"){
				$("div#BrandingHeader").css("margin-top","-16px");
			}else{
				$("div#BrandingHeader").css("margin-top","-8px");
			}
		}
		//Removing underlines and bold texts in progress bar.
		if(curl.indexOf("aip.aspx")>=0){
			$("span.COOKIEcrumbtext a, table.breadcrumbtable span.COOKIEcrumbtext a strong u, table.breadcrumbtable span.COOKIEcrumbtext a b").css({"text-decoration":"underline", "font-weight":"normal"});
			$(".PAGEtitle:gt(0)").css({"font-size":"12px", "font-family":" Arial,Helvetica,sans-serif", "font-weight":"bold"});
		}
		
		//Adjusting Page Titles.
		if(curl.indexOf("defaultlogin.aspx")>=0 || curl.indexOf("employeeprofile.aspx")>=0){
			$("h1.PAGEtitle").parent().prev().css({"display":"none"});	
			$("h1.PAGEtitle").parent().attr("align","left");//Only when then want to align the page title to the left side.
		}
		
		//Removing unnecessary break tags & underlines and bold texts in progress bar.
		if(curl.indexOf("attachments.aspx")>=0){
			$("#hdFileSelCount").parent().find("br").css({"display":"none"}); 
			$("span.COOKIEcrumbtext a, table.breadcrumbtable span.COOKIEcrumbtext a strong u, table.breadcrumbtable span.COOKIEcrumbtext a b").css({"text-decoration":"underline", "font-weight":"normal", "font-size":"11px"});
			$(".COOKIEcrumbtext, .TEXT").css({"font-weight":"normal", "font-size":"11px"});
			$(".PAGEtitle").css({"font-size":"18px", "font-family":"Arial,Helvetica,sans-serif", "font-weight":"bold"});
			$(".PAGEtitle:gt(0)").css({"font-size":"16px", "font-family":"Arial,Helvetica,sans-serif", "font-weight":"bold"});
		}
		
		//Over riding other than PAGETitle class elements except page headings.
		if(curl.indexOf("attachform.aspx")>=0 || curl.indexOf("answerquestions.aspx")>=0){
			$(".PAGEtitle:gt(0)").css({"font-size":"11px", "font-family":"Arial,Helvetica,sans-serif"});
		}


	});
