        function sendToFriend()
        {
	        openWindowBySize2('about:blank?sessionid=7bd9030469d0bd4dfe90d32dac22931e2c7d', 450, 360);	        
	        document.frmSendToFriend.submit();
        }
        
        function socialNetworkReferral(button) {
		  if (SR_READY == "yes")
            LaunchSocialReferralMenu(button);
        else {
            document.frmSocialNetworkReferral.ButtonId.value = button.id;
            openWindowBySize2('about:blank?sessionid=7bd9030469d0bd4dfe90d32dac22931e2c7d', 900, 720);
            document.frmSocialNetworkReferral.submit();
	        }
		}
        
        
        function showjob(lngNewSelection)
	    {   		    
		    document.frmJobDetail.action = "jobdetails.aspx?partnerid=25452&amp;siteid=5545&amp;jobId=2230837";
		    document.frmJobDetail.CurSel.value= lngNewSelection;
		    document.frmJobDetail.submit();
	    }
        
        function launchSocialMedia()
	    {
	        var lngSiteLocale = '1033';
	        var jobSiteID = '5545';
	        var newLocaleId = getLocaleId(jobSiteID,'');
	        var strSwitchSiteQS = '';
	        

	        if (newLocaleId == lngSiteLocale || newLocaleId == '')
		    {
			    openWindow1('about:blank?sessionid=7bd9030469d0bd4dfe90d32dac22931e2c7d');
		        document.frmSocialMedia.submit();
		    }
		    else
		    {
		        alert(MSG_SocialMediaSiteSwitch);
		        if(strSwitchSiteQS == '')
		        {
			        document.frmMassApply.action= "apply.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&src=jdsm&qs=partnerid%3d25452%26siteid%3d5545%26jobId%3d2230837";
			    }
			    else
			    {
			        document.frmMassApply.action= "apply.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&src=jdsm&qs=";
			    }
			    document.frmMassApply.submit();
		    }
        }
        
        //Save to jobcart
        function saveToJobCart()
	    {
		    var strTemp;
		    var lIdx;
		    var lLen;
		    var arrSubInfo;

		    if (document.frmJobDetail.jobcartcnt.value > 50)
			    alert (MSG_cim_jobdetail_JobCart);
		    else
		    {
			    document.frmSaveToCart.chkJobClientIds.value = ReplaceInString(document.frmSaveToCart.chkJobClientIds.value, "%%", "");
			    arrSubInfo = document.frmSaveToCart.chkJobClientIds.value.split("|");		    

			    if(arrSubInfo.length != 3)
			    {
				    strTemp =  arrSubInfo[0] + "|" + arrSubInfo[1];
				    document.frmSaveToCart.chkJobClientIds.value = strTemp;
			    }
			    openWindow3('about:blank?sessionid=7bd9030469d0bd4dfe90d32dac22931e2c7d');
			    document.frmSaveToCart.submit();
		    }
	    }
	    function goToSearchPage()
	    {	   
		    document.frmSearch.submit();
	    }
	    
	    //QC #197940
	    function reload(sParam)
	    {
		if (typeof(sParam) != 'undefined') 
		{
			if (sParam == 'ApplyAsRegularJob')
				ApplyAsRegularJob();
			return;
		}
		var sGQId =  "0";
		var arrGQId;
		var iLen = 0;
		if (sGQId == '' )
		{
			sGQId= document.frmJobDetail.JobInfo.value;
			arrGQId = (sGQId.substring(2, sGQId.length - 2)).split("%%");
			iLen = arrGQId.length;
		}
		
		if ((iLen == 1) || (sGQId != ''))
		{
			 
		document.frmJobDetail.target = "_self";
		 
		document.frmJobDetail.action = "Logout.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&gqclose=yes&crl=no&Exit=^";
		document.frmJobDetail.submit();
		}
	  }
