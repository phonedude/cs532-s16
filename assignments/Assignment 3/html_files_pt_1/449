    <script language="javascript" type="text/javascript">  
        var SR_READY = "";
        var SR_PRIDE = "%5eXPo1WBzaZuM%3d";
        var SR_STID = "5545";
        var SR_CLID = "25452";
        var SR_LOCALE = "1033";
        var SR_FN = "";
        var SR_LN = "";        
       function setReadyForSocialReferral(ready, clid, stid, pride, localeid,button,fn,ln) {
            SR_READY = ready;
            SR_PRIDE = pride;
            SR_STID = stid;
            SR_CLID = clid;
            SR_LOCALE = localeid;
            SR_FN = fn;
            SR_LN = ln;
            if (SR_READY == "yes") {
                LaunchSocialReferralMenu(document.getElementById(button));
            }
        }
        function LaunchSocialReferralMenu(button) {
            var jobstr = document.getElementById("jobid").value;
            var urlstring = "../tgnewui/referral/home/referralworkflowoptions?siteid=" + SR_STID + "&partnerid=" + SR_CLID + "&localeid=" + SR_LOCALE + "&bruid=" + SR_PRIDE + "&jobids=" + jobstr+ "&fn=" + SR_FN + "&ln=" + SR_LN
            window.dialogBuilder.showDialog(button, urlstring);

        }     
        function ApplyNow()
	    {			    
			var ApplyURL = "-1";
			if (ApplyURL=="-1" ) ApplyURL= "";
			if (ApplyURL.length>0 )
			{				
			    window.location.href=ApplyURL; 
			}
			else
			//In case GTG if the job siteid is different than the current siteid
			//display warning pop-up to confirm site switched to job site
			{			   
				ApplyJobNow('1033');
			}
		}		
			
		function ApplyJobNow(strLocale)
        {
	        var strGQId;
	        var iLen, iIndx;
	        var arrGQId;
	        var arrGQIds;
	        var lngNewSelection = 0;
	        var lngSiteLocale = 1033;
	        var strSwitchSiteQS;
	        strGQId = "0";	// from Querystring
	        strSwitchSiteQS = "";

	        //var lngSelect = 1;
	        var lngSelect = 1;
	        
	        if  (strGQId == '')
	        {	
	            strGQId= document.frmJobDetail.JobInfo.value;
			    arrGQId = (strGQId.substring(2, strGQId.length - 2)).split("%%");
			    iLen = arrGQId.length;
			    arrGQIds = arrGQId[lngSelect-1].split('|');
			    if (arrGQIds[2] != 0)
			    {
				    if (strLocale == lngSiteLocale)
				    {
				        if (js_LaunchGQ_DotNet.toUpperCase() == 'YES')
				        {
				            openWindow11("gqlogin.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&fjd=true&referer=&gqid="+ arrGQIds[2] + "&jobinfo=__" + arrGQId[lngSelect-1] + "__&applycount=1&type=search_jobdetail");
				        }
				        else
				        { 
					        openWindow11("https://sjobs.brassring.com/" + strLocale + "/asp/tg/GQLogin.asp?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&fjd=true&referer=&gqid="+ arrGQIds[2] + "&jobinfo=__" + arrGQId[lngSelect-1] + "__&applycount=1&type=search_jobdetail");
					    }
				    }
				    else
				    {
				        document.frmMassApply.GQID.value = arrGQIds[2];
				        if(strSwitchSiteQS == '')
				        {
					        document.frmMassApply.action= "apply.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&src=jd&qs=partnerid%3d25452%26siteid%3d5545%26jobId%3d2230837";
					    }
					    else
					    {
					        document.frmMassApply.action= "apply.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&src=jd&qs=";
					    }
					    document.frmMassApply.submit();
				    }
			    }
			    else
			    {
				    document.frmMassApply.action= "apply.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d";
				    document.frmMassApply.submit();
			    }	        
			        		        
	        }
	        else
	        {
	           	
		        if (strGQId != 0)
		        {
			        if (strLocale == lngSiteLocale)
			        {
			            if (js_LaunchGQ_DotNet.toUpperCase() == 'YES')
				        {
				            openWindow11("gqlogin.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&fjd=true&referer=&gqid=" + document.frmMassApply.GQID.value + "&jobinfo=__2230837|1|0__&applycount=1&type=search_jobdetail");
				        }
				        else
				        { 
				            openWindow11("https://sjobs.brassring.com/" + strLocale + "/asp/tg/GQLogin.asp?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&fjd=true&referer=&gqid=" + document.frmMassApply.GQID.value + "&jobinfo=__2230837|1|0__&applycount=1&type=search_jobdetail");
				        }
			        }
			        else
			        {
			            if(strSwitchSiteQS == '')
				        {
				            document.frmMassApply.action= "apply.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&src=jd&qs=partnerid%3d25452%26siteid%3d5545%26jobId%3d2230837";
				        }
				        else
				        {
				            document.frmMassApply.action= "apply.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d&src=jd&qs=";
				        }
				        document.frmMassApply.submit();
			        }
 	            }
 	            else
 		        {
 			        document.frmMassApply.action = "apply.aspx?SID=^m3dC4PqmqFdrtQAdqmkJjT_slp_rhc_NMvsQIyRGgOc_slp_rhc_wImB9/BkxqNPvBfS7y_slp_rhc_zJGvar41d";
			        document.frmMassApply.submit();
 		        }
 	        }
        }
        
