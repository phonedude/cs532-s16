<script type="text/javascript" src="/documents/siteManager/mx/js/xmlhttprequest.js"></script>
<script type="text/javascript">
//<![CDATA[

var URL_PATH = "index.cfm";  var XHR = getHTTPObject();
  
  var sURL = "";

  /* called to begin XMLHTTPRequest activity */
  function XHR_ClearSession()
  {  	
  	XHR.open("POST", URL_PATH, true);
		XHR.setRequestHeader('Content-Type','application/x-www-form-urlencoded');
		XHR.onreadystatechange = handleXHR_ClearSession;
 		XHR.send("fuseaction=login.logout");
  }
 
  /* used to output data received via ajax */
  function handleXHR_ClearSession()
  {
    if (XHR.readyState == 4 && XHR.status == 200)
    {
    	/* handle popup new window */
      newwindow = open(sURL+"&blnNewWindowStart=true","newwin", "scrollbars=yes,toolbar=no,directories=no,menubar=no,resizable=yes,status=yes,width=660,height=500,left=10,top=10");
	
	    try
	    {
	      var obj = newwindow.name;
        document.getElementById('popupMessage').style.display = "none";
	    }
	    catch(e)
	    {    	
        if (newwindow != null && !newwindow.closed)
	      {
	        newwindow.focus();
	      }
	      document.getElementById('popupMessage').style.display = "block";
	    }
    }
  }

  /* begin application */
  function openit(URL)
  {		
  	  sURL = URL;
  	
  	  //remote call to login.logout
      XHR_ClearSession();
  } 
  
  /* send to friend */
  function new_window(url)
  {
    link = window.open(url,"Link","toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=0,resizable=1,width=660,height=400,left=20,top=40");
  }
  
//]]>
</script>




	
