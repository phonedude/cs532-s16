$( document ).ready(function(){
    // Pattern to find all instances of http://, https://, ftp
    
    var urlPattern = /\b(?:https?|ftp):\/\/[a-z0-9-+&@#\/%?=~_|!:,.;]*[a-z0-9-+&@#\/%=~_|]/gim;
    
    // Should be improved
    // currently replaces the whole section and some <br/>'s are lost.
    // Could mess up other styling
    var newText = $("#direct_jobDescriptionText").html().replace(urlPattern, '<a target="_blank" href="$&">$&</a>');
    $("#direct_jobDescriptionText").html(newText);
});

</script>

