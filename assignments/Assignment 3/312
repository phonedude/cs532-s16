				<!--
				/**
				 * Changes the language when clicking on an item in the language dropdown.
				 */
				
				DomReady.ready( function() {
					
					// display the currently active selection
					jQuery("#language-select")
						.find('.btn-value')
						.text(jQuery("#language-dropdown li.active").text());
					
					jQuery("#language-dropdown li a").click(function(e){
						jQuery("#language-dropdown").find("li.active").removeClass('active');
						jQuery(this).parents("li").addClass('active');
						jQuery(this).parents(".input-group-btn").find('.btn-value').text(jQuery(this).text());
						
						// change the search type value for the form
						jQuery("#change_user_language").val(jQuery(this).data("value"));
						
						submitForm(jQuery("#language_select"));
						
						e.preventDefault();
						return false;
					})
				});
				
