<script type="text/javascript">
GetCaptchaContactUsBody8843();

function CaptchaSendComment8843() {
    document.getElementById('animationDiv28843').style.display = 'block';
    document.getElementById('buttonSendDiv28843').style.display = 'none';
    var caTxt = document.getElementById('txtCA2_8843').value;
    var caCd = document.getElementById('hidCA2_8843').value;

    var caSubject = document.getElementById('ctl00_ctl00_cpAB_desktopRR_ctl05_ctl02_desktopRL_ctl03_ctl03_txtSubject').value;
    var caEmail = document.getElementById('ctl00_ctl00_cpAB_desktopRR_ctl05_ctl02_desktopRL_ctl03_ctl03_txtEmlFrom').value;
    var caEmailToCntrl=document.getElementById('ctl00_ctl00_cpAB_desktopRR_ctl05_ctl02_desktopRL_ctl03_ctl03_ddlEmlTo');
    
    var caEmailTo='';
    if(caEmailToCntrl==null)
        caEmailTo='123';
    else
        caEmailTo=caEmailToCntrl.value;

    var caComment = document.getElementById('ctl00_ctl00_cpAB_desktopRR_ctl05_ctl02_desktopRL_ctl03_ctl03_txtBody').value;
    if(typeof www != 'undefined' && typeof www.rferl != 'undefined' && typeof www.rferl.org != 'undefined' && typeof www.rferl.org.Services != 'undefined' && typeof www.rferl.org.Services.CaptchaService != 'undefined' && typeof www.rferl.org.Services.CaptchaService.CaptchaContactUsSendComment != 'undefined')//For CMS
        www.rferl.org.Services.CaptchaService.CaptchaContactUsSendComment('8843',caSubject, caEmail, caEmailTo, caComment,caTxt,caCd,CaptchaContactUsSendCallback8843);
}
function GetCaptchaContactUsBody8843() {
    if (true)
    {
        var panel = document.getElementById('captchaDiv28843');
        panel.innerHTML = '<input type="hidden" value="disabled" id="txtCA2_8843"/><input type="hidden" value="disabled" id="hidCA2_8843" />';
    }
    else if(typeof www != 'undefined' && typeof www.rferl != 'undefined' && typeof www.rferl.org != 'undefined' && typeof www.rferl.org.Services != 'undefined' && typeof www.rferl.org.Services.CaptchaService != 'undefined' && typeof www.rferl.org.Services.CaptchaService.GetCaptchaBody != 'undefined')//For CMS
        www.rferl.org.Services.CaptchaService.GetCaptchaBody(2,8843,CaptchaContactUsCallback8843,ErrorCallbackContact);
} 
function CaptchaContactUsCallback8843(result){
    if(result && result!='')
    {
        var panel = document.getElementById('captchaDiv28843');
        panel.innerHTML =result;
    }
}
function CaptchaContactUsSendCallback8843(result){
    document.getElementById('animationDiv28843').style.display = 'none';
    document.getElementById('buttonSendDiv28843').style.display = 'block';
    
    var sendButtonOffs = $('#buttonSendDiv28843').offset();
    var positionPanel = function (oBox) {
        oBox.css("top", sendButtonOffs.top).css("left", sendButtonOffs.left - oBox.outerWidth(true));
    }

    if(result && result!='')
        positionPanel(smallMessageBox({html : result}));
    else
    {
        positionPanel(smallMessageBox({html : '您的邮件发送成功！'}));
        document.getElementById('ctl00_ctl00_cpAB_desktopRR_ctl05_ctl02_desktopRL_ctl03_ctl03_txtBody').value='';
    }
    GetCaptchaContactUsBody8843();
}
function ErrorCallbackContact(error)
{
}
