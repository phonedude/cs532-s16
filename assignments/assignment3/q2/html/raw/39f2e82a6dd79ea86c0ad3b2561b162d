<script>
//TICKER_CONTENT = document.getElementById("TICKER").innerHTML;
TICKER_RIGHTTOLEFT = false;
TICKER_RIGHTTOLEFTCORTO = false;
TICKER_SPEED = 2;
TICKER_PAUSED = false;
var intervalo_ticker;
function ticker_start(){
var tickerSupported = false;
TICKER_WIDTH = document.getElementById("TICKER").style.width;
var uList=document.getElementById("TICKER").getElementsByTagName('ul')[0];
var liTag = document.createElement('li');
var ttexto=[];
for(var i=0;i<(parseInt(TICKER_WIDTH)/3);i++){
ttexto.push("&nbsp;");
}
liTag.innerHTML=ttexto.join("");
uList.insertBefore(liTag,uList.firstChild);
TICKER_CONTENT = document.getElementById("TICKER").innerHTML;
document.getElementById("TICKER").innerHTML = "<DIV style='width:100%; white-space: nowrap'><DIV ID='TICKER_BODY' width='100%'></DIV></DIV>";
tickerSupported = true;
if (!tickerSupported)
document.getElementById("TICKER").outerHTML = "";
else {
document.getElementById("TICKER").scrollLeft = TICKER_RIGHTTOLEFT ? document.getElementById("TICKER").scrollWidth - document.getElementById("TICKER").offsetWidth : 0;
document.getElementById("TICKER_BODY").innerHTML = TICKER_CONTENT;
document.getElementById("TICKER").style.display = "block";
intervalo_ticker = window.setInterval(TICKER_tick,30);
}
}
function TICKER_tick(){
if (!TICKER_PAUSED)
document.getElementById("TICKER").scrollLeft += TICKER_SPEED * (TICKER_RIGHTTOLEFT ? -1 : 1);
if (TICKER_RIGHTTOLEFT && document.getElementById("TICKER").scrollLeft <= 0) {
document.getElementById("TICKER").scrollLeft = document.getElementById("TICKER").scrollWidth - document.getElementById("TICKER").offsetWidth;
}
if (!TICKER_RIGHTTOLEFT && document.getElementById("TICKER").scrollLeft >= document.getElementById("TICKER").scrollWidth - document.getElementById("TICKER").offsetWidth) {
document.getElementById("TICKER").scrollLeft = 0;
}
}
function parar_arrancar_TICKER(){
if (TICKER_PAUSED){
TICKER_PAUSED = false;
intervalo_ticker = window.setInterval(TICKER_tick,30);
toggleClass('id_para_arranca_ticker','parar');
toggleClass('id_para_arranca_ticker','play');
} else {
TICKER_PAUSED = true;
window.clearInterval(intervalo_ticker);
toggleClass('id_para_arranca_ticker','play');
toggleClass('id_para_arranca_ticker','parar');
}
}
function izquierda_TICKER(){
if (TICKER_RIGHTTOLEFT){
TICKER_RIGHTTOLEFT = false;
toggleClass('id_izquierda_ticker','deshabilitada');
toggleClass('id_derecha_ticker','deshabilitada');
}
}
function derecha_TICKER(){
if (!TICKER_RIGHTTOLEFT){
TICKER_RIGHTTOLEFT = true;
toggleClass('id_izquierda_ticker','deshabilitada');
toggleClass('id_derecha_ticker','deshabilitada');
}
}
var numerolis = document.getElementById("TICKER").getElementsByTagName('li').length;
var anchoul = ((numerolis * 230)+1732);
if(document.getElementById("TICKER").getElementsByTagName('li').length > 4){
var divTicker = document.getElementById("TICKER");
divTicker.className += " con_movimiento" ;
document.getElementById("acciones_cortas").style.display='none';
document.getElementById("acciones").style.display='block';
var ulTag = document.getElementById("TICKER").getElementsByTagName('ul')[0];
var liTag = document.createElement('li');
ulTag.style.width = anchoul+'px';
liTag.style.width = '866px';
ulTag.appendChild(liTag);
liTag = document.createElement('li');
liTag.style.width = '866px';
ulTag.insertBefore(liTag,ulTag.firstChild);
ticker_start();
}else{
var c = document.getElementById("TICKER").getElementsByTagName('li');
document.getElementById("acciones").style.display='none';
document.getElementById("acciones_cortas").style.display='none';
var l = c.length;
// Recorro los li para añadir su ancho
var ancho_completo=0;
for(var i=0; i<l; i++){
ancho_completo += jQuery(c[i]).outerWidth();
}
if (ancho_completo > 800)
{
document.getElementById("acciones_cortas").style.display='block';
jQuery("#TICKER").width('761px');
}
}
</script>
