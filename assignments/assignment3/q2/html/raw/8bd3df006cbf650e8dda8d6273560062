               <script>
                    function FOL_TFT_Servicemenu_Show_Message(message) {
                    	 $(".zendesk-form-wrapper > div").html(message);
                    	 $(".zendesk-form-wrapper .toggable").toggle();
                         $( "#zendesk-form" ).get(0).reset();
                         setTimeout(function(){ $('#zendesk-wrapper').toggleClass('slideout'); }, 5000);
                    }
                    $('#zendesk-button').click(function () {
                       $('#zendesk-wrapper').toggleClass('slideout');
                       if (!$('#zendesk-form').is(':visible')) {
                           $(".zendesk-form-wrapper .toggable").toggle();
                       } else {
                            try {
                                FOL_GTM_PushEvent('Feedback', 'clicked', 'Layer_Left');
                                TFT.Core.Console.log('Servicemenu','Clicked Feedback Form');
                            } catch (e) {}
                       }
                   });
                   $( "#zendesk-form" ).submit(function( event ) {
                         event.preventDefault();
                         var $form = $( this ),
                         url = $form.attr( "action" );
                         $.ajax({
                               contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                               data: $form.serialize(),
                               type: 'POST',
                               url: url
                             }).done(function(data, textStatus, jqXHR) {
                                 console.log(data);
                                 var response = $.parseJSON(data);
                                 if (response.status === 201) {
                                      console.log(textStatus);
                                      FOL_TFT_Servicemenu_Show_Message("<strong>Vielen Dank für Ihre Anfrage!</strong><br />Ihre Nachricht wird in Kürze bearbeitet.");
                                      try {
                                          FOL_GTM_PushEvent('Feedback', 'sent', 'Layer_Left');
                                          TFT.Core.Console.log('Servicemenu','Sent Feedback Form');
                                      } catch (e) {}
                                 } else {
                                      FOL_TFT_Servicemenu_Show_Message("<strong>Leider ist ein Fehler aufgetreten.</strong><br />Bitte versuchen Sie es später noch einmal!");
                                 }
                              }).fail(function(jqXHR, textStatus, errorThrown) {
                            	  FOL_TFT_Servicemenu_Show_Message("<strong>Leider ist ein Fehler aufgetreten.</strong><br />Bitte versuchen Sie es später noch einmal!");
                                  console.log(textStatus);
                             });
                         });
               </script>
