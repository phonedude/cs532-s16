		            <script>
		            NavCorrAvailWidth=996-parseInt($('#header-navigation #navigation ul.navigation').css('left'))+8;
		            NavCorrCntItems=($('#header-navigation #navigation ul.navigation li').length-1)*2;
		            NavCorrCntDiff = NavCorrAvailWidth-$('#header-navigation #navigation ul.navigation').width();
		            NavCorrCntDiffPerItem = Math.round((NavCorrCntDiff/NavCorrCntItems * 100))/100;
		            NavCorrectFactor = NavCorrCntDiff - (NavCorrCntDiffPerItem*NavCorrCntItems);
		            $('#header-navigation #navigation ul.navigation li a').css({'padding-left':(NavCorrCntDiffPerItem + 7)+'px','padding-right':(NavCorrCntDiffPerItem + 7)+'px'});
		            $('#header-navigation #navigation ul.navigation li.endline').css({'margin-left':NavCorrectFactor+'px'});
