eo(contentId, containerId);},onContentEnd: function (containerId, cvpId, contentId) {/* Add a VisualRevenue event on video end */if (Array.isArray(window._vrq)) {window._vrq.push(['video', 'stop', configObj.video]);}if (Modernizr && !Modernizr.phone && !Modernizr.mobile && !Modernizr.tablet) {if (typeof videoPinner !== 'undefined' && videoPinner !== null) {videoPinner.setIsPlaying(false);}}},onCVPVisibilityChange: function (containerId, cvpId, visible) {CNN.VideoPlayer.handleAdOnCVPVisibilityChange(containerId, visible);}};if (typeof configObj.context !== 'string' || configObj.context.length <= 0) {configObj.context = 'tech'.replace(/[\(\)\-]/g, '');}if (typeof configObj.adsection === 'undefined' && typeof window.ssid === 'string' && window.ssid.length > 0) {configObj.adsection = window.ssid;}if (CNN.VideoPlayer.apiInitialized) {window.cnnVideoManager.renderMultipleContainers({videoConfig:configObj, eventsCallback:callbackObj});} else {CNN.VideoPlayer.addVideo(configObj, callbackObj);}});</script><div class="js-media__video media__video" id="media__video_large-media_0--wrapper"><div id="large-media_0--thumbnail" class="media__video--thumbnail"><img class="media__image media__image--responsive" alt="SpaceX mission takes 3-D printer to space." data-src-mini="http://i2.cdn.turner.com/cnnnext/dam/assets/140921092316-vo-spacex-lauch-to-international-space-station-00000910-hp-video.jpg" data-src-xsmall="http://i2.cdn.turner.com/cnnnext/dam/assets/140921092316-vo-spacex-lauch-to-international-space-station-00000910-story-body.jpg" data-src-small="http://i2.cdn.turner.com/cnnnext/dam/assets/140921092316-vo-spacex-lauch-to-international-space-station-00000910-story-top.jpg" data-src-medium="http://i2.cdn.turner.com/cnnnext/dam/assets/140921092316-vo-spacex-lauch-to-international-space-station-00000910-horizontal-large-gallery.jpg" data-src-large="http://i2.cdn.turner.com/cnnnext/dam/assets/140921092316-vo-spacex-lauch-to-international-space-station-00000910-horizontal-large-