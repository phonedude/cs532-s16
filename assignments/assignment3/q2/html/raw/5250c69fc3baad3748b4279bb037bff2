                       "type": event.target.id,
                       "service": "Twitter",
                       "generation": s_gen,
                       "referrer": s_type,
                       "video": videoId }
                  );
            } );});

             function yt_callback(category,action,label,value){
                      console.log("category=" + category + " action=" + action + " label=" + label + " value=" + value)

                      mixpanel.track(
                        "Player",
                        {  "event":action,
                           "value": label,
                           "service": "Youtube",
                           "generation": s_gen,
                           "referrer": s_type,
                           "video": videoId }
                      );
              }


        </script>

        
