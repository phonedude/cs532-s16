                    google_adnum = google_adnum + google_ads.length;
                }

                function createAdsContainer(){
                    var AdsContainer=document.getElementById("google-ads-container3");
                    if(AdsContainer){
                        AdsContainer.innerHTML=s;
                    }
                }
                createAdsContainer();
                return;
            }
