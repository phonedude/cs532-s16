	window.location.href = host.replace('//www', '//m') + path.replace(/^\/noticias/, '/mobile');
} else {
	console.log("no se cumplen las condiciones");
}

function redireccionarDesktop() {
	window.sessionStorage.setItem('prefer', 'desktop');

	if (path.match("/landing-movil") || path.match("/opinion") || path.match("/uno-noticias") || path.match("/noticias-en-claro")) {
		window.location.href = "http://www.unotv.com/noticias/home/?prefer=desktop";
	} else {
