	<div id="tabs-s1c" class="dataPack tabChild">
		<div class="dataContents">
		<script type="text/javascript" src="/mod/js/jl_score_lib.js"></script>
<script>
	$(document).ready(function(){
		var targetLeague   = "J30";
		var targetFileName = "BR"+targetLeague+".xml";
		var targetXML      = "/soccer/jleague/j3/xml/"+targetFileName;
		//console.log("targetXML："+targetXML);

		$.ajax({
			url      : targetXML,
			dataType : "xml",
			success  : function(data){
				var TransmissionId = $("TransmissionId",data).text();
				var HeadLine = $("HeadLine",data).text();
				//試合で回す
				$("Body",data).each(function(){
					var GameID = $(this).attr("BlockId");
					GameNam = parseInt(GameID.replace(/\D{3}(\d{3})?/, "$1"));
					//console.log("+++++　"+GameNam+"試合目　+++++");
					//試合情報
					var Meta = $("Meta",this);
					var CompetitionDay = $("CompetitionDay",Meta).text();
					//console.log("節表記："+CompetitionDay);
					var Stadium = $("Site",Meta);
					var StadiumName = $("Writing",Stadium).text();
					//console.log("スタジアム："+StadiumName);
					var TermDetail = $("TermDetail",Meta);
					var StartDate = $("TermStartDate",TermDetail).text();
					var StartDate_YYYY = StartDate.substring(0, 4);
					var StartDate_MM   = StartDate.substring(5, 7);
					var StartDate_DD = StartDate.substring(8, 10);
					if (StartDate_DD.match(/[０-９]{2}/)){ StartDate_DD = toHankaku(StartDate_DD); }
					var targetGameYMD  = toHankaku(StartDate_YYYY+StartDate_MM+StartDate_DD);;
					//console.log("試合日："+StartDate_DD+"日");
					var StartTime = toHankaku($("Writing",TermDetail).text());
					//console.log("試合時間："+StartTime);
					var StartTime2 = toHankaku($("Hour",TermDetail).text())+":"+toHankaku($("Minute",TermDetail).text());
					//console.log("試合時間2："+StartTime2);
					var StartDateStr = StartDate_DD+"日"+StartTime;
					//console.log("試合："+StartDateStr);
					var State = $("State",Meta).text();
					if(State == "試合開始前"){State = "開始前"}
					//console.log("試合経過："+State);

					//チーム情報
					var Match = $("Match",this);
					var HomeTeamArry , HomeTeamName , HomeTeamNameTag , HomeId , HomeTeamScore , HomeTeamWL;
					var AwayTeamArry , AwayTeamName , AwayTeamNameTag , AwayId , AwayTeamScore , AwayTeamWL;
					$("Team",Match).each(function(){
						var Alignment = $(this).attr("Alignment");
						//console.log("Home&Away："+Alignment);
						var TeamId = $(this).attr("TeamId");
						//console.log("Home&AwayID："+TeamId);
						var TmpTeamName = $("Formal[Display='フル表記']",this).text();
						//console.log("TmpTeamName："+TmpTeamName);
						if(TeamId == 1){
							HomeTeamArry = jleagueTeam_arry[TmpTeamName];
							if(HomeTeamArry){
								var TeamNameEng = HomeTeamArry.NSTeamEng;
								HomeTeamName    = HomeTeamArry.NSTeamName;
								HomeId          = HomeTeamArry.KyodoTeamID;
								HomeTeamNameTag = "<a href=\"/soccer/jleague/team/"+TeamNameEng+"/top-"+TeamNameEng+".html\">"+HomeTeamName+"</a>";
							}else{
								HomeTeamNameTag = $("Writing",this).text();
							}
							//console.log("HomeTeamName："+HomeTeamName);
						}
						else if(TeamId == 2){
							AwayTeamArry = jleagueTeam_arry[TmpTeamName];
							if(AwayTeamArry){
								var TeamNameEng = AwayTeamArry.NSTeamEng;
								AwayTeamName    = AwayTeamArry.NSTeamName;
								AwayId          = AwayTeamArry.KyodoTeamID;
								AwayTeamNameTag = "<a href=\"/soccer/jleague/team/"+TeamNameEng+"/top-"+TeamNameEng+".html\">"+AwayTeamName+"</a>";
							}else{
								AwayTeamNameTag = $("Writing",this).text();
							}
							//console.log("AwayTeamName："+AwayTeamName);
						}
					});
					$("MatchResult",Match).each(function(){
						if( $(this).attr("ParticipantId") == 1){
							HomeTeamScore = toHankaku( $("Score Writing",this).text() );
							HomeTeamWL    = $("Outcome Formal",this).text();
						}
						else if( $(this).attr("ParticipantId") == 2){
							AwayTeamScore = toHankaku( $("Score Writing",this).text() );
							AwayTeamWL    = $("Outcome Formal",this).text();
						}
					});
					var scoreStr = HomeTeamScore + "－" + AwayTeamScore;

					//?date=20140316&l=J30&id=FMNP
					//詳細リンク
					var TargetID = HomeId+AwayId;
					var linkParms = "?date="+targetGameYMD+"&l="+targetLeague+"&id="+TargetID;
					var ScoreTag = "<a href=\"/soccer/jleague/j3/score/j3-score_detail.html"+linkParms+"\">"+scoreStr+"</a>";





					//出力用タグ
					var tagHTML = "<tr class=\""+GameID+"\">\n<td class=\"line home\">"+HomeTeamNameTag+"</td>\n<td class=\"line score\">"+ScoreTag+"</td>\n<td class=\"line away\">"+AwayTeamNameTag+"</td>\n</tr>\n";
					$(".side_scoreborad_j3 tr.nowloadingWrap").hide();
					$(".side_scoreborad_j3 tbody").append(tagHTML);

				});
			},
			error : function(){
				//console.log("試合予定データがありません");
				$(".side_scoreborad_j3 .nowloadingWrap").hide();
				$(".side_scoreborad_j3 .noDataWrap").show();
			}
		});
	});

</script>



<style>
#soccer .dataContents .side_scoreborad_j3 td.home,
#soccer .dataContents .side_scoreborad_j3 td.away{ width: 35%; }
</style>
