<script type="text/javascript" >

	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_508124").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/508124?view=json", function(json){
				jQuery("#upv_508124").empty();
				if (json.stats && json.stats.uniqueVisitors >= 0) {
					jQuery("#upv_508124").append(json.stats.uniqueVisitors);				
				} else {
					jQuery("#upv_508124").append("Not available");
				}
			});
		}
	}
</script>
			</div>
			
										
