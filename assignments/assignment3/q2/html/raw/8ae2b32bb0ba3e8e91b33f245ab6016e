			src="/cs/RedBull2/javascripts/vendor/webtrekk/webtrekk_v3_all.20160108111724.js"></script>
		<script type="text/javascript" id="webtrekk_config">        
       
        (function(namespace, win) {
			var ns = namespace, prevId;

			var getScrollIndex = function(startAt0) {
				var storyContainer = document.querySelector('.story-container');
				if (null != storyContainer) {
					if (storyContainer.dataset && 'object' === typeof(storyContainer.dataset)) {
						var isScrollToNext = storyContainer.dataset.autoNextStory;
						var scrollIndex = parseInt(storyContainer.dataset.scrollIndex) + 1;
						if (scrollIndex) {
							
							if (0 < scrollIndex && 4 > scrollIndex) {
								return (scrollIndex).toString();
							}
						} else if(isScrollToNext) {
							return (1).toString();
						}
					}
				}
				return startAt0 ? "0" : "";
			}

			
			if (ns.pageConfig) {
			    prevId = ns.pageConfig.contentId;
			}
			
			(function setPage() {
			    ns.pageConfig = {
			        linkTrack : '',
			        heatmap : '',      
			        form : '',
			        contentId : (function getContentIdByURL(w, d) {
						var url = d.location.href;
						if(url && url !== null) {
						    return url.split("?")[0].toLowerCase();
						}
						return "no_content";
					})(this, document)
				};
			})();

		
			if (prevId === ns.pageConfig.contentId) return;
			
			
			(function setParams() {
			    ns.contentGroup = {
			        1 : 'redbull.com',      
			        2 : 'redbull.com - tr',  
			        3 : 'redbull.com - tr - Red Bull'       
			    };
			    
			    ns.customParameter = {
			        1 : 'tr',         
			        2 : 'www.redbull.com',       
			        3 : '1331680048924',
			        5 : 'Story',         
			        6 : 'Default',        
			        8 : 'redbull.com',
			        9 : '',
			        10 : '20140922',
			        11 : 'eSports,Games,StarCraft 2',
			        12 : 'Red Bull',
			        13 : 'Story',
			        14 : '1331680040639' ,
			        15 : '' ,
			        16 : '',
			        17 : '',
			        18 : '',
			        19 : '',
			        21 : '22-09-2014', 
			        24 : '',
				    25 : '',
			        26 : 'translated',
				    27 : 'en_INT'
			    };
			})();
			
			
	
			(function track() {
			    
			    var scrolledTo = getScrollIndex(true);
			    var localWebTrekkV3 = new webtrekkV3(ns.pageConfig);
			    if ("0" != scrolledTo) {
			    	win.wt = localWebTrekkV3;
			    } else {
			    	win.wt = win.wt || localWebTrekkV3;
			    }
			    win.wt.contentGroup = ns.contentGroup;
			    win.wt.customParameter = ns.customParameter;
				
				
			})();		
			
		        })(rb.tracking.webtrekk, window);
		
