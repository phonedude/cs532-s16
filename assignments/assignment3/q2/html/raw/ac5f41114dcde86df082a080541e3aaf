    $(function() {

        function getCookie(cookie_name) {
            var results = document.cookie.match('(^|;) ?' + cookie_name + '=([^;]*)(;|$)');
            if ( results ) {
                return(unescape(results[2]));
            } else {
                return '';
            }
        }
        if (getCookie('rs_name') != '') {
            //document.getElementById('minc_login').innerHTML = 'Bem Vindo, ' + getCookie('rs_name').replace(/\+/g, ' ') + ' - <a href="/minc/logout/">Logout</a>';
            document.getElementById('minc_login').innerHTML = 'Logado via Ministério da Cultura - <a href="/minc/logout/" class="logout">Sair</a>';
        }

        jQuery('#mycarousel').jcarousel({
            start: 3
        });

      function pageWidth() {
        return window.innerWidth || document.body.clientWidth;
      }
      function reconfig() {
        if (pageWidth() <= 1024) {
            $('#ad_tosco_left, #ad_tosco_right').css('display', 'none');
        } else {
            $('#ad_tosco_left, #ad_tosco_right').css('display', 'block');
        }
      }
      $(window).resize(reconfig);
      reconfig();
  });
</script>
    

