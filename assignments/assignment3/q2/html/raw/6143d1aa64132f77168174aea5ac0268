<script type="text/javascript">
    // thread id
    var fos_comment_thread_id = 'article-93723';

    // api base url to use for initial requests
    var fos_comment_thread_api_base_url = "/comment/threads";

    /* requested comment page */
    var page = 1;

    var commentsModerated = false;

    var texts = {
        form_characters: "Max caracteres: total_chr (pendientes: remaining_chr)"
    };

    var comment_tracking_url = "http://track.elpais.com.uy/" + "track/comments/" + fos_comment_thread_id;

    // get comments link
    var comment_thread_url = "/comment/threads/article-93723/comments?permalink=";

    // Snippet for asynchronously loading the comments
    jQuery(function() {
        var fos_comment_script = document.createElement('script');
        fos_comment_script.async = true;
        fos_comment_script.src = '/js/66303ba.js';
        fos_comment_script.type = 'text/javascript';

        // if not logged in, load reply form and hide login links
        wfCmsOnLogin(function(ev, user) {
            // remove login form
            $('.comments-part .comment-login').remove();

            if (true == user.allow_comments) {
                $('#add-comment').show();
                $('.comment-votes, .fos_comment_comment_reply').show();
                $('.information-text').toggle();

                var image_path = 'http://registro.elpais.com.uy/avatar.asp?id=' + user.id;
                $("#add-comment .comment-author-photo img").attr("src", image_path);
            } else {
                // show div with validate account
                var $validation = $('.account-validate');
                $validation.find(".user-message").html(user.user_message);
                $validation.show();
            }
        });

        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(fos_comment_script);

        // set privacy policy checked if cookie exists
        if (readCookie('_cms-comment-terms')) {
            $('#article-comments .terms input').prop('checked', true);
        }

        setTabIndexes();
        updateRemainingChars();
    });

$('.comments-part')
    .on('submit', '#add-comment form', function(event){
        event.preventDefault();

        var that = $(this);
        var fos = window.fos.Comment;
        var serializedData = fos.serializeObject(this);

        var event = $.Event('fos_comment_submitting_form');
        that.trigger(event);

        if (event.isDefaultPrevented()) {
            return;
        }

        var overlay = $('<div>')
            .addClass('comment-overlay')
            .after($('<div>').addClass('spinner'))
            .insertAfter(that);

        fos.post(
            this.action,
            serializedData,
            // success
            function(data, statusCode) {
                Recaptcha.reload();
                fos.appendComment(data, that);
                that.trigger('fos_comment_new_comment', data);
            },
            // error
            function(data, statusCode) {
                //remove scripts from html string
                var $errorsContainer = that.find('.fos_comment_form_errors');
                if (!$errorsContainer.size()) {
                    that.prepend('<div class="fos_comment_form_errors"></div>');
                    $errorsContainer = that.find('.fos_comment_form_errors');
                }
                $errorsContainer
                    .html($(data).find('.fos_comment_form_errors').html());
                Recaptcha.reload();
            },
            // complete
            function(data, statusCode) {
                that.parent()
                    .find('.comment-overlay').remove().end()
                    .find('.spinner').remove()
                    ;
                that.trigger('fos_comment_submitted_form', statusCode);
            }
        );

        return false;

    })
    /* when replying to a comment, add partent data to reply form */
    .on('click', '.reply', function(){
        var $form = $('#add-comment form'),
            url = $form.prop('action').replace(/\?.*$/, ''),             $this = $(this),
            parentId = $this.data('parentId')
            ;
        url = url + '?parentId=' + parentId;

        $form
            .prop('action', url)
            .data('parentId', parentId);
    })
    .on('change', '.terms', function(){
        if ($(this).find('input').is(':checked')) {
            expDate = new Date();
            expDate.setMonth(expDate.getMonth() + 1);

            document.cookie = '_cms-comment-terms=true' +
                ';domain=' + document.location.hostname +
                ';path=/' +
                ';expire=' + expDate.toGMTString();
        }
    })
    /* overwrite vote submission */
    .on('click', '.votes-no', function(e){
        e.preventDefault();

        var that = $(this);
        var commentId = 5;
        window.fos.Comment.post(
            that.data('url'),
            {},
            function(data) {
                that.parents('.comment-votes').find('.fos_comment_comment_score').html(data);
                that.trigger('fos_comment_vote_comment', data);
            },
            function(data) {
                if (data == 'already_voted') {
                    that
                        .parents('.comment-box')
                        .find('.comment-text')
                        .after(
                            $('<div>').addClass('comment-denounced')
                                .text('Solo se puede votar una vez cada comentario')
                        );

                    setTimeout(function() {
                        $('.comment-denounced').remove();
                    }, 5000);
                }
            }
        );
    })
    .on('fos_comment_new_comment', function(event, data) {
        //reset the form
        var $form = $(event.target);
        $form.find('textarea').val('');
        $form.attr('action', $form.attr('action').replace(/\?=.*$/, ''));         $form.data('parentId', null);
        updateRemainingChars();
        if (readCookie('_cms-comment-terms')) {
            $form.find('.terms input').prop('checked', true);
        }
    })
    /* increase vote number after voting */
    .on('fos_comment_vote_comment', function (event, data) {
        var $target = $(event.target);
        var value = parseInt($target.text());
        $target.text(++value);
    })
    .on('fos_comment_load_thread', function (event, data) {
        if (window.wfCmsLoggedInUser && window.wfCmsLoggedInUser.allow_comments) {
            $('.comment-votes, .fos_comment_comment_reply').show();
        }

        // update social media bar comment number
        var comments_no_text = $('.comments-part-number span').text();
        comments_no_text = $.trim(comments_no_text);

        var comments_no = parseInt(comments_no_text.match( /\d+/)[0]),
            $commentNoHolder = $('.social-media-button-article.comments .number');
        $commentNoHolder.text(comments_no);
        $commentNoHolder.parent().addClass("c-no-" + comments_no);
    })
    /* when new comment is added */
    .on('fos_comment_add_comment', function (event, data) {
        var $data = $(data);

        // comments are moderated, comment is not inserted in page
        if (commentsModerated) {
            $('#add-comment.fos_comment_comment_form_holder').first().before(
                $('<div>')
                    .addClass('flash-info comment-box')
                    .text("Su comentario está pendiente de moderación. En breve será revisado. Gracias por comentar en El País Digital.")
            );
            setTimeout(function() {
                $('.flash-info').remove();
            }, 15000);

        } else {

            /* comment is inserted in page, increment count */
            var $count_display = $('.comments-part-number span');
            var comments_no_text = $.trim($count_display.text());
            var comments_no = parseInt(comments_no_text.match( /\d+/)[0]);

            var comments_no_new = comments_no + 1;
            comments_no_text = comments_no_text.replace(comments_no.toString(), comments_no_new.toString());
            $count_display.text(comments_no_text);

            $data.find('.comment-info ul>li:first-child').text('#' + comments_no_new);

            /* update comment no */
            var id = $data.find('.comment-box').attr('id');

            $('#'+id).find('.comment-info ul')
            .prepend(
                $('<li>').text('#'+comments_no_new)
            );
        }

        /* if it has no parent, insert to top of comments */
        var $form = $(event.target),
            form_data = $form.data();
        $('#add-comment .fos_comment_comment_show').remove();
        if (!form_data.parentId || form_data.parentId == '' || !$.trim($data)) {
            var $commentsPartNumber = $('#fos_comment_thread .comments-part-number').after($data),
                $newComment = $commentsPartNumber.next();
        } else {
            var $parent = $('#fos_comment_' + form_data.parentId).closest('.fos_comment_comment_show'),
                $replies = $parent.find('.fos_comment_comment_replies'),
                $newComment;

            if (!$replies.size()) {
                $parent.append('<div class="fos_comment_comment_replies"></div>');
                $replies = $parent.find('.fos_comment_comment_replies');
            }

            $replies.prepend($data);
            $newComment = $replies.find('> *').first();
            $newComment.find('.fos_comment_comment_reply').remove();
        }

        var offset = $newComment.offset(),
            top = offset && offset.top;
        if (top) {
            $('body,html').animate({scrollTop: top}, 'slow');
        }

        if (window.wfCmsLoggedInUser && window.wfCmsLoggedInUser.allow_comments) {
            $('.comment-votes, .fos_comment_comment_reply').show();
        }

        var tImg = new Image(1, 1);
        tImg.src = comment_tracking_url;
    })
    /* denounce link */
    .on('click', '.denounce', function (event){
        var $this = $(this);
        var url = $this.data('url');

        var id = $this.parents('.comment-box').attr('id').replace('fos_comment_', '');
        var ids = readCookie('_cms-comments-spam');
        if (ids) {
            ids = ids + ',' + id;
        } else {
            ids = id;
        }

        $.post(url, function(data){
            // set cookie
            expDate = new Date();
            expDate.setYear(expDate.getYear() + 1);

            document.cookie = '_cms-comments-spam=' + ids +
                ';domain=' + document.location.hostname +
                ';path=/' +
                ';expire=' + expDate.toGMTString();

            // anounce denouncement
            if (data.state == 'already_denounced') {
                $this
                    .parents('.comment-box')
                    .find('.comment-text')
                    .after(
                        $('<div>').addClass('comment-denounced')
                            .text('Solo se puede denunciar un mismo comentario una sola vez.')
                    );
            } else {
                $this
                    .parents('.comment-box')
                    .find('.comment-text')
                    .after($('<div>').addClass('comment-denounced')
                        .text('Gracias por denunciar este comentario.'));

                // if comment state is now spam
                if (data.state == 2) {
                    $this
                        .closest('.comment-details')
                        .find('.comment-votes')
                            .remove()
                            .end()
                        .find('.comment-text')
                        .addClass('comment-spam')
                        .text('Este comentario se ha marcado como spam.');
                }
            }

            setTimeout(function() {
                $('.comment-denounced').remove();
            }, 5000);
        },
        'json');
    })
    /* pass parameters to controller action */
    .on('fos_comment_before_load_thread',  function (event){
        event.params.page = parseInt(page);
        return event;
    })
    /* paginate */
    .on('click', '.pagination a', function (event){
        event.preventDefault();

        page = $(this).prop('href')
            .split('/').pop();

        if (isNaN(page)) {
            page = 1;
        }

        var fos_comment_script = document.createElement('script');
        fos_comment_script.async = true;
        fos_comment_script.src = '/js/66303ba.js';
        fos_comment_script.type = 'text/javascript';

        var target = document.getElementById('fos_comment_thread');

        $('#fos_comment_thread')
            .prepend($('<div>')
                .addClass('comment-overlay')
                .after($('<div>').addClass('spinner'))
            );

        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0])
            .appendChild(fos_comment_script);
    })
    /* remaining charcters count*/
    .on('keyup', 'textarea', function(event){
        updateRemainingChars();
    });

    function setTabIndexes($form) {
        $form = $form || $('#add-comment form');
        $form.find('textarea')
                .attr('tabindex', 1)
            .end()
            .find('#fos_comment_comment_terms')
                .attr('tabindex', 4)             .end()
            .find('.fos_comment_submit button')
                .attr('tabindex', 5)
            .end();
    }
    function updateRemainingChars() {
        var
            $target = $('#add-comment form textarea'),
            total_len = $target.attr('maxlength'),
            remaining = total_len - ($target.val().length)
            ;
        $('#form-characters').text(texts.form_characters.replace('total_chr', total_len).replace('remaining_chr', remaining));
    }

    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
    }
</script>
                </div>
