<script type="text/javascript" src="https://www.google.com/recaptcha/api/js/recaptcha_ajax.js"></script><script type="text/javascript">
        function genemu_recaptcha_load(event, div)
        {
            if (!div) {
                var div = 'fos_comment_comment_captcha_div';
            }

            Recaptcha.create('6LcP7t4SAAAAANDWRasy52WUodTEXQzbT2MAsMNY', div, {"lang":"es","theme":"clean","tabindex":2,"custom_translations":{"instructions_visual":"Inserte el captcha","incorrect_try_again":"Error al introducir el captcha"}});
        }

        jQuery(function($) {
            genemu_recaptcha_load();
        });
    </script>
            <div class="account-validate">
                <span>Validar cuenta</span>
