        <div class="security">
            <div id="fos_comment_comment_captcha_div" style="display: none;"></div><noscript><iframe src="http://api.recaptcha.net/noscript?k=6LcP7t4SAAAAANDWRasy52WUodTEXQzbT2MAsMNY" width="500" height="300" frameborder="0"></iframe><br /><textarea name="recaptcha_challenge_field" rows="3" cols="40"></textarea><input type="hidden" name="recaptcha_response_field" value="manual_challenge" /></noscript>
