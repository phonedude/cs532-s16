<script type='text/javascript'>
	var flag=1; var loaded = 0;
	$(document).ready(function(){
		$("#anotherBox").css({'display':'block'});
		$("#recBox").css({'display':'block'});
		$("#recBox").css({"left":$(window).width()});
		$("#anotherBox").css({"left":$(window).width()});
		$("#recBox").click(function(){
			$("#recBox").animate({"left":$(window).width()+12},200,function(){
				$("#anotherBox").animate({"left":$(window).width()-$("#anotherBox").width()+10},300);
			});
			flag=1;
		});
	});
	var exClose = function(){
		$("#anotherBox").animate({"left":$(window).width()},300,function(){
			$("#recBox").animate({"left":$(document.body).innerWidth()-45},200);
		});
		flag=0;
	}
	$(window).resize(function(){
		$("#recBox").css({"left":$(window).width()});
		$("#anotherBox").css({"left":$(window).width()});
	});
	$(window).scroll(function(){
		if(($(window).height()+$(window).scrollTop())>$("#comment").position().top){
			if(loaded==0) {
				$("#anotherBox iframe").attr("src", "/home/news/recommend.php");
				loaded = 1;
			}
			if(flag==1){
				$("#recBox").css({"left":$(window).width()});
				$("#anotherBox").css({"left":$(window).width()-$("#anotherBox").width()+10});
			}else{
				$("#recBox").css({"left":$(window).width()-45});
				$("#anotherBox").css({"left":$(window).width()});
			}
		}else{
			$("#recBox").css({"left":$(window).width()});
			$("#anotherBox").css({"left":$(window).width()});
		}
	});
	</script>
	<div id="anotherBox" style="width: 370px;height: 103px;overflow: hidden;position:fixed;bottom:0px;background: url(http://assets.latinpost.com/static/common/_v0.0.0/img/recommend/shadow.png);padding: 5px;border-radius: 5px;z-index: 100000;display:none;"><iframe src="" frameborder="0" width="370" height="108" marginheight="0" marginwidth="0"></iframe></div><img src="http://assets.latinpost.com/static/common/_v0.0.0/img/recommend/close.png" id="recBox" width="46" height="198" style="display: none;cursor: pointer;position:fixed;bottom:0px;z-index: 100000;" alt="recommend close" />   <!-- uni-aux start -->
