<script>
$('input[type="button"]').click(function(){
	if($(this).val() == 'Submit'){$.ajax({url:'/home/news/newsletter/mailchimp_insert.php',type:'post',data:{email:$('#nmail').val()}}).done(function(e){  $('.rword').html(e);  $("#rword a").css("display","block"); });}
});

if(!$.cookie('social_popup')){
	$('.wh_transf').show();
	$.cookie('social_popup', '1', { path: '/', domain: 'latinpost.com', expires: 3 });
	$('html').on("click",function() { $('.wh_transf').hide(); });
	$('.subscribe_popup').click(function(event){ event.stopPropagation(); });
}

</script>


