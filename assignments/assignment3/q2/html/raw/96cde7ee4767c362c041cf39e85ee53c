<script type="text/javascript">
    RadioCanada.Lib.jQuery(document).ready(function () {
        var cookie = RadioCanada.Lib.jQuery.cookie("RCID");
        var editionregionid = 9;

        if (cookie != undefined) {
            var json = RadioCanada.Lib.jQuery.parseJSON(cookie);

            if (json.editionregionid != undefined) {
                editionregionid = json.editionregionid;
            }
        }

        RadioCanada.Lib.jQuery.ajax('/widgets/OnAir/GetOnAirForSpecificRegion/' + editionregionid).done(function (data) {
            if (data != undefined) {
                RadioCanada.Lib.jQuery("#on-air-emissions li:last-child a img").attr("src", data.ImageUrl);
                RadioCanada.Lib.jQuery("#on-air-emissions li:last-child a img").attr("alt", data.Title);
                RadioCanada.Lib.jQuery("#on-air-emissions li:last-child a img").attr("title", data.Title);
                RadioCanada.Lib.jQuery("#on-air-emissions li:last-child a").attr("href", data.LinkHref);
            }
        });
    });
</script>


