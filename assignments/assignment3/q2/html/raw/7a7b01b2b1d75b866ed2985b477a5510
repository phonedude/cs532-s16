    		var spe_render_likes = function(context) {

    		    var imgCache = new Image();
    		    imgCache.src = 'http://www.imore.com/sites/all/modules/spe_share/likes/loading-spokes.svg'; // Preload the spinner
    		      
    		    (function($) {
    		    		
					var mbn_share_loaded = new Array();
					$('.mbn_share',context).mouseover( 
				    	function () { 
				    		var mbn_share_id = $(this).attr('id');
								var mbn_share_delay = $(this).attr('data-share-delay'); 
				    		var href_load = '/sites/all/modules/spe_share/likes/likes.php?b=' + $(this).attr('data-share');
				    		if (jQuery.inArray(mbn_share_id, mbn_share_loaded) == -1) {
                    var share = $('#' + mbn_share_id);
				    		    var loaderImg = $('<img class=\'mbn_share_loader\' src=\'http://www.imore.com/sites/all/modules/spe_share/likes/loading-spokes.svg\' alt=\'loading...\' />');

				    			  share.after(loaderImg);
				    			  share.fadeOut();
                    share.load(href_load, function() {
                        share.fadeIn(function() {
                            loaderImg.detach();
                        });
                    });
				    		}
				    		mbn_share_loaded.push(mbn_share_id);	
				   		}
				   	); 

				   	var postObj=new Object();
				   	var postObj_count = 0;
				   	$("[id*='mbn_share_count_']",context).each(
				   		function() {
				   			var mbn_share_count_id = $(this).attr('id');
				   			var share_data;
				   			if($(this).is('[data-share]')) share_data = $(this).data('share');
                else share_data = $(this).parent().data('share');
				   			postObj[mbn_share_count_id] = share_data;
				   			postObj_count++;			   		
	  					}
	  				);
	
	  				if(postObj_count>0) {
		  				$.post('/sites/all/modules/spe_share/likes/likes.php', postObj,
							function(data){
								for(var key in data) {
							    	var value = data[key];
							    	$('#' + key).text(value);
								}
						 	}, 'json');
						}
						
						})(jQuery);						
		}
		
		
				
