<script>
	
	var cnnOutbrain_Manager = {
		OB_permalink : "",
		addScript : function() {
			var OBscr = document.createElement('script');
			OBscr.setAttribute('language', 'javascript');
			OBscr.setAttribute('async', 'async');
			OBscr.setAttribute('type', 'text/javascript');
			OBscr.setAttribute('src','http:\/\/widgets.outbrain.com/outbrain.js');
			document.body.appendChild(OBscr);
		},
		get_OB_permalink  : function() {
			return this.OB_permalink;
		},
		set_OB_permalink : function() {
			var cnnLinkTags = document.getElementsByTagName("head")[0].getElementsByTagName("link"),
				isCanonicalPresent = false,		
				i;
	
			for (i = 0; i < cnnLinkTags.length; i++) {
				try {
					if (cnnLinkTags[i].getAttribute("rel") == "canonical") {
						isCanonicalPresent = true;
						this.OB_permalink = cnnLinkTags[i].getAttribute("href");
						break;
					}
				}catch(e){}
			}
	
			if (!isCanonicalPresent) { 
				this.OB_permalink = window.location.href;
			}
	
			/* permalink only works with live URLs */
			this.OB_permalink = this.OB_permalink.replace(/http:\/\/\w+\.money/g,'http://money');
		},
		load : function(initObj) {
				if(typeof initObj === 'undefine') {
					return;
				}
	
				if(typeof initObj.permalink !== 'undefined') {
					this.OB_permalink = initObj.permalink;
				}
				else {
					this.set_OB_permalink();
				}
	
				/* Feed */
				if(typeof initObj.feeds !== 'undefined' && true === initObj.feeds) {
				}
	
				/* Modules */
				if(typeof initObj.modules !== 'undefined' && initObj.modules.length > 0) {
					for (i = 0; i < initObj.modules.length; i ++) {
						cnnOutbrainModules.load(initObj.modules[i]);
					}
				}
	
				this.addScript();
		}
	};
	
	var cnnOutbrainModules = {
		OB_data_widget_id : '',
		OB_anchor : null, /* HTML element to add OB module*/
	
		addModule : function() {
			var OB_div_elem = '';
			if (this.OB_anchor) {
				this.OB_div_elem = '<div class="OUTBRAIN" data-widget-id="'+this.OB_data_widget_id+'" data-src="'
				+ cnnOutbrain_Manager.get_OB_permalink()+'" data-ob-template="cnnmoney"></div>';
				this.OB_anchor.innerHTML =this.OB_div_elem;
			}
		},
	
		load : function(initData) {
			initData = initData || {};
			var anchorid = initData.anchorid || '';
			this.OB_data_widget_id = initData.widgetid || this.OB_data_widget_id;
			this.OB_anchor = document.getElementById(anchorid);
	
			if(this.OB_anchor != null && this.OB_data_widget_id != '') {
				this.addModule();
			}
		}
	};

    /* Outbrain */
    try {
        cnnOutbrain_Manager.load({'modules': [{ widgetid : 'AR_8', anchorid : 'js-outbrain-module'},{ widgetid : 'AR_18', anchorid : 'js-outbrain-relateds'},{ widgetid : 'AR_17', anchorid : 'js-outbrain-recommended'}] }); 
    }catch(e){}

</script>
