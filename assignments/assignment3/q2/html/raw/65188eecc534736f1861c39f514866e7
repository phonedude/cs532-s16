    /** Adds an image ping beacon to the end of the HTML page. */
    CNN.tracking.cnn_wpBlogPing = function() {
        var loc = window.location.href;
        var img = '<img src="http://money.cnn.com/images/tracking/wpblog.gif?loc=' + encodeURIComponent(loc) + '" class="cnn-tracking-img" width="1" height="1">';
        $('body').append(img);
    };

    $(document).ready(function() {
        CNN.tracking.cnn_wpBlogPing();
    }); 
</script>

