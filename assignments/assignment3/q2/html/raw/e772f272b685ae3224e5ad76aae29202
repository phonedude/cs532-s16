    , filterAliases: []
    , isDoCtw: 1
    , pageType: "GoldBox"
    , requestId: "083CTXMJ0E2FHMJP0FWP"
    , keydownTriggeredWeblabs: []
    , displayTriggeredWeblabs: [      function(data) {
        return data && data.fastlabs ? data.fastlabs : undefined;
      }
