<script>
var hasFlash = (function() {
  try {
    if (typeof navigator.plugins != "undefined" && typeof
      navigator.plugins["Shockwave Flash"] == "object") {
      return true;
    } else if (typeof window.ActiveXObject != "undefined") {
      new ActiveXObject("ShockwaveFlash.ShockwaveFlash");
      return true;
    }
  } catch (e) { }
    return false;
})();
</script>


