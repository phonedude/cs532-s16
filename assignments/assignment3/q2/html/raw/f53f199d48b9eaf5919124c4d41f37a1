/* Insere RCT nos links do trending: Remover 31/12/2014 */
var trendingParam = "";
function addRTC(param) {
      // $("div[data-vr-zone='24 horas']").size()
      // $("div[data-vr-zone='Trending']").size()
      var node1 = $("div[data-vr-zone='Trending']").find(".content-list-item");
      var node2 = $("div[data-vr-zone='24 horas']").find(".content-list-item");

      if( node1.size() > 0 && node2.size() > 0 && document.location.href == "http://exame.abril.com.br/") {
      // var node2 = $("div").attr("data-tab-section","24-horas").find(".content-list-item");
      urlNode1 = node1.find("a.content-item-image").attr("href").split("?");
      // urlNode2 = node2.find("a.content-item-image").attr("href").split("?");

      if(urlNode1[1] == undefined) {
        node1.each(function() {
          linkImagem = $(this).find("a.content-item-image");
          linkArticle = $(this).find("p.content-item-title a");
          linkImagem.attr("href", linkImagem.attr("href") + param);
          linkArticle.attr("href", linkArticle.attr("href") + param);
        });
        node2.each(function() {
          linkImagem = $(this).find("a.content-item-image");
          linkArticle = $(this).find("p.content-item-title a");
          linkImagem.attr("href", linkImagem.attr("href") + param);
          linkArticle.attr("href", linkArticle.attr("href") + param);
        });
        clearInterval(trendingParam);
      }
    } else {
      // vazio
      clearInterval(trendingParam);
    }
  }

  function ativaPbcTrending() {
    var trendingParam = setInterval('addRTC("/?rct=rolex")', 2000);
  }

   //ativaPbcTrending();

   /* Correção temporária de acento no Blogs Mais Lidos da Home (05/12/2014)*/

   var link = $(".content-post-title a[href='http://exame.abril.com.br/blogs/primeiro-lugar/2014/12/05/paulo-guedes-criara-ibmec-da-saude/']");

   if (link && link.length > 0) {
    varTitle = $('<textarea />').html(link.text()).text();
    link.text(varTitle);
  }

