/*****************  Personalização do sidebar para cobertuda ao vivo ******************/

// IMPORTANTE: quando remover este bloco, remover também o style definido no fim do arquivo como
// [data-live-article-highlights]:first .content-item-info {
  if (window.location.pathname.indexOf('/ao-vivo/') > -1) {
    var list = [];

    var square = $('.sidebar-container > .sidebar-box-container > .square:first').parent();

    if (square.length > 0) {
      list.push(square[0]);
    }

  // var highlights = $('.sidebar-container [data-live-article-highlights]:first');
  // if (highlights.length > 0) {
  //   list.push(highlights[0]);
  // }

  $('.sidebar-container .sidebar-box-container').each(function() {
    if (list.indexOf(this) == -1) {
      console.log($(this).html());
      $(this).html('');
    }
  });

  var wrap = $(document);
  var box = $("[data-sidebar]");
  var boxTopPosition = box.offset().top;
  var articleFooter = $('.article-footer');

  var right = $('body').width() - (box.offset().left + box.width());

  wrap.on("scroll", function(e) {
   var scrollBottom = wrap.scrollTop();
   if (wrap.scrollTop() > boxTopPosition) {
     if (scrollBottom < articleFooter.offset().top){
       box.attr('style', 'position: fixed; top: 35px; right: ' + right + 'px;');
     } else {
       fixedTop = parseInt(articleFooter.offset().top) - parseInt(scrollBottom);
       box.attr('style', 'position: fixed; top: '+ fixedTop +'px; right: ' + right + 'px;');
     }
   } else {
     box.removeAttr('style');
   }
 });
}

