// Superbanner Fixa 15 segundos
if (window.location.pathname == '/') {
  var first_banner = $(".ad.page-container").eq(0);
  var count_banner = 1;
  $(window).scroll(function() {
    if($(this).scrollTop() > 113 && count_banner == 1 && $(".leaderboard-ad").eq(0).html() != "") {
      first_banner.addClass("banner-flut");
      setTimeout(function(){
        first_banner.removeClass("banner-flut");
        count_banner++;
      }, 15000);

    } else {
      first_banner.removeClass("banner-flut");
    }
  });
}


