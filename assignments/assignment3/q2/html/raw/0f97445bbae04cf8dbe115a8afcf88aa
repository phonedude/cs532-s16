    <div id="countdownDealsBox" class="a-box a-spacing-small countdownDealsComment"><div class="a-box-inner a-padding-small">
        <div id="countdownWidget" class="a-section a-spacing-micro a-padding-none">
            <div id="countdownTitle" class="a-section a-spacing-none a-padding-none">
                <span class="a-text-bold">Kindle Countdown Deal</span>
            </div>
            <span id="countdownMsg">Price goes up to <span class="a-color-price">$3.99</span> in</span>
            <span id="countdownTimer" class="a-color-success"></span>
        </div>

        <c:if test="true">
  <script>
    P.when('A', 'jQuery', 'ajaxClientComponent').register('rcpTimerComponent', function (A, jQuery, ajaxClient) {
        var rcpTimer = (function() {
          // Verification states define the status of the ajax price refresh
          // which is done to make sure the price has changed before a full
          // page refresh
          // NOT_STARTED: NO AJAX CALL HAS BEEN MADE
          // STARTED: AJAX CALL HAS BEEN MADE, BUT NO RESPONSE HAS BEEN RECEIVED YET
          // VERIFIED: AJAX CALL HAS BEEN MADE AND THE PRICE HAS BEEN VERIFIED
          // FAILED: AJAX CALL FAILED
          var VERIFICATION_STATES = {NOT_STARTED : 1, STARTED : 2, VERIFIED : 3, FAILED : 4};
          var timerContainerId = "countdownTimer";
          var messageContainerId = "countdownMsg";
          var timeout;
          var strings;
          var asin;
          var ourPriceAmount;
          var ourPriceCurrency;
          var remainingTime;
          var expirationTime;
          var verificationState = VERIFICATION_STATES.NOT_STARTED;
          var latencyMessageDisplayed = false;
          // P90 in seconds of the ATF latency
          var P90_ATF = 3;
          // P90 of the getPrice ajax call. This is just an estimate
          // since we do not have PROD metrics yet.
          // JIRA: https://kindle-jira.amazon.com/browse/KICSDEP-131
          var P90_GET_PRICE = 2;
          var REFRESH_MIN_TIME = P90_ATF + P90_GET_PRICE;

          /**
           * Display the latency message
           */
          function displayLatencyMessage() {
            latencyMessageDisplayed = true;
            jQuery('#' + timerContainerId).empty();
            jQuery('#' + messageContainerId).html(strings.countdownLatencyMsg);
          }

          /**
           * Update the timer with the new remaining time formatted in the
           * "n days 00h 00m 00s" format
           */
          function updateContainer() {
            // Get the number of days, hours and minutes
            var days = 0;
            var hours = 0;
            var minutes = 0;
            var seconds = 0;
            var remainingSecs = remainingTime;
            if(remainingSecs > 0) {
              var days = Math.floor(remainingSecs / (60*60*24));
              remainingSecs = remainingSecs % (60*60*24);
              var hours = Math.floor(remainingSecs / (60*60));
              remainingSecs = remainingSecs % (60*60);
              var minutes = Math.floor(remainingSecs/60);
              seconds = remainingSecs % (60);
            }
            var daysString = days + ' ' + ((days == 1) ? strings.day : strings.days) + ' ';
            var hourString = strings.hour;
            var minuteString = strings.minute;
            var secondString = strings.second;

            var finalTimer = daysString + padLeft(hours) + hourString + ' ' + padLeft(minutes) + minuteString + ' ' + padLeft(seconds) + secondString;
            jQuery('#' + timerContainerId).text(finalTimer);
          }

          function refresh() {
            window.location.reload(true);
          }

          function padLeft(i) {
            return (i<10) ? ('0' + i) : i;
          }

          function countdown() {
            remainingTime = expirationTime - Math.round(new Date().getTime()/1000);
            updateContainer();
            // Verify before running down to 0, that the price has expired or will
            // expire at the same time as what this timer thinks it will expire at.
            // The verification is done 3 seconds before the timer reaches 0, since
            // it takes around between 2.75 (avg) and 3.5(p90) seconds for the ATF data
            // to be loaded after click. The network latency for the ajax call was
            // estimated using the network latency for this ATF data.
            // ATF p90: http://tiny/lb5fhj8l/iGraph
            // ATF avg: http://tiny/danzrpo7/iGraph
            if(remainingTime <= P90_ATF && verificationState === VERIFICATION_STATES.NOT_STARTED) {
              // Verify that the price has expired.
              verifyPriceExpiration();
            }
            if(remainingTime > 0) {
              // Count down again after 1 second.
              timeout = window.setTimeout(countdown, 1000);
            } else {
              handlePriceExpiration();
            }
          }

          /**
           * Function that takes the right action when the counter has gone
           * down to 0. The action has to be taken depending on the state of
           * the price expiration verification.
           */
          function handlePriceExpiration() {
              switch(verificationState) {
                case VERIFICATION_STATES.VERIFIED:
                case VERIFICATION_STATES.FAILED:
                  //Refresh the page
                  refresh();
                  break;
                case VERIFICATION_STATES.NOT_STARTED:
                  // Verify the price has expired and display latency message
                  verifyPriceExpiration();
                  displayLatencyMessage();
                  break;
                case VERIFICATION_STATES.STARTED:
                  // Just display latency message and wait
                  displayLatencyMessage();
                  break;
              }
          }

          function getPriceAjaxRequest() {
            var request = {};
            request.url = "/gp/digital/fiona/ajax/price.html";
            request.query = {asin : asin};
            request.recordMetrics = true;
            request.csmScope = 'price_verification';
            return request;
          }

          /**
           * Make an ajax call to refresh the current price.
           */
          function refreshPrice() {
            var request = getPriceAjaxRequest();
            request.failureCallback = handlePriceRefreshFailure;
            request.successCallback = handlePriceRefresh;
            ajaxClient.fetch(request);
          }

          /**
           * Handle the new price json fetched from the server.
           * If the price has changed, then just refresh the page.
           * If the price has not changed, then
           *  a. If the latency message is displayed, then just refresh page
           *     after the remaining time which is received in the ajax response
           *  b. If the timer is still counting down, then edit the timer with the
           *     new remaining time that is fetched from the ajax response.
           */
          function handlePriceRefresh(priceInfo) {
            if(ourPriceAmount === priceInfo.ourPriceAmount && ourPriceCurrency === priceInfo.ourPriceCurrencyCode) {
              var remainingSecs = parseInt(priceInfo.remainingTime, 10);
              if(latencyMessageDisplayed) {
                window.setTimeout(refresh, remainingSecs*1000);
              } else {
                expirationTime = remainingSecs + Math.round(new Date().getTime()/1000);
                window.clearTimeout(timeout);
                countdown();
              }
            } else {
              // The price has changed. Just refresh the page.
              refresh();
            }
          }

          /**
           * Handles the case where the ajax call failed.
           */
          function handlePriceRefreshFailure() {
              refresh();
          }

          /**
           * Make an ajax call to verify the current price.
           */
          function verifyPriceExpiration() {
            var request = getPriceAjaxRequest();
            request.failureCallback = handlePriceVerificationFailure;
            request.successCallback = handlePriceVerification;
            ajaxClient.fetch(request);
            verificationState = VERIFICATION_STATES.STARTED;
          }

          /**
           * Handle the new price json fetched from the server.
           * If the price has changed, then just refresh the page.
           * If the price has not changed, then
           *  a. If the latency message is displayed, then just refresh page
           *     after the remaining time which is received in the ajax response
           *  b. If the timer is still counting down, then edit the timer with the
           *     new remaining time that is fetched from the ajax response.
           */
          function handlePriceVerification(priceInfo) {
            verificationState = VERIFICATION_STATES.VERIFIED;
            handlePriceRefresh(priceInfo);
          }

          /**
           * Handles the case where the ajax call failed.
           * a. If the latency message has been displayed, then just
           *    refresh the page.
           * b. Else set the state to FAILED and let the timer go
           *    down to 0 before it refreshes the page.
           */
          function handlePriceVerificationFailure() {
            if(latencyMessageDisplayed) {
              // Since we are in the latency phase, the price has
              // most probably expired according to the initial page
              // load. So refresh the page.
              refresh();
            } else {
              // Since the latency message is not yet displayed, the price
              // has not expired according to the initial page load.
              // So just set this state and let the timer run down to 0.
              // Once it runs down to 0, it will refresh the page.
              verificationState = VERIFICATION_STATES.FAILED;
            }
          }

          return {
            start: function(input) {
              if(!input.remainingTime || !input.ourPriceAmount || !input.ourPriceCurrency || !input.strings || !input.asin) {
                throw new Error('Required input parameters not set.');
              }
              var remainingSecs = parseInt(input.remainingTime, 10);
              // Calculate the expiration time of the offer with respect to
              // the system time of the client. If we use the expiration time,
              // then any inconsistencies or delays caused by the javascript
              // timer intervals will be mitigated.
              expirationTime = Math.round(new Date().getTime()/1000) + remainingSecs;
              strings = input.strings;
              asin = input.asin;
              ourPriceCurrency = input.ourPriceCurrency;
              ourPriceAmount = input.ourPriceAmount;

              if(input.timerContainerId) {
                timerContainerId = input.timerContainerId;
              }
              if(input.messageContainerId) {
                messageContainerId = input.messageContainerId;
              }
              if(remainingSecs > REFRESH_MIN_TIME) {
                // If the remaining seconds is less than 5 seconds, then just
                // wait for the price verification to be done when the timer reaches
                // 3 seconds. Else refresh the price asynchronously using ajax. This
                // fixes the case where we move to the detail page of a reverse cadence
                // asin with a back or a forward button click.
                refreshPrice();
              }
              countdown();
            }
          };
        }());

        return {start: rcpTimer.start};
    });
  </script>
</c:if>

        <c:if test="true">
  <script>
    P.when('A', 'jQuery').register('ajaxClientComponent', function (A, jQuery) {
        /**
         * The client used to make ajax calls.
         * In order to make ajax calls the call the fetch method with
         * a request object which has the following parameters:
         * 1. url: The url for the ajax call
         * 2. query: the json object of the query string parameters
         * 3. type(optional): post OR get (defaults to get)
         * 4. failureCallback(optional): method to call if the ajax call fails.
         * 5. successCallback(optional): method to call if the ajax call succeeds.
         * 6. recordMetrics(optional): Boolean indicating if CSM should be recorded.
         * 7. csmScope: The scope for the csm.
         */
        var ajaxClient = (function() {
          var response;
          function makeAjaxCall(request) {
            var isCSM = request && request.recordMetrics && request.csmScope && window.ue;

            var resolve = function(obj, statusText, xhr) {
              if(isCSM) {
                var requestId = response.getResponseHeader('x-amz-id-1');
                ues("id", request.csmScope, requestId);
                ues("t0", request.csmScope, new Date());
                ues( "ctb", request.csmScope , "1" );
                uet( "cf", request.csmScope );
              }
              if (!obj && request.failureCallback) {
                request.errorCallback();
              } else if(request.successCallback) {
                request.successCallback(obj, xhr);
              }
              if (isCSM) {
                uex( "ld", request.csmScope );
              }
            };

            var reject = function() {
              if (request.failureCallback) {
                request.errorCallback();
              }
            };

            if(isCSM) {
              uet( "tc", request.csmScope );
            }

            // make the ajax call and deal with the response
            response = jQuery.ajax({
              url: request.url,
              dataType: 'json',
              cache: false,
              data: request.query,
              type: request.type ? request.type : 'get',
              success: resolve,
              error: reject
            });
          };

          return {
            fetch: function(request) {
              if(request && request.url && request.query) {
                return makeAjaxCall(request);
              }
            }
          };
        })();
        return {fetch: ajaxClient.fetch};
    });
  </script>
</c:if>
    </div></div>



    <script>
        P.when('A', 'jQuery', 'rcpTimerComponent').execute(function (A, $, rcpTimer) {
            var countdownDealsStart = function () {
                var input = {
                    remainingTime : "479436",
                    timerContainerId: 'countdownTimer',
                    messageContainerId: 'countdownMsg',
                    asin: "B00B3B3QNS",
                    ourPriceAmount: "1.99",
                    ourPriceCurrency: "USD",
                    strings: {
                        days: "days",
                        day: "day",
                        hour: "h",
                        minute: "m",
                        second: "s",
                        countdownLatencyMsg: "The deal will expire momentarily. Order now before price goes up to <span class='a-color-price'>$3.99</span>"
                    }
                };

                $('#countdownWidget').removeClass('hidden');
                rcpTimer.start(input);
            }();
        });
    </script>

