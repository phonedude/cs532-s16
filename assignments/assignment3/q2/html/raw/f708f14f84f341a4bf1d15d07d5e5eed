<script type="text/javascript">
    jQuery(document).ready(function(){
        var path = window.location.pathname.toLowerCase();

        if (path.indexOf("/checkout/") == -1) { // Don't show on /checkout/ pages

            /********** MAIN SETTINGS **********/
          var lightboxImage = 'http://nba.frgimages.com/partners/leagues/NBAStore/2014Redesign/20160218/TNT-Thursdays[LIGHTBOX].jpg'; // Lightbox image
            var lightboxLink = 'http://store.nba.com/Gear_Up'; // Link for the lightbox image
            var closeClickOverlay = true; // Close when clicking the overlay/background of lightbox
            var closeClickX = true; // Close when clicking the 'X' on lightbox
            var closeEsc = true; // Close lightbox when hitting <ESC> key
            var closeClickLightbox = true; // Close lightbox when clicking actual lightbox content
            var useCookie = true; // Use a cookie to control how often lightbox is shown
            var lightboxCookie = 'lightboxCookieDesktop'; // Name of cookie to set
            var lightboxCookieMinutes = 480; // Expire after 8 hours (60min x 8)
            /********** MAIN SETTINGS **********/

            // Lightbox content
            var overlay = jQuery('<div id="lightboxOverlay"></div>');
            var lightboxOverlayContent = '<style type="text/css">#lightboxCloseImg { width: 40px; height: 40px; position: absolute; top: -15px; right: -15px; z-index: 10002; } div#lightboxModal { z-index: 10001; padding: 0px; position: fixed; left: 50%; top: 50%; height: auto; width: auto; margin-top: -300px; margin-left: -300px; } div#lightboxModal img { border: 0px; outline: none; } #lightboxOverlay {position: fixed; top: 0px; left: 0px; width: 100%; height: 100%; background-color: #000; filter:alpha(opacity=75); -moz-opacity:0.75; -khtml-opacity: 0.75; opacity: 0.75; z-index: 10000; } </style> <div id="lightboxModal"> <a href="' + lightboxLink + '" title="Shop the Sale"><img src="' + lightboxImage + '" alt="Sale" /></a> <a id="lightboxSaleCloseLink" href="javascript:" title="Close"><img id="lightboxCloseImg" src="//images.footballfanatics.com/partners/fanatics/madnessSaleX.png" alt="Close" /></a> </div>';

            if (useCookie) {
                var cookieValue = $.cookie(lightboxCookie); // Check cookie

                if (cookieValue != "seen") { // If lightbox hasn't been seen, show it and set cookie
                    jQuery(overlay).appendTo(document.body);
                    jQuery("body").prepend(lightboxOverlayContent);

                    var date = new Date();
                    date.setTime(date.getTime() + (lightboxCookieMinutes * 60 * 1000));
                    $.cookie(lightboxCookie, "seen", { expires: date, path: '/' });
                }
            } else {
                jQuery(overlay).appendTo(document.body);
                jQuery("body").prepend(lightboxOverlayContent);
            }

            // Close when clicking overlay
            if (closeClickOverlay) {
                jQuery("#lightboxOverlay").click(function() {
                    jQuery("#lightboxOverlay").hide();
                    jQuery("#lightboxModal").hide();
                    jQuery("#lightboxCloseImg").hide();
                });
            }

            // Close when clicking 'X'
            if (closeClickX) {
                jQuery("#lightboxSaleCloseLink").click(function() {
                    jQuery("#lightboxOverlay").hide();
                    jQuery("#lightboxModal").hide();
                    jQuery("#lightboxCloseImg").hide();
                });
            }

            // Close when clicking lightbox/content
            if (closeClickLightbox) {
                jQuery("#lightboxModal").click(function() {
                    jQuery("#lightboxOverlay").hide();
                    jQuery("#lightboxModal").hide();
                    jQuery("#lightboxCloseImg").hide();
                });
            }

            // Close when hitting <ESC>
            if (closeEsc) {
                jQuery(document).keyup(function(e) {
                    if (e.keyCode == 27) {
                        jQuery("#lightboxOverlay").hide();
                        jQuery("#lightboxModal").hide();
                        jQuery("#lightboxCloseImg").hide();
                    }
                });
            }

        }
    });
</script>

</span>
