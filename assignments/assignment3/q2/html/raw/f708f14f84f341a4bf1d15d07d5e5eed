<script>
    /* Non-Foundation Desktop */
    jQuery(document).ready(function() { /*Message to look for*/
        var shipMessageToCheck = "flat rate shipping"; /*Message to display next to in stock.*/
        var shipMessageInStock = " GUARANTEED by 12/24 or it's FREE when you order today.";
        if ((jQuery("div.pdpShippingMemo .shippingStrong:contains('" + shipMessageToCheck + "')").length > 0) && (jQuery("div.inStockMessage span:contains('In Stock')").length > 0)) {
            jQuery("div.inStockMessage").html("<span class='b-el-4 b-font-weight-medium b-swatch-3'>In Stock - </span><span class='content-ship-logo'></span><span class='b-el-4 b-font-weight-medium b-swatch-3'>" + shipMessageInStock + "</span><a class='b-font-weight-medium' href='#' id='shipContentModal' style='font-size:15px'> See Details.</a>"); /*Lightbox content*/
            var body = document.body; /*create the overlay, add to body*/
            var overlay = document.createElement('div');
            overlay.setAttribute('id', 'lightboxOverlay');
            body.appendChild(overlay); /*create & add css*/
            var css = [ /*base styles*/ 'body {position: relative !important;}', '#lightboxModal { z-index: 10001; padding:0; position: fixed; left: 50%; top: 50%; margin-top: -300px; margin-left: -300px; display: none; }', '#lightboxCloseImg { width: 40px; height: 40px; position: absolute; top: -15px; right: -15px; z-index: 10002; }', '#lightboxModal .primary { border:0; outline: none; height: auto; width: 100%; margin: 0px auto; text-align: center; }', '#lightboxOverlay {position: fixed; top:0; left:0; width: 100%; height: 100%; background-color: #000; filter:alpha(opacity=75); -moz-opacity:0.75; -khtml-opacity: 0.75; opacity: 0.75; z-index: 10000; display: none; }', /*mobile styles*/ '@media(max-width: 1000px) {#lightboxModal { left: 10%; right: 10%; top: 10%; margin: 0px auto; text-align: right; }}'].join('\n');
            var style = document.createElement('style');
            style.innerHTML = css;
            body.insertBefore(style, body.firstChild); /*create modal, add to body*/
            var modal = document.createElement('div'); /*please use this id so we can track the interstitial being shown*/
            modal.setAttribute('id', 'lightboxModal');
            modal.innerHTML = '<div class="ship-promo-container"><a id="lightboxSaleCloseLink" href="javascript:" title="Close"><img id="lightboxCloseImg" src="//images.footballfanatics.com/partners/fanatics/madnessSaleX.png" alt="Close"></a><img src="//images.footballfanatics.com/Fanatics/gtgt_lightbox_nolink.jpeg" /><a href="/pages/holiday_shipping" class="ship-terms-conditions-text">See Terms & Conditions for Details</a></div>';
            body.appendChild(modal); /*show the modal*/
            function show() {
                modal.style['display'] = overlay.style['display'] = 'block';
            } /*hide the modal*/
            function hide() {
                modal.style['display'] = overlay.style['display'] = 'none';
            }
            overlay.onclick = hide;
            modal.lastChild.onclick = hide;
            body.onkeyup = function(e) {
                if (e.keyCode === 27) {
                    hide();
                }
            } /*Hide modal when clicking image*/
            modal.onclick = hide; /*On click for details show modal*/
            jQuery('#shipContentModal').on('click', function() {
                show();
            });
        }
    });
</script>
