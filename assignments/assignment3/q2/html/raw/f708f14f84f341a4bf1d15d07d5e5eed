<script type="text/javascript">

function getCookie(name) {
    var cookie = document.cookie;
    var cookieNames = cookie.split(';');
    var cookieValue = '';
    if(name.indexOf('=') < 0){
       name = name + '=';
    }
    jQuery.each(cookieNames, function (i, item) {
        var trimmedItem = jQuery.trim(item).toLowerCase();
        var itemIndex = trimmedItem.indexOf(name);
	if (itemIndex >= 0){
           cookieValue = trimmedItem.substring(name.length,trimmedItem.length);
           return false;
	}
    });

    return cookieValue;
};

var test1Id = getCookie('s_fuid');
	if(test1Id === 'undefined' || test1Id === null){
           test1Id = '';        
        };

var google_tag_params = {
ecomm_prodid: '',
ecomm_pagetype: 'homepage',
ecomm_totalvalue: '0',
_google_crm_id: test1Id
};
</script>
