}// Shrink The Story if too large
$j(function() {
    if($j('#thestory')) {
        var containerHeight = $j('.fundraiserContent').height();
        var containerMax = $j('#thestory-wrapper').data('containermax');
        var storyHeight = $j('#thestory').height();
        var showMoreButtonHeight = 50;
        if(containerHeight > containerMax) {
            var newStoryHeight = storyHeight - (containerHeight - containerMax) - showMoreButtonHeight;
			if(newStoryHeight < 0) {
				newStoryHeight = 0;
			}
            $j('#thestory-wrapper').data('shrunkenheight',newStoryHeight);
            $j('#thestory-wrapper').height(newStoryHeight).css('overflow','hidden');
            $j('#thestory-showmoreless').show();
        }
    }
});

// Toggle full/partial display of The Story
function storyShowMoreLess() {
    var shrunkenHeight = $j('#thestory-wrapper').data('shrunkenheight');
    var storyHeight = $j('#thestory').height() + 20;
    var currentHeight = $j('#thestory-wrapper').height();
    if(currentHeight == shrunkenHeight) {
        $j('#thestory-wrapper').animate({height:storyHeight});
        $j('#thestory-showmoreless a').text('Show Less');
    } else {
        $j('#thestory-showmoreless a').text('See More');
        $j('#thestory-wrapper').animate({height:shrunkenHeight});
    }
}

// jQuery ready
$j(function() {
	// Donate button
	$j('.sponsorProject').click(function(e) {
		e.preventDefault();
		window.location.href='/fundraise-and-volunteer/donate-desktop/project/avril/avrillavigne/0';
	});

	// Registry Donate button
	$j('.donateRegistry').click(function(e) {
		e.preventDefault();
		var args = {
			'display_default': 	'',
			'ask_matching': 		'N',
			'ask_newsletter': 		'N',
			'ask_company_behalf': 	'N',
			'fundraiser_id': 	'211917',
			'campaign_id':    '',
			'charity_name': 		'THE AVRIL LAVIGNE FOUNDATION',
			'project_username': 	'avril',
			'owner_name': 			'Avril Lavigne',
			'owner_username':		'avrillavigne',
			'payment_processor':        'NFG',
			'project_name': 		'My Birthday Wish for Special Olympics',
		    'charities':	{
						}		 }
		launchLightbox('project_registry_donate',args,650,true);
	});
});


function get_charity_donation_lightbox_text()
{
    var charity_username = $j('#charities_select').val();
    if(charity_username.length == 0)
    {
        $j('#charity_donation_text').html("&nbsp;");
        return false;
    }

    showProcessing('sponsor_project_form');
    $j.ajax({
        type: 'POST',
        url: '/ajaxer/get_charity_donation_lightbox_text',
        data: {charity_username: charity_username},
        success: function(d){
            $j('#charity_donation_text').html(d.donation_text);
           hideProcessing();
        },
        dataType: 'json'
    });

    return false;
}


