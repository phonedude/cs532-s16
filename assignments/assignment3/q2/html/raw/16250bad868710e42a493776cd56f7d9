$j(document).ready(function () {

	var $email_signup = $j('#email_signup');
	$email_newsletter_button = $j('#footerNewsletterSubmit');

    $j('#email_signup_form').submit(function(e) {
        if($email_signup.val() == '') {
            launchModal('newsletter_subscribe_general', 'small', { 'message':'Please enter an valid email!' });
        } else {
            launchModal('newsletter_subscribe_progress', 'small', {'data':null});
            email_signup_submit();
        }
        return false;
    });


	$email_newsletter_button.click(function (e) {
        if($email_signup.val() == '') {
            launchModal('newsletter_subscribe_general', 'small', {'message':'Please enter an valid email!'});
        } else {
       	launchModal('newsletter_subscribe_progress', 'small', {'data':null});
		email_signup_submit();
        }
	});	
});

function email_signup_submit() {
	email_signup_val = $j('#email_signup').val();

	$j.ajax({
		type: 'POST',
		url: '/ajaxer/newsletter_signup',
		data: {email:email_signup_val},
		beforeSend: function () {

			var form_data=Form.serialize('email_signup_form');
			var tmp=new Ajax.Request('/ajaxer/validate_form',{method:'post',parameters:form_data,asynchronous:false});
			var info=eval('('+tmp.transport.responseText+')');

			if(info.data[0].status=='OK' && info.data[0].result===0) {
				params = {
					message: info.data[0].error.message
				}

                		switchModalView('newsletter_subscribe_general', 'small', params);
				return false;
			} else if (info.data[0].status=='ERROR') {
				params = {
					message: "Something went wrong. Close this and try again."
				}
               		switchModalView('newsletter_subscribe_general', 'small', params);
				return false;
			}

		},
		success: function(d){
			if(d.data[0].status == "OK") {
				var response_message = d.data[0].message;

				var response_message = response_message.replace( /<.*?>/g, '' );
				params = {
					message:response_message
				}
               		switchModalView('newsletter_subscribe_general', 'small', params);
			} else {
				var response_message = d.data[0].message;
				var response_message = response_message.replace( /<.*?>/g, '' );
				param = {
					message:response_message
				}
                		switchModalView('newsletter_subscribe_general', 'small', params);
			}
		},
		failure: function(d){
			param = {
				message: "There was a problem adding you subscription. Please try again or contact us."
			}
            		switchModalView('newsletter_subscribe_general', 'small', params);
		},
		dataType: 'json'
	});
	return false;
}	var data = {
		formid:'popup_login_form',
		id: 'popup_login_email',
		passwd:'popup_login_password',
		alert_target:'popup_login_error',
		remember:'popup_remember',
		comp:false,
		redirect:'popup_redirect',
		processing:'signin',
        action:'login_js'
	}

    var sc = $('popup_login_submit');
    if(sc!=undefined){
        $(sc).observe('click', function(){
            if(login.checkForm(data)==true){
                $('popup_login_form').submit();
                return false;
            }	
        });
    }


var pmm=$('popup_message_message');
if(pmm!=undefined){
    pmm.observe('keyup', function(e){
        if(pmm.value.length>2500){
            truncate('popup_message_message',2500);
            $('#popup_message_target').html("<p>Sorry, but we've got to limit messages to 2500 characters.</p>");
        }
    });
}

