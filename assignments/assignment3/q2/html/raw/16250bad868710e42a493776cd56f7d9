<script>

    
    
    $j(".has-dropdown a").click(function(){
    
        var dropdownParent = $j(this).closest("li.has-dropdown");
        
        if(dropdownParent.hasClass("hover")){
            dropdownParent.removeClass("hover");
        } else {
            dropdownParent.addClass("hover");
        }   
    });
</script>
