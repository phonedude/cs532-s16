function updateImages(){
    $j('.botContent img').each(function(){
         if( $j(this).attr('src') == 'https://www.crowdrise.com/media/static/crowdrise-default-2012-195x195.png') {
            $j(this).attr('src', '/media/static/AvrilSquare.jpg');
         }
    });
}
$j(function(){
    updateImages();

    $j('.botContent').bind('DOMSubtreeModified', function(e) {
        updateImages();
    });
});


