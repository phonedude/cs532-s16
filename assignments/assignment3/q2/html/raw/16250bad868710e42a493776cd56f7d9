// Load join team modal on desktop
function joinTeamModal(){
	$j(document).ready(function() {
					launchModal('login', 'small', {redirect: document.URL}, function(){
				$j("#login_email").autofocus();
                set_user_session('join_lb');
			});
			});
}

// Load join_register modal
$j('.joinTheEvent').on("click", function() {

    var join_team_data = [];

    var data = {
        'campaign_id': '',
        'fundraiser_id': '211917',
        'campaign_registration_register_button_text': '',
        'campaign_registration_already_registered_text': '',
        'join_team_data': join_team_data
    }

    
    launchModal('join_register', 'small', data);
});


function aspectRatioPadFix(iframes) {
	for (var i = 0; i < iframes.length; i++) {
		if($j(iframes[i]).attr('height') == 281){
			$j(iframes[i]).parent().attr('style', 'height:281px;padding:47px 0 48px');
		}
		else if($j(iframes[i]).attr('height') == 375){
			$j(iframes[i]).parent().attr('style', 'height:375px;padding:0');
		}
	}
}


