<div class="shareBarHelper">
	<div id="shareBar"></div>
</div>

<script>
	
	$j(document).ready(function(){
		if(!device.mobile()){
		
			var shareBarHelper = $j(".shareBarHelper");
			var shareBarHelperClone = shareBarHelper.clone();
			
			shareBarHelperClone.empty();
			shareBarHelperClone.attr("id", "");
			
			shareBarHelperClone.insertAfter($j(".ls-articoloTesto"));
	
			shareBarHelper.addClass("top-helper");
			shareBarHelperClone.addClass("bottom-helper");
	
			$j("#shareBar").width($j(".shareBarHelper").width());
			
			var barBounding = "";
			var barHelperBounding = "";
			var barHelperCloneBounding = "";
			var fixedBounding = "";
			var articoloBounding = "";
			
			var h = Math.max(document.documentElement.clientHeight, window.innerHeight || 0)
					
			window.addEventListener("scroll", function(){
				
				h = Math.max(document.documentElement.clientHeight, window.innerHeight || 0)
				
				barBounding = $j("#shareBar")[0].getBoundingClientRect();
				barHelperBounding = $j(".shareBarHelper.top-helper")[0].getBoundingClientRect();
				barHelperCloneBounding = $j(".shareBarHelper.bottom-helper")[0].getBoundingClientRect();
				fixedBounding = ($j("#getFixed").length > 0) ? $j("#getFixed")[0].getBoundingClientRect() : 0;
				articoloBounding = $j(".ls-articoloTesto")[0].getBoundingClientRect();
				
				if(fixedBounding != 0){
					if($j("#shareBar").parent().hasClass("top-helper")){
						if(barHelperBounding.top < fixedBounding.bottom){
							//FIXED
							if(!$j("#shareBar").hasClass("fixed")){
								$j("#shareBar").addClass("fixed").addClass("shareBar-border");
								
								$j("#shareBar").css({
									top: fixedBounding.bottom,
									left: fixedBounding.left
								});
							}
						}
						
						if(barHelperBounding.top > fixedBounding.bottom){
							//NO FIXED
							if($j("#shareBar").hasClass("fixed")){
								$j("#shareBar").removeClass("fixed").removeClass("shareBar-border");
							}
						}
						
						if(barHelperCloneBounding.top < h){
							//SPOSTA IN BASSO
							if($j(".shareBarHelper.bottom-helper").find("#shareBar").length == 0){
								$j("#shareBar").removeClass("fixed").removeClass("shareBar-border");
								$j("#shareBar").detach().appendTo($j(".shareBarHelper.bottom-helper"));
							}
						}
					}else if($j("#shareBar").parent().hasClass("bottom-helper")){
						
						if(barHelperCloneBounding.top > h){
							//SPOSTA IN ALTO
							$j("#shareBar").detach().appendTo($j(".shareBarHelper.top-helper"));
							
							if(!$j("#shareBar").hasClass("fixed")){
								$j("#shareBar").addClass("fixed").addClass("shareBar-border");
								
								$j("#shareBar").css({
									top: fixedBounding.bottom,
									left: fixedBounding.left
								});
							}
						}
					}
				}
				
				
			});
		}
	});
	
	
</script>

