	$j("#mmArrowUp").click(function(){
		
		if($j(this).hasClass("scrollActive")){
			var newTop = $j("#carouselPiuVisti").position().top + 240;
		
			if(newTop >= 0){
				newTop = 0;
			}
			
			$j("#carouselPiuVisti").animate({
				top: newTop
			}, 200, "linear", function(){
				updateArrows();
			});
		}
	});
	
	$j("#mmArrowDown").click(function(){
		
		if($j(this).hasClass("scrollActive")){
			var newTop = $j("#carouselPiuVisti").position().top - 240;
		
			var offsetHeight = $j("#carouselPiuVisti").height() - $j("#contenitorePiuVisti").height();

			if(offsetHeight - Math.abs(newTop) <= 0){
				newTop = 0 - offsetHeight - 20;
			}

			$j("#carouselPiuVisti").animate({
				top: newTop
			}, 200, "linear", function(){
				updateArrows();
			});
		}
	});
	
	function updateArrows(){
	
		var arrowUp = $j("#mmArrowUp");
		var arrowDown = $j("#mmArrowDown");
		
		var curTop = $j("#carouselPiuVisti").position().top;
		var offsetHeight = $j("#carouselPiuVisti").height() - $j("#contenitorePiuVisti").height();

		if(curTop < 0){
			arrowUp.addClass("scrollActive");
		}else{
			arrowUp.removeClass("scrollActive");
		}

		if(offsetHeight - Math.abs(curTop) <= 0){
			arrowDown.removeClass("scrollActive");
		}else{
			arrowDown.addClass("scrollActive");
		}
	}
</script>

</div>
