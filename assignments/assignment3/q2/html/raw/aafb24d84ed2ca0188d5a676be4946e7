		//
		if ($j("#containerArtCorr .ls-boxUltimoArticolo").size() <= 3) {
			$j("#containerArtCorr #articoliCorrScrollDown").attr('src','/common/img/arrow_down_disabled.png');
			$j("#containerArtCorr #articoliCorrScrollDown").removeClass('.active');

			$j("#containerArtCorr #articoliCorrScrollDown").css('display','none');
			$j("#containerArtCorr #articoliCorrScrollUp").css('display','none');
