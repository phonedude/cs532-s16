	function popolateCookie(){
		//se nel cookie c'è questo id
		var LStp_mcp_nl = getCookie("LStp_mcp_NL");
		//console.log("LStp_mcp_nl => " + LStp_mcp_nl);
		 
		if(LStp_mcp_nl.indexOf("|"+4+"|")>=0){
			//console.log("c'è-");
		}else{
			LStp_mcp_nl = LStp_mcp_nl + "|" + 4 + "|";
			//console.log("non c'è => "+LStp_mcp_nl);
			createCookie("LStp_mcp_NL", LStp_mcp_nl, 1);
		}
		
		$j("#loading_modal").fadeIn(300);
		$j.ajax({
		  	type: "POST",
		  	url: "/modulo/registrazione/newsletter/jsp/gestione_newsletter.jsp",
		  	data: { 
		  		rdr: 1,
		  		logutente:'http://www.lastampa.it/2014/09/20/esteri/i-medici-stanno-scappando-cos-la-sierra-leone-muore-eOVyzVcOZasXQHqUGq00RM/pagina.html?lgut=1'
		  	}
		}).done(function( data ) {	
			$j("#loading_modal").fadeOut(300);
			
			$j(".conferma_reg_nl").fadeIn();	
			$j("#overlay_data").html(data);
		});
	}
	
	//returns the value of a specified cookie
	function getCookie(cname) {
	    var name = cname + "=";
	    var ca = document.cookie.split(';');
	    for(var i=0; i < ca.length; i++) {
	        var c = ca[i];
	        while (c.charAt(0)==' ') c = c.substring(1);
	        if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
	    }
	    return "";
	} 
	
	//create cookie
	function createCookie(name,value,days) {
	    if (days) {
	        var date = new Date();
	        date.setTime(date.getTime()+(days*24*60*60*1000));
	        var expires = "; expires="+date.toGMTString();
	    }
	    else var expires = "";
	    document.cookie = name+"="+value+expires+"; path=/;172.20.60.63";
	}
	
	$j(".close_conf").click(function(e){
		e.preventDefault();
		$j(".conferma_reg_nl").fadeOut();
		$j("#overlay_data").empty();
	});	
	
	$j(".close_box_nl").click(function(e){
		e.preventDefault();
		$j(".contenitore_nl").fadeOut();
		createCookie("LStp_mcp_closeBox", 1, 1);
	});			
</script>

