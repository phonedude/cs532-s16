<script type="text/javascript">
	$j(function() {
		$j.cachedScript(
			'http://www.lastampa.it/common/js/element.write.min.js',
			function () {
				$j.cachedScript(
					'http://www.lastampa.it/common/js/writeCapture2.min.js',
					function () {
						if (!device.mobile()){
							var adFrame1 = $j('<div id="ad_Frame1" style="width:320px;margin-left:0px;margin-bottom:12px;">');
							adFrame1.insertAfter($j("#frame1PlaceHolder"));
							if (adv_server == "adagio") {
								writeCapture.write(document.getElementById('ad_Frame1'),'<scr'+'ipt type="text/javascript">adagioWriteBanner("HALFPAGE")</scr'+'ipt>');
							}
							else {
								writeCapture.write(document.getElementById('ad_Frame1'),'<scr'+'ipt type="text/javascript">OAS_AD("Frame1")</scr'+'ipt>');
							}
						}
						else {
							var adFrame1 = $j('<div id="ad_Frame1" style="text-align:center;width:300px;margin:auto;">');
							if (mm_section == '/sport/web-tv') {
								adFrame1.insertAfter($j("#ls-multimedia"));
							} else if (mm_section == "/forum" && titolo_articolo == "") {
								adFrame1.insertAfter($j("table"));
							}
							else if (mm_section == "/Blogs" && titolo_articolo == "") {
								adFrame1.insertAfter($j(".ls-lastBlog-list"));
							}
							else if (mm_section == "/societa/montagna" && titolo_articolo == "") {
								adFrame1.insertAfter($j(".ls-boxMultimedia:eq(0)"));
							}
							else if (titolo_articolo == "") {
								// se in homepage
								if (mm_section == "/" || mm_section == "/test-interni") {
									adFrame1.insertAfter($j("#apertura"));
								}
								else if ($j(".ls-boxMM3col").length > 0) {
									adFrame1.insertAfter($j(".ls-boxMM3col").parent());
								}
								else if ($j(".ls-box-object").length > 4) {
									adFrame1.insertAfter($j(".ls-box-object:eq(1)"));
								}
							}
							else {
								if (media_type == 'articolo') {
									if ($j(".ls-articoloTesto p:eq(1)").length > 0) {
										$j(".ls-articoloTesto p:eq(1)").after(adFrame1);
									}
									else if ($j(".ls-articoloTesto br:eq(1)").length > 0) {
										$j(".ls-articoloTesto br:eq(1)").after(adFrame1);										
									}
									else {
										$j(".ls-articoloTesto p:eq(0)").after(adFrame1);
									}
								}
								else {
									adFrame1.insertAfter($j("#ls-multimedia"));
								}
							}
							if (adv_server == "adagio") {
								writeCapture.write(document.getElementById('ad_Frame1'),'<scr'+'ipt type="text/javascript">adagioWriteBanner("MBOX1")</scr'+'ipt>');
							}
							else {
								writeCapture.write(document.getElementById('ad_Frame1'),'<scr'+'ipt type="text/javascript">OAS_AD("Frame1")</scr'+'ipt>');
							}
						}
					}
				);
			}
		);
	});
