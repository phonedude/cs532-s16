<script class="_iub_cs_activate" type="text/plain">
	function sendTrack(){
		trackNielsen();
		trackWebTrekk();
	}
	
	if (!Array.prototype.indexOf) {
	    Array.prototype.indexOf = function(obj, start) {
	         for (var i = (start || 0), j = this.length; i < j; i++) {
	             if (this[i] === obj) { return i; }
	         }
	         return -1;
	    }
	}
	
	function sendTrack(arrayTracciatori){
		if(arrayTracciatori.indexOf("nielsen") > -1){
			//console.log("sendTrack nielsen");
			trackNielsen();
		}
		if(arrayTracciatori.indexOf("webtrekk") > -1){
			//console.log("sendTrack webtrekk");
			trackWebTrekk();
		}
	}
