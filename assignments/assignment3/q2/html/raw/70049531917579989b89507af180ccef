        <script>

        function isMobileSafari() {
            return navigator.userAgent.match(/iPod|iPhone|iPad/) && navigator.userAgent.match(/Safari/)
        }

        if(isMobileSafari()){
             //Get the fixed element, and the input element it contains.
            var fixed_el = document.getElementById('fixed-nav');
            var input_el = document.getElementById('mce-EMAIL');
            //Listen for touchstart, focus will fire too late.
            input_el.addEventListener('touchstart', function() {
                //Switch to position absolute.
                fixed_el.style.cssText = 'position: absolute; top: '+ window.scrollY + 'px; -o-transition-duration: 0s; -moz-transition-duration: 0s; -ms-transition-duration: 0s; -webkit-transition-duration: 0s;transition-duration: 0s;';

                //stay at top when user scrolls
                function onScroll() {
                    fixed_el.style.top = window.scrollY + 'px';
                }

                window.addEventListener("scroll", onScroll);

                //Switch back when focus is lost.
                function blured() {
                    fixed_el.style.position = 'fixed';
                    fixed_el.style.top = '0';
                    input_el.removeEventListener('blur', blured);
                    window.removeEventListener("scroll", onScroll);
                }
                input_el.addEventListener('blur', blured);
            });
        }


        $(window).scroll(function(event) {
            if ($(window.scrollY)[0] > $('#next_button').offset().top - $(window).height()){
                $('#subscribe-nav').removeClass('hide');
            } else {
                $('#subscribe-nav').addClass('hide');
            }
        });
        $(document).ready(function(){
            $('#mc-embedded-subscribe').click(function(){
                $('#mc_embed_signup').empty();
                $('#mc_embed_signup').append('<p>Thanks! Check your email to confirm.</p>');
            });
        });
    </script>
