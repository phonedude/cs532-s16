    <script type="text/javascript">

        //mobile nav toggle logic
        $('.navbar-toggle').click(function(){
            $('.navbar').css('background-color', 'rgba(0, 0, 0, 0.83)')
            $('.navbar-fixed-top .navbar-collapse').slideToggle('fast',function(){
                if ($('.navbar-fixed-top .navbar-collapse').is(':hidden'))
                {
                    $('.navbar-toggle').empty();
                    $('.navbar-toggle').append('<span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span>');

                }
                else
                {
                    $('.navbar-fixed-top .navbar-collapse').css('padding-bottom','50px');
                    $('.navbar-toggle').empty();
                    $('.navbar-toggle').append('<span class="fa fa-times" style="color:white; font-size:30px;" aria-hidden="true"></span>');
                }
            })
        });

        //fade in black after scroll
        $(window).scroll(function() {
            if ($(this).scrollTop() > 200) {
                $('.navbar').addClass('navbar-black');
            }
        })
    </script>
