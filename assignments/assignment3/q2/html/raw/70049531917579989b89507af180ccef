            <script type="text/javascript">

        var qs = (function(a) {
            /*
            Get a query string value in with javascript.
            Usage:
            qs["topic"];    // 123
            qs["name"];     // query string
            qs["nothere"];  // undefined (object)
             */
            if (a == "") return {};
            var b = {};
            for (var i = 0; i < a.length; ++i)
            {
                var p=a[i].split('=', 2);
                if (p.length == 1)
                    b[p[0]] = "";
                else
                    b[p[0]] = decodeURIComponent(p[1].replace(/\+/g, " "));
            }
            return b;
        })(window.location.search.substr(1).split('&'));


        function createPidTrackingCookie(name,value,days) {
            // This cookie needs to expire after 3 minutes, so pass in 1 for 'days'
            if (days) {
                var date = new Date();
                date.setTime(date.getTime()+(days*6000*30));
                var expires = "; expires="+date.toGMTString();
            }
            else var expires = "";
            document.cookie = name+"="+value+expires+"; path=/";
        }

        function readCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for(var i=0;i < ca.length;i++) {
                var c = ca[i];
                while (c.charAt(0)==' ') c = c.substring(1,c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
            }
            return null;
        }

        function eraseCookie(name) {
            createPidTrackingCookie(name,"",-1);
        }

        function get_campaign_name(){
            return window.location.pathname;
        }

        function get_pid_from_url(){
            var pid_from_hash = 5;
            if (qs['adp']) {
                pid_from_hash = qs['adp'];
            }

            if (qs['utm_campaign']) {
                pid_from_hash = qs['utm_campaign'];
            }

            return pid_from_hash;
        }

        function get_or_create_pid_cookie(){
            var pid_cookie;
            if(document.cookie.indexOf("allday_campaign") >= 0){
                pid_cookie = readCookie('allday_campaign');
            } else {
                createPidTrackingCookie('allday_campaign', get_pid_from_url()+'|'+ get_campaign_name(), 1);
                pid_cookie = readCookie('allday_campaign');
            }
            return pid_cookie;
        }

        function get_cookied_campaign_data(){
            return get_or_create_pid_cookie().split('|');
        }

        function set_page_context_pid_to_cookie_value(){
            window.STM.page_context.pid = get_cookied_campaign_data()[0];
        }

        set_page_context_pid_to_cookie_value();

    </script>

