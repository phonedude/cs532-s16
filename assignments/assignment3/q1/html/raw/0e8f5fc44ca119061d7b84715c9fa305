<script type="text/javascript">
  

  
P.when("A", "ready").execute(function(A){
  var triggered = 0;
  var reviewsSection = A.$("#cm_cr_dpwidget");  
  onScreenTriggerMethod();
  A.on.scroll(onScreenTriggerMethod);
  
  function onScreenTriggerMethod(){
    if (!triggered && A.onScreen(reviewsSection)) {
      triggered = 1;
      A.post('/gp/customer-reviews/aj/customer/ranking-trigger', {
          success: function(data){
          },
 
          error: function(data){
          },

          params: {
            csrfT: '0.506788270168897;1455821408;d4e6c26dd128e7e70976884e5911a547fb256349b7f1c5b7767ed87d19cf0fe0',
            requestId: '1GCG1SPBFVTZ0RJT1SV1',
            sessionId: '000-0000000-0000000',
            weblabID: ''
          }
      });
    }
  }

});

</script>








