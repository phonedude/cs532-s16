<script type="text/javascript">
function addStockHidden(hidden) {
    var html = '';
    for (key in hidden) {
        html += '<input name="' + key + '" type="hidden" value="' + hidden[key] + '">';
    }
    document.getElementById("mdItemInfoStockHidden").innerHTML = html;
}
</script>
