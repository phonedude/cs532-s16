jQuery('.bt.twitter.popup').attr('data-link','https://twitter.com/intent/tweet?original_referer='+url+'&text=Bacánika - ES MEJOR SER COMO JON SNOW Y NO SABER NADA&url='+url+'&via=Revista Bacanika');
