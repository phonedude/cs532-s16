})
.mouseout(function() {
	//alert("se fue");
	jQuery(".boxsocial").css('opacity','0');
});

jQuery(".boxsocial").mouseover(function(){
	jQuery(".boxsocial").css('opacity','1');
});

jQuery(".popup").click(function(){
//window.location = jQuery(this).attr('data-link');
window.open(jQuery(this).attr('data-link'),'_blank');
});

