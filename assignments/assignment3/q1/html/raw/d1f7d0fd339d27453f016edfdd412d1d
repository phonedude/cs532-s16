	    /* Page Metrics */
	    ITSMetrics.omniture.pageName="SEO-Software-JP-CYBIRD Co., Ltd.-\u30A4\u30B1\u30E1\u30F3\u5E55\u672B\u25C6\u904B\u547D\u306E\u604B \u5973\u6027\u5411\u3051\u4E59\u5973\u30FB\u604B\u611B\u30B2\u30FC\u30E0-733707653";
