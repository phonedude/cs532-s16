            var teamMapping = {
                "team-3": "arsenal",
                "team-7": "aston-villa",
                "team-57": "watford",
                "team-8": "chelsea",
                "team-31": "crystal-palace",
                "team-11": "everton",
                "team-45": "norwich-city",
                "team-13": "leicester-city",
                "team-14": "liverpool",
                "team-43": "manchester-city",
