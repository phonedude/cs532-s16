<script>
offbeatRecirc();
setInterval(function(){
stickyHeader();
}, 10000);
$jq(document).scroll(function () {
stickyHeader();
});
function stickyHeader() {
var headerPlaceHolder = $jq('.js-header-placeholder');
var headerPlaceHolderOffset = headerPlaceHolder.offset().top;
var scrollTop = $jq(window).scrollTop();
var headerPosition = (headerPlaceHolderOffset - scrollTop);
var header = $jq('.js-tpx-header-wrapper');
if (headerPosition <= 0) {
header.addClass('str-fixed-header');
headerPlaceHolder.addClass('str-fixed-header-placeholder');
} else {
header.removeClass('str-fixed-header');
headerPlaceHolder.removeClass('str-fixed-header-placeholder');
}
}
function offbeatRecirc() {
var offbeatRecircLink = $jq('.js-offbeat-recirc-link');
var offbeatRecircDiv = $jq('.js-offbeat-recirc');
offbeatRecircLink.hover(
function() {
$jq('.js-offbeat-recirc').stop(true, true).slideDown();
}, function() {
$jq('.js-offbeat-recirc').stop(true, true).slideUp();
});
offbeatRecircDiv.hover(
function() {
$jq(this).stop(true, true).show();
}, function() {
$jq(this).stop(true, true).slideUp();
});
}
$jq('.js-header-section-dropdown').hover(
function(){
$jq('.js-header-section').removeClass('header-item-hover');
$jq(this).closest('.js-header-section').addClass('header-item-hover');
if ($jq(this).hasClass('js-offbeat-recirc')) {
$jq('.js-offbeat-recirc-link').addClass('header-item-hover');
}
},
function(){
$jq('.js-header-section').removeClass('header-item-hover');
});
function showHideHeaderItem(link) {
var menuLeftWidth = $jq('.js-header-left').width();
var menuRightWidth = $jq('.js-header-right').width();
var headerWidth = $jq('.js-header').width();
var videoLink = $jq(link);
var videoLinkWidth = videoLink.width();
var occupiedWidth = ( menuLeftWidth + menuRightWidth);
var availableWidth = ( headerWidth - occupiedWidth );
if(availableWidth >= videoLinkWidth ) {
videoLink.show();
} else {
videoLink.hide();
}
}
showHideHeaderItem('.js-videos');
</script>
<script>
$jq(document).ready(function() {
if (navigator.appVersion.indexOf("Win")!=-1) {
$jq('.js-header li a').addClass('windows-bold');
}
});
</script>
