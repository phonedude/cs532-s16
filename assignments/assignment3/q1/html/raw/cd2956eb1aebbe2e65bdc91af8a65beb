sbi_morpheus.register('div-gpt-ad-6', '97762c449c1b0519df5e'); // topix_300_apex_flex
//sbi_morpheus.register('div-gpt-ad-7', '97762c449c1b0519df5e'); // topix_300_apex_flex
sbi_morpheus.register('div-gpt-ad-2', 'c52783c03e51f25b23a0'); // topix_160_apex
if (gpt_request.staging) {
console.log("sonobi placments registered");
}
} catch(e) {
if (gpt_request.staging) {
alert(e);
}
logAdEvent("sonobi-exception");
}
}
</script>
<script> var perform_gpt_div_id;
var crtg_content;
var rtp_size = ""; // set via TT
var adSlot;
googletag.cmd.push(function() {
// dart_oop
adSlot = googletag.defineOutOfPageSlot("/4908/Desktop_Politics", "div-gpt-ad-16")
.addService(googletag.pubads())
.setTargeting("bw", "1")
.setTargeting("n", "us/politics")
.setTargeting("forumtype", "forumview")
.setTargeting("sect", "forums");
gpt_request.gpt_adslots['dart_oop'] = adSlot;
if (gpt_request.debugPubmatic) {
adSlot.setTargeting('bid','.53434');
}
setupRubiconTargeting("dart_oop");
if (gpt_request.yieldbot && $jq.inArray('dart_oop', gpt_request.yieldbot_slots) >= 0) {
try {
yieldbot.setSlotTargeting('dart_oop', adSlot);
if (gpt_request.staging) {
console.log('yieldbot targeting set for dart_oop');
}
}
catch (e) {
if (gpt_request.staging) {
console.log('yieldbot exception: ' + e);
yieldBotNotReady('yieldbot-targeting');
}
}
}
// dart_pushdown
adSlot = googletag.defineSlot("/4908/Desktop_Politics", [[970,90],[970,250],[728,90],[970,66]] , "div-gpt-ad-1")
.addService(googletag.pubads())
.setTargeting("bw", "1")
.setTargeting("n", "us/politics")
.setTargeting("forumtype", "forumview")
.setTargeting("sect", "forums");
adSlot.setTargeting('pos','top');
gpt_request.gpt_adslots['dart_pushdown'] = adSlot;
if (gpt_request.debugPubmatic) {
adSlot.setTargeting('bid','.53434');
}
setupRubiconTargeting("dart_pushdown");
if (gpt_request.yieldbot && $jq.inArray('dart_pushdown', gpt_request.yieldbot_slots) >= 0) {
try {
yieldbot.setSlotTargeting('dart_pushdown', adSlot);
if (gpt_request.staging) {
console.log('yieldbot targeting set for dart_pushdown');
}
}
catch (e) {
if (gpt_request.staging) {
console.log('yieldbot exception: ' + e);
yieldBotNotReady('yieldbot-targeting');
}
}
}
// dart_rect
adSlot = googletag.defineSlot("/4908/Desktop_Politics", [[300,1050],[300,600],[300,250]] , "div-gpt-ad-6")
.addService(googletag.pubads())
.setTargeting("bw", "1")
.setTargeting("n", "us/politics")
.setTargeting("kw", "top")
.setTargeting("forumtype", "forumview")
.setTargeting("sect", "forums");
gpt_request.gpt_adslots['dart_rect'] = adSlot;
if (gpt_request.debugPubmatic) {
adSlot.setTargeting('bid','.53434');
}
setupRubiconTargeting("dart_rect");
if (gpt_request.yieldbot && $jq.inArray('dart_rect', gpt_request.yieldbot_slots) >= 0) {
try {
yieldbot.setSlotTargeting('dart_rect', adSlot);
if (gpt_request.staging) {
console.log('yieldbot targeting set for dart_rect');
}
}
catch (e) {
if (gpt_request.staging) {
console.log('yieldbot exception: ' + e);
yieldBotNotReady('yieldbot-targeting');
}
}
}
if (gpt_request.amazon) {
try {
amznads.setTargetingForGPTAsync('amznslots');
}
catch(e) { /*ignore*/}
}
if(gpt_request.criteo && crtg_content) {
var crtg_split = crtg_content.split(';');
for (var ii=1;ii < crtg_split.length;ii++)
{
googletag.pubads().setTargeting("" + (crtg_split[ii-1].split('='))[0] + "", "" + (crtg_split[ii-1].split('='))[1] + "");
}
topix_adinfo += ":At";
}
if (gpt_request.aol) {
try {
gpt_setAolTargeting();
}
catch(e) {
if(gpt_request.staging) {
console.log('aol exception');
}
logAdEvent("aol-exception");
}
}
if (gpt_request.exclusionCategories != null) {
for ( var jj = 0; jj < gpt_request.exclusionCategories.length; jj++) {
googletag.pubads().setCategoryExclusion(gpt_request.exclusionCategories[jj]);
}
}
googletag.pubads().addEventListener('slotRenderEnded', function(event) {
if (!gpt_request.firstAdRendered) {
gpt_request.firstAdRendered = true;
if (gpt_request.pubmatic) {
var gptElapsed = (new Date()).getTime() - startGPTTime;
tpx_sendLoadStat("elapsed-ads-pubmatic-gpt", gptElapsed);
try {
gpt_parsePubmaticDesktopTargeting();
}
catch (e) {
logAdEvent("pubmatic-exception");
}
}
if(gpt_request.openx_bidder)
{
try {
gpt_parseOpenXTargeting()
}
catch (e) {
logAdEvent("openx-exception");
}
}
}
});
if(window.innerWidth >= 1280)
{
googletag.pubads().setTargeting("screen", "wide");
}
// 7/25/2014 - Turning off SRA to see if that fixes defaults
// 4/7/2015 - turnin on for Adobe Roadblock
googletag.pubads().enableSingleRequest();
googletag.enableServices();
});
</script>
