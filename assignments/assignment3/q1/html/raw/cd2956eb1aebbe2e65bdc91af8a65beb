var start_time;
var topix_adinfo;
var topix_inst_adinfo;
var d = new Date();
var pv_i = new Image(1,1);
var cplparam = "";
if (start_time > 0) {
var cpltime = (new Date()).getTime() - start_time;
if (cpltime > 1000000)
cplparam = "";
else
cplparam = "&ct=" + cpltime;
}
topix_adinfo += topix_inst_adinfo;
var aiparam = "";
