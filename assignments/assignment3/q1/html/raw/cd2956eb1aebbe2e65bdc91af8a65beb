<script type="text/javascript">
var tpx_pageTopTime = (new Date()).getTime();
var tpx_loadStatRandom = Math.random();
var gpt_request = {};
gpt_request.tinyua = "UN";
gpt_request.isMobile = false; // Mobile browser request desktop site
gpt_request.isTablet = false; // Tablet browser request desktop site
gpt_request.exclusionCategories = null;
gpt_request.gpt_adslots = {};
gpt_request.inside = false;
gpt_request.staging = false;
gpt_request.admin = false;
gpt_request.forceLoadTrackers = false;
gpt_request.doRubicon = false;
gpt_request.debugRubicon = false;
gpt_request.debugCasale = false;
gpt_request.debugPubmatic = false;
gpt_request.amazon = true;
gpt_request.criteo = true;
gpt_request.openx_bidder = true;
gpt_request.pubmatic = true;
gpt_request.aol = true;
gpt_request.useJSONP = false;
gpt_request.sonobi_loaded = true;
gpt_request.yieldbot = false;
gpt_request.yieldbot_slots = ['dart_tower', 'dart_pushdown', 'dart_rect'];
gpt_request.adPositions = [];
gpt_request.adPositions.push('dart_oop');
gpt_request.adPositions.push('dart_pushdown');
gpt_request.adPositions.push('dart_rect');
var pm_pub_id = 79532; // 79532 PubMatic Publisher ID.
function loadPubmatic() {
var src = '//ads.pubmatic.com/AdServer/js/dm_gpt_generic.js';
document.write('<scr' + 'ipt src="' + src + '"></scr' + 'ipt>');
}
var ybotq = ybotq || [];
function loadYieldBot() {
// Loading Yieldbot script tag
var js = document.createElement('script');
js.src = '//cdn.yldbt.com/js/yieldbot.intent.js';
var node = document.getElementsByTagName('script')[0];
node.parentNode.insertBefore(js, node);
}
function setupYieldBotDesktop() {
// Setting up a function to be executed one yieldbot script tag is loaded
// Can never be accidentally fired since only the yieldbot script looks for ybotq
ybotq.push(function () {
var yieldBotId = !gpt_request.isMobile ? 'c7c4' : 'ea1a';
if (gpt_request.staging) {
console.log('yieldbot id: ' + yieldBotId);
}
yieldbot.pub(yieldBotId);
yieldbot.domainName('topix.com');
for (var ii=0; ii < gpt_request.adPositions.length; ii++) {
if ($jq.inArray(gpt_request.adPositions[ii], gpt_request.yieldbot_slots) >= 0) {
if (gpt_request.adPositions[ii] == 'dart_rect') {
yieldbot.defineSlot(gpt_request.adPositions[ii], { sizes: [[300, 250], [300, 600]]});
}
else {
yieldbot.defineSlot(gpt_request.adPositions[ii]);
}
if (gpt_request.staging) {
console.log('yieldbot setup: ' + gpt_request.adPositions[ii]);
}
}
}
yieldbot.enableAsync();
yieldbot.go();
});
}
function yieldBotNotReady(label) {
if (gpt_request.staging) {
console.log("YIELDBOT: " + tpx_log_img.src);
}
logAdEvent(label);
}
function logAdEvent(type, custom1, custom2, custom3) {
var tpx_log_type = "event-" + type;
var tpx_log_cb = parseInt(Math.random() * 1000000000);
var src = "http://stats.topix.com/adbackfill.gif?type=" + tpx_log_type + "&cb=" + tpx_log_cb;
if (custom1) {
src += "&custom1=" + custom1;
}
if (custom2) {
src += "&custom2=" + custom2;
}
if (custom3) {
src += "&custom3=" + custom3;
}
var tpx_log_img = new Image(1,1);
tpx_log_img.src = src;
tpx_log_img.onload = function() { return; };
}
function tpx_sendRTBid(adPos, label, price) {
var url = "http://stats.topix.com/adbid.gif?x=1";
if (adPos)
url += "&adslot=" + adPos;
if (label)
url += "&adlabel=" + label;
if (price)
url += "&adprice=" + price;
var tpx_log_img = new Image(1,1);
tpx_log_img.src = url;
tpx_log_img.onload = function() { return; };
}
function tpx_sendLoadStat(type, elapsed) {
if(!tpx_pageTopTime)
return;
// just log 5% of hits for now
if (tpx_loadStatRandom > 0.05 && !gpt_request.forceLoadTrackers)
return;
if (typeof elapsed == "undefined") {
elapsed = (new Date()).getTime() - st_pageTopTime;
}
if (elapsed > 60000) {
elapsed = 60000;
}
var url = "http://stats.topix.com/load.gif?x=1";
url = url + "&type=" + encodeURIComponent(type);
url = url + "&elapsed=" + elapsed;
tpx_reqTrackerImage(url);
if (gpt_request.forceLoadTrackers) {
console.log("ELAPSED (" + type +"): " + elapsed);
}
}
function tpx_reqTrackerImage(url) {
var img = new Image();
img.src = url;
img.onload = function() {};
return;
}
gpt_request.sizeToSlotMapping = {
"728x90" : 1,
"160x600" : 2,
"300x250" : 6,
"300x600" : 6
};
gpt_request.slotToPosition = {
1: "dart_pushdown",
2: "dart_tower",
6: "dart_rect"
};
gpt_request.aolBids = gpt_request.aolBids || {};
// Stores bids in gpt_request
function gpt_storeAolBid(bidData){
for (var ii = 0; ii < bidData.seatbid.length; ii++) {
var bidArr = bidData.seatbid[ii].bid;
for (var jj = 0; jj < bidArr.length; jj++) {
var curBid = bidArr[jj];
var adSize = curBid.w + "x" + curBid.h;
var adSlotIdx = gpt_request.sizeToSlotMapping[adSize];
var price = curBid.price;
var priceBand = formatNum10000(Math.floor(price * 10) * 10);
tpx_sendRTBid(adSlotIdx, "aol_" + adSize + "_" + priceBand, price);
if (gpt_request.staging) {
console.log("storing :" + adSize+ " price:" + price);
}
if (gpt_request.aolBids[adSize] && gpt_request.aolBids[adPos].price > price) {
continue;
}
var bidInfo = {
size: adSize,
price: price,
adm: curBid.adm,
};
gpt_request.aolBids[adSize] = bidInfo;
}
}
var aolElapsed = (new Date()).getTime() - startAolTime;
if (gpt_request.staging) {
console.log("Aol request elapsed time: " + aolElapsed);
}
tpx_sendLoadStat("elapsed-ads-tpx-aol", aolElapsed);
}
var gpt_aolAdPositions = [ "728x90", "300x250", "300x600", "160x600" ];
var gpt_aolUrl = {
"cors_suffix" : "cors=yes;",
"jsonp_suffix" : "callback=gpt_storeAolBid",
"728x90": "http://adserver.adtechus.com/pubapi/3.0/9897.1/3524658/0/225/;noperf=1;alias=;cmd=bid;",
"160x600": "http://adserver.adtechus.com/pubapi/3.0/9897.1/3586935/0/154/;noperf=1;alias=;cmd=bid;", // 160x600
"300x250": "http://adserver.adtechus.com/pubapi/3.0/9897.1/3524659/0/170/;noperf=1;alias=;cmd=bid;", // 300x250
"300x600": "http://adserver.adtechus.com/pubapi/3.0/9897.1/3524657/0/529/;noperf=1;alias=;cmd=bid;" // 300x600
}
// Check gpt_request for bids, sends them to DFP
function gpt_setAolTargeting(bid) {
for (var ii = 0; ii < gpt_aolAdPositions.length; ii++) {
var adPos = gpt_aolAdPositions[ii];
var bid = gpt_request.aolBids[adPos];
var slotIdx = gpt_request.sizeToSlotMapping[adPos];
var slotLabel = gpt_request.slotToPosition[slotIdx];
var adslot = gpt_request.gpt_adslots[slotLabel];
if(typeof bid != 'undefined' && typeof adslot != 'undefined') {
adslot.setTargeting('aolbid', bid.price);
}
}
}
// Used by DFP callback
function gpt_getAolAd(adPos) {
try {
var bid = gpt_request.aolBids[adPos];
if (typeof bid == 'undefined') {
if (gpt_request.staging) {
console.log("No aol bid object found for: " + adSize);
}
logAdEvent("aol-exception-" + adPos);
bid = null;
}
return bid;
}
catch (e) {
if (gpt_request.staging) {
alert(e);
}
logAdEvent("aol-exception");
return null;
}
}
function loadAolViaJSONP() {
for (var ii = 0; ii < gpt_aolAdPositions.length; ii++) {
var adPos = gpt_aolAdPositions[ii];
var url = gpt_aolUrl[adPos] + gpt_aolUrl['jsonp_suffix'];
var js = document.createElement('script');
js.src = url;
var node = document.getElementsByTagName('script')[0];
node.parentNode.insertBefore(js, node);
}
}
function loadAolViaCORS() {
$jq.support.cors = true; // need for IE cors to work
for (var ii = 0; ii < gpt_aolAdPositions.length; ii++) {
var adPos = gpt_aolAdPositions[ii];
var url = gpt_aolUrl[adPos] + gpt_aolUrl['cors_suffix'];
$jq.ajax({
url: url,
type: 'GET',
dataType: 'json',
crossDomain: true,
xhrFields: {
withCredentials: true
},
success: function(result) {
gpt_storeAolBid(result);
},
error: function (request, status, error) {
if (gpt_request.staging) {
console.log('CORS error: ' + error);
}
logAdEvent("aol-CORS-error", error, status, gpt_request.tinyua);
}
});
}
}
if (gpt_request.aol) {
var startAolTime = (new Date()).getTime();
if (gpt_request.useJSONP) {
loadAolViaJSONP();
}
else {
loadAolViaCORS();
}
}
if (gpt_request.pubmatic) {
loadPubmatic();
}
if (gpt_request.yieldbot) {
loadYieldBot();
setupYieldBotDesktop();
}
// According to setKeys() in morpheus.topix.js, targeting is set at the slot level
var gpt_pubmaticAdPositions = [ "dart_pushdown", "dart_tower", "dart_rect" ];
function gpt_parsePubmaticDesktopTargeting() {
if (gpt_request.staging) {
console.log('pubmatic targeting');
}
var key = "bid";
for (var ii=0; ii < gpt_pubmaticAdPositions.length; ii++) {
if($jq.inArray(gpt_pubmaticAdPositions[ii], gpt_request.adPositions) < 1) {
if(gpt_request.staging) {
console.log("Pubmatic size error for " + gpt_pubmaticAdPositions[ii]);
}
continue;
}
if(typeof gpt_request.gpt_adslots[gpt_pubmaticAdPositions[ii]] == 'undefined') {
if(gpt_request.staging) {
console.log("Skipping undefined slot " + gpt_pubmaticAdPositions[ii]);
}
continue;
}
var pubmaticTargetingArr = gpt_request.gpt_adslots[gpt_pubmaticAdPositions[ii]].getTargeting(key);
if (pubmaticTargetingArr && pubmaticTargetingArr.length > 0) {
for(var jj=0; jj < pubmaticTargetingArr.length; jj++) {
var price = pubmaticTargetingArr[jj];
if (price == '0' || price == 0) // skip zero values
continue;
if (gpt_request.inside && price < 0)
console.log("Pubmatic pricing error for " + price + " size: " + gpt_pubmaticAdPositions[ii]);
var priceBand = formatNum10000(Math.floor(price * 10) * 10);
tpx_sendRTBid(gpt_pubmaticAdPositions[ii], 'tpx_pubmatic_' + priceBand, price);
if (gpt_request.staging) {
console.log("Pushing Pubmatic " + price + " size: " + gpt_pubmaticAdPositions[ii] + " into DFP");
}
}
}
}
}
var gpt_bidderAdPositions = [ "dart_pushdown", "dart_tower", "dart_rect" ];
var gpt_bidderAdSizes = { "dart_pushdown":"728x90", "dart_tower":"160x600", "dart_rect":"300x250" };
function gpt_parseOpenXTargeting () {
if (gpt_request.staging) {
console.log('openx targeting');
}
var prefix = "ox";
for (var ii=0; ii < gpt_bidderAdPositions.length; ii++) {
if($jq.inArray(gpt_bidderAdPositions[ii], gpt_request.adPositions) < 1) {
if(gpt_request.staging) {
console.log("OpenX size error for " + gpt_bidderAdPositions[ii]);
}
continue;
}
if(typeof gpt_request.gpt_adslots[gpt_bidderAdPositions[ii]] == 'undefined') {
if(gpt_request.staging) {
console.log("Skipping undefined slot for size " + gpt_bidderAdPositions[ii]);
}
continue;
}
var key = prefix + gpt_bidderAdSizes[gpt_bidderAdPositions[ii]];
var bidderTargetingArr = gpt_request.gpt_adslots[gpt_bidderAdPositions[ii]].getTargeting(key);
if (bidderTargetingArr && bidderTargetingArr.length > 0) {
for(var jj=0; jj < bidderTargetingArr.length; jj++) {
var price = bidderTargetingArr[jj];
// skip zero price values
// 's' and 't' which I don't know what openx is using for, but nothing in DFP targets them
// waiting on their reply
if (price == '0' || price == 't' || price == 's' || price == 0) {
continue;
}
if (gpt_request.inside && price < 0)
console.log("OpenX pricing error for " + price + " size: " + gpt_bidderAdPositions[ii]);
var priceBand = formatNum10000(price);
price /= 100;
tpx_sendRTBid(gpt_pubmaticAdPositions[ii], 'tpx_openx_' + priceBand, price);
if (gpt_request.staging) {
console.log("Pushing OpenX " + price + " size: " + gpt_bidderAdPositions[ii] + " into DFP");
}
}
}
}
}
function formatNum10000(num) {
if (num < 10)
return "000" + num;
if (num < 100)
return "00" + num;
if (num < 1000)
return "0" + num;
return num;
}
