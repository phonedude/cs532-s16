<script type="text/javascript">
function setupRubiconTargeting (position) {
var rubicon_size_mapping = {
"dart_lb_header": ["728x90"],
"dart_pushdown": ["728x90"],
"dart_rect": ["300x600", "300x250"],
"dart_tower": ["160x600"]
};
if (typeof position == 'undefined' || typeof rubicon_size_mapping[position] == 'undefined') {
if(gpt_request.debugRubicon)
console.log("rubicon position: [" + position + "] not found");
return true;
}
var rtp_sizes = rubicon_size_mapping[position];
for(var ii = 0; ii < rtp_sizes.length; ii++) {
if (rubicon_rtp[rtp_sizes[ii]])
adSlot.setTargeting("tier", rubicon_rtp[rtp_sizes[ii]].tier);
}
return false;
}
</script>
