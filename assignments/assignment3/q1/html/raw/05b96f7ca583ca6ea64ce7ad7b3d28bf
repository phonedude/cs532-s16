<div class="copy post-body" itemprop="text" data-swiftype-index="true">

<aside class="meta-social" data-swiftype-index="false">
	<div class="social-share">
	   <ul class="social-button inline-list"></ul>
        </div>
<div class="clear"></div>
<script>
$(function(){
    	$('.social-button').popnSocialButton([ 'facebook', 'twitter', 'hatebu', 'gplus', 'pocket', 'linebu'], {
