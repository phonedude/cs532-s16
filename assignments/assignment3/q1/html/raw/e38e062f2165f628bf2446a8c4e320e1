<script>
$(document).ready(function(e) {
	
	var timer = 1500;
	var optin_animation;
	var isOptinClosed = false;
	
	var isHidden = getCookie('wsd_optin');
	
	if(isHidden == null) {
		document.cookie="wsd_optin=show; expires=Thu, 18 Dec 2020 12:00:00 UTC; path=/";
		isHidden = 'show';
	}
	
	if(isHidden == 'hide') {
		closeOptin();
		setTimeout(function() {
			$('#optin_form').css('display', 'block');
		}, 2500);
	}
	else if(isHidden == 'show') {
		
		$( "#optin_form #optin_error" ).css('bottom', '-210px');
		$( "#optin_form #container" ).css('bottom', '-210px');
		setTimeout(function() {
			$('#optin_form').css('display', 'block');
			openOptin();
		}, 2500);
	}
	
	$('.close_optin').click(function() {
		
		if(isOptinClosed) {
			document.cookie="wsd_optin=show; expires=Thu, 18 Dec 2020 12:00:00 UTC; path=/";
			openOptin();
		}
		else {
			document.cookie="wsd_optin=hide; expires=Thu, 18 Dec 2020 12:00:00 UTC; path=/";
			closeOptin();
		}
	});
	
	function getCookie(cookie_name) {
		
		c = document.cookie.split('; ');
        cookies = {};

        for(i=c.length-1; i>=0; i--){
           C = c[i].split('=');
           cookies[C[0]] = C[1];
        }
		
		return cookies[cookie_name];
	}
	
	function closeOptin() {
		
		$( "#optin_form #optin_error" ).animate({
			bottom: "-210px"
		}, timer );
		
		$( "#optin_form #container" ).animate({
			bottom: "-210px"
		}, timer );
		
		isOptinClosed = true;
		$('#optin_form #error_tags').fadeOut();
		$('#optin_form .close_optin img').attr('src', 'http://media.wallstreetdaily.com/website/banners/crawl_up/Show.png');
		
	}
	
	function openOptin() {
		
		$( "#optin_form #optin_error" ).animate({
			bottom: "0px"
		}, timer );
		
		$( "#optin_form #container" ).animate({
			bottom: "0px"
		}, timer );
		
		isOptinClosed = false;
		$('#optin_form #error_tags').fadeIn();
		$('#optin_form .close_optin img').attr('src', 'http://media.wallstreetdaily.com/website/banners/crawl_up/Hide.png');
	}
});

</script>


