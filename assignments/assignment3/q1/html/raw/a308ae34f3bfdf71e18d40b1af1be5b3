<script>
  $(document).ready(function() {

    $(".img-desktop .close").click(function(){
      // $('.fixed').css('visibility', 'hidden');
      // $('.footer-expand').css('visibility', 'hidden');
      $('.fix-desk').css('visibility', 'hidden');
    });

    $(".img-tablet .close").click(function(){
      $('.img-tablet').css('visibility', 'hidden');
    });

    $(".footer-expand .close").click(function(){
      // $('.footer-expand').css('transform', 'translateY(340px)');
      // $('.footer-expand').css('visibility', 'hidden');
      $('.footer-expand').remove();
    });

    $(".img-mobile .close").click(function(){
      $('.img-mobile').css('visibility', 'hidden');
    });

  });

  $(window).load(function() {
    var heightAdvFooterMob = $('.img-mobile').height();
    if(heightAdvFooterMob == 0) {
      // $('.adv-footer').css('height', '0');
      $('.img-mobile').remove();
    } else {
      $('.img-mobile').css('height', heightAdvFooterMob);
    };
    var heightAdvFooterTab = $('.img-tablet').height();
    if(heightAdvFooterTab == 0) {
      // $('.adv-footer').css('height', '0');
      $('.img-tablet').remove();
    } else {
      $('.img-tablet').css('height', heightAdvFooterTab);
    };
    var heightAdvFooterDek = $('.img-desktop').height();
    if(heightAdvFooterDek == 0) {
      $('.fix-desk').css('display', 'none');
      $('.footer-expand').remove();
    } else {
      $('.img-desktop').css('height', heightAdvFooterDek);
    };
    var heightAdvFooterExp = $('.footer-expand').height();
    if(heightAdvFooterExp !== 0) {
      $('.footer-expand').css('height', heightAdvFooterExp);
    } else {
      $('.footer-expand').remove();
    };

    $(window).scroll(function(){
      if($('.header-menu #undefined-sticky-wrapper').hasClass('is-sticky'))
      {
        $('.footer-expand').css('transform', 'translateY(5px)');
      };
    });

  })
</script>

<!-- END SCRIPT STICKY FOOTER -->

