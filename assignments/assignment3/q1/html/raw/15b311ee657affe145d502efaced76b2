<script>
        var conn = new WebSocket('wss://websocket.betmoose.com/');
    </script>
<script>
    $(document).ready(function(){
        initialChatWindow();
        $('#chat-input').val('');
    
        var t;
        conn.onopen = function(e) {
            console.log("Connection established!");

            function sendMsg() {
                var msg = {type: 'msg', token: $('#chat').val(), msg: $('#chat-input').val()};
                conn.send(JSON.stringify(msg));
                $('#chat-input').val('');
                $('#chat-submit').removeClass('chat-live');
            }
            
            $("#chat-input").on("input", function() {
                if ($(this).val()=='') 
                {
                    if($('#chat-submit').hasClass('chat-live')) {
                        $('#chat-submit').removeClass('chat-live');   
                    }
                }
                else{
                    if($('#chat-submit').hasClass('chat-live')==false) {
                        $('#chat-submit').addClass('chat-live');   
                    }
                }
            });
            
            $('#chat-submit').click(function()
            {
                if($(this).hasClass('chat-live'))
                {
                    sendMsg();
                }
            });
            
            $('#chat-tab').click(function(){
                if ($('#mute').is(':hover')) { toggleMute(); }
                else {toggleChatWindow($(this));}
            });
        
            $('#chat-content').mouseover(function(event) {
                $(this).focus();
            });

            $('#chat-input').keyup(function(e){
                if (e.keyCode == 13 && !e.shiftKey) {
                    sendMsg();
                }
            });
        };
    
        conn.onmessage = function(e) {
            msg = jQuery.parseJSON(e.data);
            if(msg['type'] == 'msg')
            {
                $('#chat-content').append('<div class="message"><p><img src="' + msg['flagUrl'] + '" title="From ' + msg['country'] + '"/><a href="/profile/' + msg['profileLink'] + '">' + msg['user'] + '</a> <span>' + msg['timestamp'] + '</span></p><p>' + msg['msg'] + '</p></div>');
                $('#chat-content').scrollTop($('#chat-content')[0].scrollHeight);
                if(!$('#chat-tab').hasClass('open'))
                {
                    var chat_mute = store.get('chat_mute');
                    if(chat_mute != '' && chat_mute == 'false')
                    {
                        var ping = $('audio')[0]; 
                        ping.play();
                    }
                    
                    $('#missedMessages').html(parseInt($('#missedMessages').html()) + 1);
                    if (!$('#missedMessages').hasClass('live')) { $('#missedMessages').addClass('live'); }
                    $( "#missedMessages" ).animate({"bottom": "+=20px" }, 400, function() { $( "#missedMessages" ).animate({"bottom": "-=20px" }, 400)  } );
                }
            } else if (msg['type'] == 'error')
            {
                $('#chat-content').append('<div class="message"><p>' + msg['msg'] + '</p></div>');
                $('#chat-content').scrollTop($('#chat-content')[0].scrollHeight);
            }
        };
    });
    
    function toggleMute() {
        if ($('#mute').hasClass('fa-volume-up')) {
            $('#mute').addClass('fa-volume-off').removeClass('fa-volume-up');
            store.set('chat_mute', 'true');
        } else {
            $('#mute').removeClass('fa-volume-off').addClass('fa-volume-up');
            store.set('chat_mute', 'false');
        }
    }

    function toggleChatWindow(chatWindow)
    {
        var chat_on = store.get('chat_position');
        if(chat_on != '' && chat_on == 'open')
        {
            $('#chat-wrap').removeAttr('style');
            chatWindow.removeClass('open');
            store.set('chat_position', 'closed');
        }
        else
        {
            $('#chat-wrap').css('bottom','0');
            $('#chat-content').scrollTop($('#chat-content')[0].scrollHeight);
            chatWindow.addClass('open');
            store.set('chat_position', 'open');
            $('#missedMessages').html(0).removeClass('live');
        }
    }

    function initialChatWindow()
    {
        var chat_on = store.get('chat_position');
        if(chat_on != '' && chat_on == 'open')
        {
            $('#chat-tab').addClass('open');
            $('#chat-wrap').css('bottom','0').show();
            $('#chat-content').scrollTop($('#chat-content')[0].scrollHeight);
        }
        else
        {
            $('#chat-tab').removeClass('open');
            $('#chat-wrap').removeAttr('style').show();
        }

        var chat_mute = store.get('chat_mute');
        if(chat_mute != '' && chat_mute == 'true')
        {
            $('#mute').addClass('fa-volume-off').removeClass('fa-volume-up');
            store.set('chat_mute', 'true');
        }
        else
        {
            $('#mute').removeClass('fa-volume-off').addClass('fa-volume-up');
            store.set('chat_mute', 'false');
        }
    }
</script> </div>
</div>
