<script>
    $(document).ready(function(){
        try{
            var conn = new ab.Session(loc,
                function() {
                    /*
                    if($("#chat").length != 0 && $('#chat').val().length > 0) {
                        conn.subscribe('User/' + $("#chat").val(), function(topic, data) {
                        });
                    }
                    */

                    if($("#push-subscription").length != 0) {
                        conn.subscribe($("#push-subscription").val(), function(subscription, data) {
                            data = jQuery.parseJSON(data);
                            parseSubscriptionPacket(data);
                        });
                    }
                },
                function() {
                    function show_disconnect()
                    {
                        console.warn('Push server connection closed');
                        $('#popup-box').append('<div id="connection-closed" class="popup-alert popup-red"><p>The notification server has disconnected, site elements may not update in real time. Try refreshing the page.</p></div>');
                        //$('#connection-closed').show().delay( 10000 ).fadeOut( 5000 );
                    };
                    window.setTimeout( show_disconnect, 5000 );
                },
                {'skipSubprotocolCheck': true}        
            );
        } catch(e)
        {
            console.warn('Push server connection failed');
            $('#popup-box').append('<div id="connection-failed" class="popup-alert popup-red"><p>The notification server was unreachable, site elements will not update in real time. Try refreshing the page.</p></div>');
            //$('#connection-closed').show().delay( 10000 ).fadeOut( 5000 );
        }
    });
</script>
<script type="text/javascript">
    function parseSubscriptionPacket(data)
    {
        var data = data.data;
        if(data.packetType == 'PariBet')
        {
            if($('#pToken').val() != data.pToken) // Don't double update.
            {
                if($('#no-bets-placed').length != 0)
                {
                    $('#no-bets-placed').hide();
                    $('#placed-bets-table').show();
                }
                $('#placed-bets-table > tbody > tr:first').after('<tr><td class="placed-time">' + data.time + '</td>' 
                    + data.user 
                    + data.optionName 
                    + '<td class="placed-amount"><i class="fa fa-btc"></i>' + data.amount + '</td>'
                    + data.time_weight
                    + data.payout 
                    + '</tr>');
               $('#placed-bets-table > tbody > tr').eq(1).css('background','rgba(255, 244, 162,1)').animate({
                        backgroundColor: 'rgba(255, 244, 162,0)'
                    }, 4000);
                $('.option' + data.optionPosition + '-vol').html(data.optionVolume).flash( '255,215,0', 1000 );
                $('#option' + data.optionPosition + '-numbets').html(data.optionNum).flash( '255,215,0', 1000 );
                $('#total-volume-bets').html(data.totalVolume).flash( '255,215,0', 1000 );

                data.optionData.forEach(function(entry)
                {
                    $('#' + entry.bar + '-overlay').attr('title', entry.title);
                    $('#' + entry.bar).animate({ width: entry.width + '%' });
                });
            }
        } else if(data.packetType == 'FixedBet')
        {
            if($('#pToken').val() != data.pToken) // Don't double update.
            {
                if($('#no-bets-placed').length != 0)
                {
                    $('#no-bets-placed').hide();
                    $('#placed-bets-table').slideDown();
                }
                $('#placed-bets-table > tbody > tr:first').after('<tr><td class="placed-time">' + data.time + '</td>' 
                    + data.user 
                    + data.option 
                    + '<td class="placed-amount"><i class="fa fa-btc"></i>' + data.amount + '</td>'
                    + '<td class="placed-mod">' + data.odds + '</td>'
                    + '<td class="placed-payout"><span><i class="fa fa-btc"></i>' + data.payout + '</span></td>'
                    + '</tr>');

                $('#option' + data.optionPosition + '-slipColumns').html(data.slipColumns).flash( '255,215,0', 1000 );
                $('#bucket' + data.optionPosition + 'odds').html(data.bestOdds).flash( '255,215,0', 1000 );
                
                $('#bucket' + data.optionPosition).attr('title', data.newHoverover);
                if(data.newBucketFill <= 0.01)
                {
                    $('#bucket' + data.optionPosition + 'fill').animate({ paddingTop: '0%' });
                    $('#bucket' + data.optionPosition + 'fill').hide();
                } else {
                    $('#bucket' + data.optionPosition + 'fill').show().animate({ paddingTop: data.newBucketFill + '%' });
                }
                
                $('#total-volume').html(data.totalVolume).flash( '255,215,0', 1000 );
                
                var allOffers = JSON.stringify(data.allOffersJSON);
                $('#offersJSON').val(allOffers);

                $('#outdated-warning').show();
            }
        } else if(data.packetType == 'LayBet')
        {
            if($('#pToken').val() != data.pToken) // Don't double update.
            {
                $('#option' + data.optionPosition + '-slipColumns').html(data.slipColumns).flash( '255,215,0', 1000 );
                $('#bucket' + data.optionPosition + 'odds').html(data.bestOdds).flash( '255,215,0', 1000 );
                
                $('#bucket' + data.optionPosition).attr('title', data.newHoverover);
                if(data.newBucketFill <= 0.01)
                {
                    $('#bucket' + data.optionPosition + 'fill').animate({ paddingTop: '0%' });
                    $('#bucket' + data.optionPosition + 'fill').hide();
                } else {
                    $('#bucket' + data.optionPosition + 'fill').show().animate({ paddingTop: data.newBucketFill + '%' });
                }

                $('#offered-volume').html(data.offeredVolume).flash( '255,215,0', 1000 );
                
                var allOffers = JSON.stringify(data.allOffersJSON);
                $('#offersJSON').val(allOffers);
            }
        } else if(data.packetType == 'EditLay')
        {
            if($('#pToken').val() != data.pToken) // Don't double update.
            {
                $('#option' + data.optionPosition + '-slipColumns').html(data.slipColumns).flash( '255,215,0', 1000 );
                $('#bucket' + data.optionPosition + 'odds').html(data.bestOdds).flash( '255,215,0', 1000 );
                
                $('#bucket' + data.optionPosition).attr('title', data.newHoverover);
                if(data.newBucketFill <= 0.01)
                {
                    $('#bucket' + data.optionPosition + 'fill').animate({ paddingTop: '0%' });
                    $('#bucket' + data.optionPosition + 'fill').hide();
                } else {
                    $('#bucket' + data.optionPosition + 'fill').show().animate({ paddingTop: data.newBucketFill + '%' });
                }
                
                $('#offered-volume').html(data.offeredVolume).flash( '255,215,0', 1000 );

                var allOffers = JSON.stringify(data.allOffersJSON);
                $('#offersJSON').val(allOffers);
            }
        } else if(data.packetType == 'Ticker/Bitcoin')
        {
            var nextP = [data.timestamp, data.value_usd];
            $('#bitcoinTickerNextPoint').val(JSON.stringify(nextP));
        }
    }
</script> <div id='popup-box'>
