<script>
        $('.bet_info_option_bet').click(function(){ //clicking place bet while not logged in
            var n = $(this).prop('name');
            var index = $( ".bet_info_option_bet" ).index( this );
            if ($('div[name='+n+']').is(':visible')) {
                var title = $('.option-title').eq(index).text();
                var c = $('.bet_info_option_box').eq(index).css('background-color');
        
                $('#anon-box-content').css('height','235px');
                $('#remove-later').fadeTo('fast', 1).slideDown();
                $('.turn-on').prop('checked',false);
                $('#anon-address').val('');
                $('#anon-option-id').val($('.option-id').eq(index).val());
                if ($('#anon-proceed').hasClass('button-disabled')){}else{$('#anon-proceed').addClass('button-disabled');}
                $('#anon-title').text(title);
                $('#anon-choice').css('background-color',c);
                $('#anon-box').fadeIn(300);   
            }
            else
            {
                $('div[name='+n+']').slideDown(200); 
                $(this).text('Place Anonymously').addClass('biob-green');
                $('.option-title').eq(index).addClass('option-title-click');
            }
        });
        
        $('body').on('click','.option-title',function (e) {
            if ($(this).hasClass('no-click')==false)
            {
                var index = $( ".option-title" ).index( this );
                if ($(this).hasClass('option-title-click'))
                {
                    $('.option-open').eq(index).slideDown(200);
                    $('.hider').eq(index).slideUp(200);
                    $('.flip-container').eq(index).hide();
                    $(this).removeClass('option-title-click');
                }
                else
                {
                    $('.option-open').eq(index).hide();
                    $('.hider').eq(index).slideDown(200);
                    $('.bet-amount').eq(index).focus();
                    $('.flip-container').eq(index).show();
                    $('.bet_info_option_bet').eq(index).text('Place Anonymously').addClass('biob-green');
                    $(this).addClass('option-title-click');
                } 
            }
        });
        
        $('body').on('click','.option-open',function (e) {
            var index = $( ".option-open" ).index( this );
            $('.option-title').eq(index).click();
        });
        
        $('#anon-box-content h3 span img').click(function() { //close button clicked
            $('#anon-box').fadeOut(300);
        });
        
        $('#anon-done').click(function() {
            $('#anon-box').fadeOut(300);
        });
        
        $('#anon-box').click(function(e) { //click on the darkened area to close
            var target = $( e.target );
            if (target.is( "#anon-box")) {$('#anon-box').fadeOut(300);}
        });
        
        $('.turn-on').change(function(){
            $('.button-disabled').removeClass('button-disabled'); 
        });
        $('#anon-address').change(function(){
            $('.button-disabled').removeClass('button-disabled'); 
        });
        
        $('#anon-proceed').click(function(){          
            if ($(this).hasClass('button-disabled')){}
            else
            { 
                $('#anon-proceed').addClass('button-disabled');
                $('#anon-proceed').text('Loading...');

                var referrer = getCookie('referrer');

                var url = '';
                if(referrer != '')
                {
                    url = '/ajax/generateAnonAddress?optionId=' + $('#anon-option-id').val() + '&mode=' + $('#this-address').is(':checked') + '&address=' + $('#anon-address').val() + '&referrer=' + referrer;
                } else {
                    url = '/ajax/generateAnonAddress?optionId=' + $('#anon-option-id').val() + '&mode=' + $('#this-address').is(':checked') + '&address=' + $('#anon-address').val();
                }

                $.get(url, function( data )
                {
                    if(data == '-1')
                    {
                        // Print an error, too many addresses generated.
                        alert("Anonymous bettors are limited to 30 bet addresses per day. Please sign up for more - it's free!");
                        $('#anon-proceed').text('Proceed');
                    } else if (data == '-2')
                    {
                        // Print an error, node failure.
                        alert('Unable to contact node, please try again.')
                        $('#anon-proceed').text('Proceed');
                    } else if (data == '-3')
                    {
                        // Print an error, some of the data sent is invalid.
                        alert('Some of the data was invalid, please try again.')
                        $('#anon-proceed').text('Proceed');
                    } else {
                        
                        $('#anon-send-address').empty().html(data);
                        var a = document.getElementById('qr-test');
                        var g = $('#gen');
                        var ctx = g[0].getContext('2d');
                        ctx.clearRect(0,0,150,150);
                        g.qrcode({
                            render: 'canvas',
                            size:150,
                            left:0,
                            mode:4,
                            mSize: 0.32,
                            mPosX: 0.5,
                            mPosY: 0.5,
                            image: a,
                            color: '#333',
                            text: 'bitcoin:'+data,
                            ecLevel: 'H'
                        });
                        
                        $('#remove-later').fadeTo('slow', 0).slideUp(); 
                        $('#anon-box-content').css('height','450px'); 
                    }
                });
            } 
        });
    </script>
