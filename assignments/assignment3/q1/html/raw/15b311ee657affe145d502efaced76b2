<script>
        var split = location.search.replace('?', '').split('&').map(function(val){
            return val.split('=');
        });

        var referrer = '';
        for (index = 0; index < split.length; ++index) {
            if(split[index][0] == 'ref')
            {
                referrer = split[index][1];
                break;
            }
        }

        if(referrer != '')
        {
            setCookie('referrer', referrer, 1);
        }
    </script>
