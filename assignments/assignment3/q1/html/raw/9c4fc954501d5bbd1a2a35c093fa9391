    <script type="text/javascript">//更改字級大小
        function doZoom(size) {
            size=size*1;
            $('#smaller').removeClass();
            $('#middle').removeClass();
            $('#bigger').removeClass();
            $('#smaller').addClass('smaller overcooked');
            $('#middle').addClass('middle overcooked');
            $('#bigger').addClass('bigger overcooked');
            switch(size){
                case 16:
                    $('#smaller').addClass('smon');
                    break;
                case 18:
                    $('#middle').addClass('mion');
                    break;
                case 20:
                    $('#bigger').addClass('bion');
                    break;
            }
            document.cookie="FRONTSIZE="+size+"; path=/";
            document.getElementById('h1').style.fontSize=(size+8)+'px';
            document.getElementById('summary').style.fontSize=size+'px';
            document.getElementById("caption0").style.fontSize=size+"px";document.getElementById("caption1").style.fontSize=size+"px";document.getElementById("caption2").style.fontSize=size+"px";document.getElementById("caption3").style.fontSize=size+"px";document.getElementById("caption4").style.fontSize=size+"px";        }

        function changePage(SeoSec,IssueID,art_id,page){
            $.ajax({
                url: '/realtimenews/ajaxarticle/'+SeoSec+'/'+IssueID+'/'+art_id+'/'+page+'/201602190248',
                dataType: 'html',
                type:"GET",
                error: function(xhr) {
                },
                success: function(response) {
                    $('#realtimelist').html(response);
                }
            });
          }

          function getCookieData(name) {
             var patrn = new RegExp( "^" + name + "=(.*?);" ),
                   patr2 = new RegExp( " " + name + "=(.*?);" );
             if ( match = (document.cookie.match(patrn) || document.cookie.match(patr2)) )
                   return match[1];
             return false;
          }

          $(document).ready(function() {
              fontsize = getCookieData('FRONTSIZE');
              if(fontsize) {
                    doZoom(fontsize);
                }
                else {
                    doZoom(18);
                }
                changePage('international',20140921,473643,1);
                $('.lbimg a').lightBox();
                $.ajax({
                            url: 'http://weblogging.appledaily.com.tw/api/SetClick/r/v/473643/103/0/20140921/realtimenews/'+Math.random(),
                            dataType: 'jsonp',
                            success: function(data) {}
                        });
                          });
          
