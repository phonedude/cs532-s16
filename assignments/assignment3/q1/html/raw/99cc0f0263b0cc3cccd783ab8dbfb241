    <div class="row-fluid">
        <!-- This article call will be used for all standard data -->
                <div class="span4">
            <article id="main-story" class="full-story">
                <div class="body">
                        <div class="article">  
                            <div class="hnews hentry item">
                                <header id="article-top">
                                    <!-- built by /sports/20140919/quarterback-morgan-roberts-ready-to-lead-yale on 20160213020805 -->
<ul class="breadcrumb block small hide-for-phone">
    <li><a href="http://www.nhregister.com">Home</a><span class="divider"> →</span></li>
    <li><a href="/sports">Sports</a><span class="divider"> →</span></li>
    <li class="active">Yale</li>
