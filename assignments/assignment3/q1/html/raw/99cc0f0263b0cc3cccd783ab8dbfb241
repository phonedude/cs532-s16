    }
     function create_mc_gallery(){
       $.ajax({async:true, url:"http://extras.mnginteractive.com/live/partners/MediaCenter/embedded_galleries/mc_embed_1.0.8.1.js", dataType:'script', timeout:500});
       var mediaCenterCodeLoadTester = self.setInterval(
           function(){
               if(typeof mc_embed_gallery != 'undefined'){
                   mc_embed_gallery.initWidget();
                   window.clearInterval(mediaCenterCodeLoadTester);
               }
           },100
       )
     }
