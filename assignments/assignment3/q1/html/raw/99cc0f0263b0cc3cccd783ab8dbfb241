        var parentCount = parentProfileArray.length;
        /* last item is empty, 2nd last is mainProfileID, 3rd last is parentProfileID */
        var parentProfile = parentProfileArray[parentCount-3];
        /* pull stories for parent profile */
        var most_pop_list = 'New Haven magnet school teacher charged with sexual assault of student~!~/general-news/20160210/new-haven-magnet-school-teacher-charged-with-sexual-assault-of-student~!~http://21C-NHwebvarnish.newscyclecloud.com/apps/pbcsi.dll/storyimage/NH/20160210/NEWS/160219946/AR/0/AR-160219946.jpg&exactW=150&exactH=108~#~West Haven police identify woman, 35, found dead in water~!~/general-news/20160208/west-haven-police-identify-woman-35-found-dead-in-water~!~http://21C-NHwebvarnish.newscyclecloud.com/apps/pbcsi.dll/storyimage/NH/20160208/NEWS/160209599/AR/0/AR-160209599.jpg&exactW=150&exactH=108~#~Hamden man faces murder, arson charges for woman found dead after fire~!~/general-news/20160211/hamden-man-faces-murder-arson-charges-for-woman-found-dead-after-fire~!~http://21C-NHwebvarnish.newscyclecloud.com/apps/pbcsi.dll/storyimage/NH/20160211/NEWS/160219922/AR/0/AR-160219922.jpg&exactW=150&exactH=108~#~School officials want Connecticut to take charge on changing start times~!~/general-news/20160210/school-officials-want-connecticut-to-take-charge-on-changing-start-times~!~http://21C-NHwebvarnish.newscyclecloud.com/apps/pbcsi.dll/storyimage/NH/20160210/NEWS/160219974/AR/0/AR-160219974.jpg&exactW=150&exactH=108~#~New Haven teacher texted explicit photos before alleged sexual assault, affidavit says~!~/general-news/20160211/new-haven-teacher-texted-explicit-photos-before-alleged-sexual-assault-affidavit-says~!~http://21C-NHwebvarnish.newscyclecloud.com/apps/pbcsi.dll/storyimage/NH/20160211/NEWS/160219900/AR/0/AR-160219900.jpg&exactW=150&exactH=108~#~Editorial: First responders deserve a round of applause~!~/opinion/20160210/editorial-first-responders-deserve-a-round-of-applause~!~http://21C-NHwebvarnish.newscyclecloud.com/apps/pbcsi.dll/storyimage/NH/20160210/NEWS/160219959/AR/0/AR-160219959.jpg&exactW=150&exactH=108~#~Probable cause found for felony murder charge in 2010 New Haven shooting~!~/general-news/20160209/probable-cause-found-for-felony-murde