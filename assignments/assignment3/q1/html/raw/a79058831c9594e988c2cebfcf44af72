      var msgs = {'loadMore': 'Cargar m√°s...', 'loading': 'Cargando...', 'loaded': 'Ya no hay m√°s', 'addComment': 'A√±adir comentario', 'reply': 'Responder', 'delete': 'Eliminar'};
      var config = {'blogId': '5191524568417415305', 'postId': '2648885272851715739', 'feed': 'http://geeknacion.blogspot.com/feeds/2648885272851715739/comments/default', 'authorName': 'Adriana Cuellar L.', 'authorUrl': 'https://www.blogger.com/profile/16552599261167137423', 'baseUri': 'https://www.blogger.com', 'maxThreadDepth': 2};

