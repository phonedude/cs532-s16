<script>
var keys = '';
$('#edit-keys').click(function(e){ $(this).focus(); });
$(".main-search-form").submit(function(e) {
  e.preventDefault();
  si_search();
});

function si_search() {
  uri_key = keys;
  if (uri_key == '') uri_key = jQuery('#edit-keys').val();
  window.location = encodeURI('/search/site/'+uri_key);
  return false;
}
function si_track(field) {
  keys = field.value;
}
</script>
