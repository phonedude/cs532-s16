try{var c=[a()],d=window.innerWidth||document.body.clientWidth,e=0<=document.body.className.indexOf("webapp");e?c.push("news-narrow"):760>d?c.push("news-skinny"):970>d?c.push("news-narrow"):c.push("news-normal");document.documentElement.className+=" "+c.join(" ");if(e){var f=window.innerHeight||document.body.clientHeight;document.body.className=d>f?document.body.className+" landscape":document.body.className+" portrait"}}catch(b){}; })()
</script><div id="main-wrapper" class="a2"><div class="main-pane sp-" id="main-pane"><div class="background"><div class="centered"><div class="search"><div id="page-header"><div id="kd-googlebar"><div id="kd-appswitcher"><a class="glogo" href="/news?pz=1&amp;ned=us"></a></div>
