<script language="javascript" type="text/javascript">$(document).ready(function() {
        $.getJSON('/json/chart/stats_hourly_eur_statistics_48hour.json', function(data) {

        // split the data set into ohlc and volume
		var ohlc = [],
			mid_3h = [];
			mid_12h = [];
    		dataLength = data.length;



		for (i = 0; i < dataLength; i++) {
		    if(
                data[i][0] &&
                data[i][1] > 0 &&
                data[i][2] > 0 &&
                data[i][3] > 0 &&
                data[i][4] > 0 &&
                data[i][7] > 0 &&
                data[i][8] > 0
            )
            {
                ohlc.push([
                        data[i][0], // the date
                        data[i][1], // open
                        data[i][2], // high
                        data[i][3], // low
                        data[i][4] // close
                ]);


                if(data[i][7] > 0){
                    mid_3h.push([
                        data[i][0],
                        data[i][7]
                    ]);
                }

                if(data[i][8] > 0){
                    mid_12h.push([
                        data[i][0],
                        data[i][8]
                    ]);
                }
            }

		}

		var c_red = '#ff631b',
            c_bordeaux = '#5F021F', //'#7c1510',
            c_blue = '#4c7ba6', // #2f7ed8
            c_green = '#8bbc21',
            c_gray = '#999';

        var max_padding = 0.1;


		// set the allowed units for data grouping
		var groupingPixelWidth = 15;
		var groupingUnits = [[
                                'millisecond', // unit name
                                [1000] // allowed multiples
                            ], [
                                'second',
                                [60]
                            ], [
                                'minute',
                                [1,15] //[1, 2, 5, 10, 15, 30]
                            ], [
                                'hour',
                                [1, 2, 3, 4, 6, 8, 12]
                            ], [
                                'day',
                                [1]
                            ], [
                                'week',
                                [1]
                            ], [
                                'month',
                                [1, 3, 6]
                            ], [
                                'year',
                                null
                            ]];

		// create the chart
		    Highcharts.setOptions({
            global: {
                useUTC: false,
                timezoneOffset: 0,
            },
            lang: {
                months: ['Januar', 'Februar', 'März', 'April', 'Mai', 'Juni',  'Juli', 'August', 'September', 'Oktober', 'November', 'Dezember'],
                weekdays: ['Sonntag', 'Montag', 'Dienstag', 'Mittwoch', 'Donnerstag', 'Freitag', 'Samstag'],
                shortMonths:['Jan.', 'Feb.', 'März', 'Apr.', 'Mai', 'Jun.',  'Jul.', 'Aug.', 'Sep.', 'Okt.', 'Nov.', 'Dez.'],
                thousandsSep: '.',
                decimalPoint: ',',
                printChart: 'Chart drucken',
                downloadJPEG: 'als JPEG speichern',
                downloadPNG: 'als PNG speichern',
                downloadPDF: 'als PDF speichern',
                downloadSVG: 'als SVG speichern',
                contextButtonTitle: 'Daten Export',
                rangeSelectorZoom: 'Zeitraum',
            }
        });

		$('#chart').highcharts('StockChart', {
		    credits: {
		        enabled: false,
		        text: 'copyright (c) bitcoin.de',
		    },
		    chart: {
		        backgroundColor: 'transparent',
		        plotBackgroundColor: 'transparent',
		        //plotBackgroundImage: '/images/layout/logo-bg.jpg',
		        events:{
		            load: function() {
	        		    var chart = this;
	        		    current_extremes = chart.xAxis[0].getExtremes();
                        min = current_extremes.dataMax - (24 * 4 * 900e3);
                        max = current_extremes.dataMax;
                        chart.xAxis[0].setExtremes(min,max);
	        	    }
		        },
		    },


		    legend: {
		        enabled: false,
		    },
		    exporting: {
		        enabled: false,
		    },

		    rangeSelector: {
		        enabled: false,
		        buttons: [
		        {
		            type: 'hour',
		            count: 24,
		            text: '24h'
		        },
		        {
		            type: 'hour',
		            count: 48,
		            text: '48h'
		        },
		        ],
		        selected: 0,
		        inputEnabled: false,
		    },

		    navigator: {
		        enabled: false,
		    },

		    scrollbar: {
		        enabled: true,
		    },

		    title: {
		        text: ''
		    },

		    xAxis:{
		        maxZoom: 48*36e5,
		        minRange: 6 * 4 * 900e3,
		    },

		    yAxis: [{
		        title: {
		            text: 'Preis (EUR/BTC)'
		        },

		        offset: 30,
		        opposite: true,
		        height: 300,
		        minPadding: 0,
                maxPadding: max_padding,

		    }, {
		        title: {
		            text: 'Gewichteter Kurs (EUR/BTC)'
		        },
		        offset: 30,
		        linkedTo: 0,
		        height: 300,
		        minPadding: 0,
                maxPadding: max_padding,
		    }],



		    series: [
		    {
		        type: 'line',
		        name: 'Gewichteter Kurs 3h (EUR/BTC)',
		        id: 'mid_3h',
		        yAxis: 1,
		        data: mid_3h,
		        color: c_blue,
                dataGrouping: {

		            dateTimeLabelFormats:{
                        millisecond: ['%A, %b %e, %H:%M:%S.%L', '%A, %b %e, %H:%M:%S.%L', '-%H:%M:%S.%L'],
                        second: ['%A, %b %e, %H:%M:%S', '%A, %b %e, %H:%M:%S', '-%H:%M:%S'],
                        minute: ['%A, %b %e, %H:%M', '%A, %b %e, %H:%M', '-%H:%M'],
