                    "blogPartsContainer"    : "#entry-blogparts",
                    "tagArea"               : "input.blogparts-text",
                    "widgetTagArea"         : "input.blogparts-widget-text",
                    "entryTitle"            : "携帯電話のCMってどうしてああなっちゃったの？",
                    "formContainer"         : "#blogparts-container",
                    "previewDiv"            : "#blogparts-preview",
                    "widgetContainer"       : "#blogparts-widget-container",
                    "widgetPreviewDiv"      : "#blogparts-widget-preview",
                    "linkCheckboxUl"        : "#blogparts-link-checkbox-container"
                });
                blogparts.registerToggleBlogPartsContainer("#entry-blogparts-toggle-button");
                blogparts.registerLinkCheckbox("input.blogparts-link-checkbox");
                blogparts.registerTogglePreview("#preview-button");
                blogparts.registerToggleWidgetPreview("#preview-widget-button");
                blogparts.init();
            </script><script type="text/javascript" src="http://cdn-ak.b.st-hatena.com/js/bookmark_button.js" charset="utf-8" async="async"></script>

