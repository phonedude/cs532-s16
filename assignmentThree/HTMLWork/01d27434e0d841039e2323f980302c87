    <script>
        (function setupBookmarksTabChanger() {
            var started_new = false;
            var started_all = false;
            var allBookmarksTab = {
              id: 'all-bookmarks',
              onSelect: function () {
                if (started_all) { return; }
                started_all = true;
                var container = document.getElementById('remaining-all-bookmarks');
                (new Hatena.Bookmark.Entry.CommentListLoader({
                    container     : container,
                    commentOnly   : 0
                })).load();
              }
            };
            var tabmenu = new Hatena.Bookmark.Entry.BookmarksTabMenuV3(document.getElementById('public-bookmarks'), [
                  { id: 'scored-bookmarks' },
                  {
                    id: 'new-bookmarks',
                    fallBack: allBookmarksTab,
                    onSelect: function () {
                      if (started_new) { return; }
                      started_new = true;
                      var container = document.getElementById('remaining-new-bookmarks');
                      (new Hatena.Bookmark.Entry.CommentListLoader({
                          container     : container,
                          commentOnly   : 1
                      })).load();
                    }
                  },
                  allBookmarksTab
            ], 'bookmarks_tab');
            tabmenu.addEventListener("tabchanged", function () {
                var EntryPage = Hatena.Bookmark.UI.Pages.EntryPage;
                EntryPage.dispatchEvent("tabcontainerresized", {});
            });
        }).call(this);
    </script>

