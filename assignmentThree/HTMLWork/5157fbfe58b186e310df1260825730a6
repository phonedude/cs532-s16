            <script>
                if( jQuery('#tnt-gallery-ad').length === 0 ){
                    document.write('<div id="tnt-gallery-ad" style="display:none"><div class=\"tnt-ads-container text-center \"><div class=\"tnt-ads dfp-ad\" data-dfp-adunit=\"/8438/host.madison.com\" data-dfp-custom-pos=\"fixed-big-ad-gallery, stf\" id=\"fixed-big-ad-gallery\" data-dfp-responsive=\"no\" data-dfp-size=\"[300,250]\"></div></div></div>');
                }
            </script><script>
            jQuery(document).ready(function($){
                var gallery_ad_0dd4070e_d278_5293_a1ec_82e59e54351a_photo_modal = false;
                var gallery_changed_0dd4070e_d278_5293_a1ec_82e59e54351a_photo_modal = false;
                $('#gallery-items-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal').owlCarousel({
                    items: 1,
                    autoHeight: true,
                    lazyLoad: true,
                    loop: false,
                    navRewind: true,
                    responsiveRefreshRate: 500,
                    
                    
                    dots : false,
                    onChanged: function(){
                        if( gallery_ad_0dd4070e_d278_5293_a1ec_82e59e54351a_photo_modal ){
                                $('#tnt-gallery-ad').hide();
                                try {
            
            var bOnlyDFP = false, bIsOAS = false;
            if( typeof __tnt.ads.dfp != 'undefined' ){
                    bOnlyDFP = true;
                googletag.cmd.push(function(){
                        googletag.pubads().clear([__tnt.ads.dfp.slots['fixed-big-ad-gallery'].dfp_slot]);
                    });}} catch(e) {
            __tnt.log(e);
        }
                                gallery_ad_0dd4070e_d278_5293_a1ec_82e59e54351a_photo_modal = false;
                                $sGalleryAdInView = false;
                            }
                            
                            if( $('#gallery-items-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal .photo-container').eq(this._current).hasClass('photo-ad') ){
                                /** if any gallery in page is on ad slide and another gallery serves ad, advance last instance */
                                if( typeof $sGalleryAdInView !== 'undefined' && $sGalleryAdInView && $sGalleryAdInView !== '#gallery-items-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal'){
                                    $($sGalleryAdInView).data('owlCarousel').next();
                                }
                                var adHTML = $('#tnt-gallery-ad').detach().show();
                                $('#gallery-items-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal .photo-container').eq(this._current).find('.gallery-ad').append(adHTML);
                                try {
            
            var bOnlyDFP = false, bIsOAS = false;
            if( typeof __tnt.ads.dfp != 'undefined' ){
                    bOnlyDFP = true;
                
                   amznads.getAdsCallback('3266', function (){
                       amznads.setTargetingForGPTAsync('amznslots');
                        googletag.cmd.push(function(){
                            googletag.pubads().clear([__tnt.ads.dfp.slots['fixed-big-ad-gallery'].dfp_slot]);
                            googletag.pubads().refresh([__tnt.ads.dfp.slots['fixed-big-ad-gallery'].dfp_slot]);
                        });
                       googletag.pubads().clearTargeting('amznslots');
                   });}
                /* reloadByIds will return false/0 if the ad slot called for hasn't actually been loaded to the page. */
                if( typeof __tnt.ads.oas != 'undefined' ) bIsOAS = !!__tnt.ads.oas.reloadByIds(['fixed-big-ad-gallery']);
            
                if( !bOnlyDFP && !bIsOAS && typeof TNCMS.AdManager != 'undefined' ){
                        TNCMS.AdManager.refresh({region: 'fixed-big-ad-gallery'});}
            } catch(e) {
            __tnt.log(e);
        }
                                gallery_ad_0dd4070e_d278_5293_a1ec_82e59e54351a_photo_modal = true;
                                /** set instance of carousel containing active ad slide. */
                                $sGalleryAdInView = '#gallery-items-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal';
                            }
                            if( gallery_changed_0dd4070e_d278_5293_a1ec_82e59e54351a_photo_modal ){
                                try {
            
            var bOnlyDFP = false, bIsOAS = false;
            if( typeof __tnt.ads.dfp != 'undefined' ){
                    bOnlyDFP = true;
                
                   amznads.getAdsCallback('3266', function (){
                       amznads.setTargetingForGPTAsync('amznslots');
                        googletag.cmd.push(function(){
                            googletag.pubads().clear([__tnt.ads.dfp.slots['fixed-leaderboard-top'].dfp_slot]);
                            googletag.pubads().refresh([__tnt.ads.dfp.slots['fixed-leaderboard-top'].dfp_slot]);
                        });
                       googletag.pubads().clearTargeting('amznslots');
                   });}
                /* reloadByIds will return false/0 if the ad slot called for hasn't actually been loaded to the page. */
                if( typeof __tnt.ads.oas != 'undefined' ) bIsOAS = !!__tnt.ads.oas.reloadByIds(['fixed-leaderboard-top']);
            
                if( !bOnlyDFP && !bIsOAS && typeof TNCMS.AdManager != 'undefined' ){
                        TNCMS.AdManager.refresh({region: 'fixed-leaderboard-top'});}
            } catch(e) {
            __tnt.log(e);
        }
                            }
                            gallery_changed_0dd4070e_d278_5293_a1ec_82e59e54351a_photo_modal = true;
                        

                        
                            var currItem = $('#gallery-items-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal .photo-container').eq(this._current).data('count');
                            if(currItem){
                                $('#carousel-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal .current-count').html(currItem);
                            }
                            $('.modal.in, #carousel-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal .item').removeClass('hover');
                        
                    },
                    onTranslated:function(){
                        
                        
                        
                            __tnt.trackEvent($('#gallery-items-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal .photo-container').eq(this._current).data('track-object'));
                        

                        
                        
                    }
                });

                
                var photo_carousel_0dd4070e_d278_5293_a1ec_82e59e54351a_photo_modal = $('#gallery-items-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal').data('owlCarousel');

                $('#carousel-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal .photo-carousel-control.left').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_0dd4070e_d278_5293_a1ec_82e59e54351a_photo_modal.prev();
                });

                $('#carousel-0dd4070e-d278-5293-a1ec-82e59e54351a-photo-modal .photo-carousel-control.right').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_0dd4070e_d278_5293_a1ec_82e59e54351a_photo_modal.next();
                });
                    
                    });
        </script>
                    <div class="clearfix"></div>
                </div>
            </div>
