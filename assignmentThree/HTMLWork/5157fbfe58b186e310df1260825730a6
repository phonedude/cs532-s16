        <script>
            jQuery(document).ready(function($){
                $('#photo-carousel-0dd4070e-d278-5293-a1ec-82e59e54351a').on('show.bs.modal', function(e){
                    $('#fixed-leaderboard-top-container').addClass('overlay-leaderboard');
                });
                $('#photo-carousel-0dd4070e-d278-5293-a1ec-82e59e54351a').on('hide.bs.modal', function(e){
                    $('#fixed-leaderboard-top-container').removeClass('overlay-leaderboard');
                });
            });
