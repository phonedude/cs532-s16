<script type="text/javascript">
$(window).scroll(function(e){
    $el = $('.row.top');
    $el.toggleClass('main-header-scroll', $(this).scrollTop() > 100); 
}); 
</script>





<script type="text/javascript">
$(window).scroll(function(e){
    $el = $('.main-header .bottom-row');
    $el.toggleClass('main-header-scroll-bottomrow', $(this).scrollTop() > 100); 
}); 
</script>










<!--DROP IN MENU SCRIPT-->
<script type="text/javascript">

$('#manNav_Products').click(function() {
    $('#dropinmenu').animate({
       
        height: ['toggle', 'swing'],
        opacity: 'toggle'
    }, 500, 'linear');
	
	$("html, body").animate({ scrollTop: 0 }, "slow");
  return false;
    
});

</script>
<!--DROP IN MENU SCRIPT-->


<!--DROP IN MENU SCRIPT-->
