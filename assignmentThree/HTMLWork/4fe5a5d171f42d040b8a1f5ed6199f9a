$(function() {
	$(window).scroll(function(){		
		var distanceTop = $('#last').offset().top - $(window).height();
		
		if  ($(window).scrollTop() > distanceTop)
			$('.bottomboxslide').animate({'bottom':'-10px'},300);
		else 
			$('.bottomboxslide').stop(true).animate({'bottom':'-500px'},100);	
	});
	$('.bottomboxslide .btclose').bind('click',function(){
		$(this).parent().remove();
		$('.bottomboxslide').animate({'bottom':'-500px'},100);	
		return false
	});
});
