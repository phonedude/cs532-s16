<script>
		$(document).ready(function(){
			 $(window).load(function(){
				   setTimeout(function () {
			 	$('.overlay').fadeIn();
		        $('.subscribe_popup').show();  
				
				 }, 3000);
		
		 
			$('.closeI, .close').click(function(){
			 $('.overlay').fadeOut();
		            $('.subscribe_popup').fadeOut();
		});
		
		
			  });
			  
			 function newS(){
				 if($('#right_show').hasClass('openNew')) {
				   setTimeout(function () {
			 	$('.overlay').fadeOut();
		        $('.subscribe_popup').hide();  
				
				 }, 3000);
				 }
				 }

		$('.subscribe_button').click(function(){

			
			 if(validateText()){
			if(validateEmailText()){
		          
		   			var email =  $('#useremail').val();
					var name =  $('#username').val();
					var url = '/iw-cc/jagran_subscribe.jsp?uname='+name+'&uemail='+email;
		    		$.get(url, function(data, status){
		    			$('#left_show').hide();
		        		$('#right_show').show();
						newS();
		    		});
		         saveDataInstorage();
			    }   
		      }
		   });	
		   	
		});		
		</script>
</div><script type="text/javascript">
		 
  		  var timeNow = new Date().getTime();
		  var hours='24';

		if (typeof (Storage) !== "undefined") {
			var dbsave = localStorage.getItem("dbsave");
			if(dbsave == "null" || dbsave==null){
				dbsave='no';	
			}
			localStorage.dbsave = dbsave;
			
			if (localStorage.dbsave == 'no') {
					    		 var lastClearTime = localStorage.getItem('lastClearTime');
					  			if (lastClearTime == null) {
					    		     localStorage.setItem('lastClearTime', timeNow);
					     		  } else {
					     		  	 if(timeNow-lastClearTime > hours*60*60*1000) {
					   	 		     localStorage.clear(localStorage.clickcount);
					   	 		     localStorage.clear(localStorage.lastClearTime);
					         	     localStorage.setItem('lastClearTime', timeNow);
					         	   }
					           }
					if (localStorage.clickcount) {
						localStorage.clickcount = Number(localStorage.clickcount) + 1;
						
						if (localStorage.clickcount > 1) {
						document.getElementById('hidepopup').style.display = "none";
					 }
					} else {
						localStorage.clickcount = 1;
					}
				} else {
				    document.getElementById('hidepopup').style.display = "none";
				}
		} else {
			alert('Sorry, your browser does not support web storage...');
		}
	</script><script type="text/javascript">
	 function saveDataInstorage() {
		localStorage.setItem("dbsave", "yes");
		}
	 </script>
