					myForm.iwPreActions.value='saveAction2';
					myForm.id07.value=document.URL;
					alert("आपके कॉमेंट्स का स्वागत करते हैं, उप्युक्त कॉमेंटेस को प्रदर्शित/लाइव किया जाएगा.");
					myForm.submit();
				}
			}
			
		 function googleLogin() {
					popupWindow = window.open("/iw-cc/openid/googlelogin.jsp?provider=google&project=JAGRAN_POST&ty=comment","_blank","directories=no, status=no, menubar=no, scrollbars=yes, resizable=no,width=700, height=350,top=200,left=200");
			}
			
	
		function yahooLogin(){
			window.open('/iw-cc/openid/yahoologin.jsp?project=JAGRAN_POST&provider=yahoo', "_blank","directories=no, status=no, menubar=no, scrollbars=yes, resizable=no,width=600, height=280,top=200,left=200");
		}

	function fbLogin(){
		popupWindow = window.open('/iw-cc/openid/facebookRequest.jsp?cmd=comment', "_blank", "directories=no, display=popup, status=no, menubar=no, scrollbars=yes, resizable=no,width=600, height=280,top=200,left=200");
	}
	
	function clearField(field){
        if(field.value =='अपना कमेंट लिखें। कॉमेंट में किसी भी तरह की अभद्र भाषा का प्रयोग न करें।'){
              field.value='';
             }
		}
		
	function validateComments(){
                var commentsTextField = document.getElementById("comment");
                if(commentsTextField.value.length != 0){
                     if(commentsTextField.value!='अपना कमेंट लिखें। कॉमेंट में किसी भी तरह की अभद्र भाषा का प्रयोग न करें।'){
                        if(commentsTextField.value.length>1000){
                            var comments = commentsTextField.value;
                             alert("counter called."+comments);
                            commentsTextField.value = comments.substring(0, 999);
                            document.getElementById("commentsErrorField").innerHTML="Character Limit Exceeded";
                        }else{
                            document.getElementById("commentsErrorField").innerHTML="";
                        }
                        return true;
                    }else{
                        document.getElementById("commentsErrorField").innerHTML="Enter your comment    shashi.";
                        return false;
                    }
                }else{
                   document.getElementById("commentsErrorField").innerHTML="Enter your comment.";
                    return false;
                }
            }
            
            function textCounter(field, countfield, maxlimit) {
				if (field.value.length > maxlimit) 
				field.value = field.value.substring(0, maxlimit);
				// otherwise, update 'characters left' counter
				else 
				countfield.value = maxlimit - field.value.length;
			}    
            
        function trim(str) {
			return str.replace(/^\s+|\s+$/g,"");
		}
            
	
</script><body onload="setUrlValue();">
