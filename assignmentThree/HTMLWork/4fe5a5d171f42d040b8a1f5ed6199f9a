function validateText(){
                var noalpha = /^([A-Za-z ]{1,99})$/;
                var nameTextField = document.getElementById("username");
                if(nameTextField.value.length!=0){
                    if (!noalpha.test(nameTextField.value)) {
                        document.getElementById("nameError").innerHTML="Name should not have Numericals & Special Characters.";
                        return false;
                    }else{
                        document.getElementById("nameError").innerHTML="";
                        return true;
                    }
                }else{
                    document.getElementById("nameError").innerHTML="Please Enter your Name.";
                    return false;
                }
            }

            function validateEmailText() {
                var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
                var emailTextField = document.getElementById("useremail");
                if(emailTextField.value.length!=0){
                    if(!reg.test(emailTextField.value)) {
                        document.getElementById("emailError").innerHTML="Please enter a correct Email Address.";
                        return false;
                    }else if(emailTextField.value.length > 99){
                    	document.getElementById("emailError").innerHTML="Email length must be less then 100 char ";
                        return false;
                    }else{
                        document.getElementById("emailError").innerHTML="";
                        return true;






                    }
                }else{
                    document.getElementById("emailError").innerHTML="Please enter your Email .";
                    return false;
                }
            }
   </script><div id="hidepopup">
