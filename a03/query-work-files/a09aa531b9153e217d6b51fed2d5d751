function logAndRedirect(data,destination){var logURL=renderWebTrackURL(data,destination),image=new Image();var timeout=setTimeout(function(){redirectToDest(destination);},2000);image.onload=image.onerror=function(){clearTimeout(timeout);redirectToDest(destination);};image.src=logURL;return false;}
function attachHandler(){$('body').on('click.bct','a',function(event){var $target=$(event.target).closest('a'),destination=$target.attr('href'),dataToLog;if($target.closest('.cts-disabled').parents().length>$target.closest('.cts-enabled').parents().length){return true;}
dataToLog=getData($target);if(typeof dataToLog["linkId"]==="undefined"){return true;}
event.preventDefault();if($target.hasClass('cts-clicked')){return false;}
$target.addClass('cts-clicked');return logAndRedirect(dataToLog,destination);});}
return{attachHandler:function(){attachHandler();},redirect:function(ctsData,destinationHref,item){var data=[];data["linkId"]=ctsData["linkId"]
