function getItem($target){var $closestWithItem=$target.closest('[data-item]'),item='';if(1===$closestWithItem.length){item=$target.text().trim();}
return item;}
function getOrdinal($target,$dataItem){var $closestWithOrdinal,value='';if($dataItem!==$target){$closestWithOrdinal=$target.closest('[data-ordinal]');if(0!==$closestWithOrdinal.length){value=1;$closestWithOrdinal.find('a').each(function(index,element){if($target[0]===element){return false;}
value+=1;});}}
return value;}
function getCommonData(destination){var data=[];data["cacheBuster"]=Math.floor((Math.random()*899999999)+100000000);data["pageName"]=pageName;data["mseg"]=getMsegValue();data["destination"]=encodeURIComponent(destination);return data;}
function redirectToDest(location){window.location=location;}
function getCookieByName(name){var value=null,cookies=document.cookie,searchedCookie=cookies.indexOf(" "+name+"=");if(searchedCookie===-1){var checkFirstCookie=cookies.indexOf(name+"=");searchedCookie=checkFirstCookie==0?0:-1;}
if(searchedCookie>-1){var startPositionOfValue=cookies.indexOf("=",searchedCookie)+1,endPositionOfValue=cookies.indexOf(";",searchedCookie);if(endPositionOfValue===-1){endPositionOfValue=searchedCookie.length;}
value=cookies.substring(startPositionOfValue,endPositionOfValue);}
return value;}
function getMsegValue(){var msegVal;if(typeof cookiesManager!=="undefined"){msegVal=cookiesManager.getCookieValue("mseg");}else{msegVal=getCookieByName("mseg");}
return msegVal||"";}
